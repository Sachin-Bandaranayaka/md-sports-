{
  "name": "mssports",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "dev:server": "node server.js",
    "build": "prisma generate && next build",
    "start": "next start",
    "start:server": "node server.js",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "type-check": "tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --ci --coverage --watchAll=false",
    "test:unit": "jest --testPathPattern=tests/unit",
    "test:integration": "jest --testPathPattern=tests/integration",
    "test:api": "jest --testPathPattern=tests/api",
    "test:components": "jest --testPathPattern=tests/components",
    "test:utils": "jest --testPathPattern=tests/utils",
    "test:critical": "jest --testPathPattern='(auth|db|api)'",
    "test:affected": "jest --onlyChanged",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:debug": "playwright test --debug",
    "test:all": "npm run test:ci && npm run test:e2e",
    "test:performance": "lighthouse http://localhost:3000 --output=json --output-path=./test-results/lighthouse.json",
    "test:security": "npm audit --audit-level high",
    "test:accessibility": "pa11y http://localhost:3000",
    "prisma:seed": "ts-node --compiler-options {\"module\":\"CommonJS\"} prisma/seed.ts",
    "prisma:reset": "prisma migrate reset --force",
    "prisma:studio": "prisma studio",
    "db:setup": "prisma generate && prisma db push",
    "vercel:build": "prisma generate && next build",
    "qa:full": "npm run lint && npm run type-check && npm run test:all && npm run test:security",
    "qa:quick": "npm run lint && npm run test:critical",
    "qa:pre-commit": "npm run lint:fix && npm run test:affected",
    "qa:pre-push": "npm run qa:quick"
  },
  "prisma": {
    "seed": "ts-node --compiler-options {\"module\":\"CommonJS\"} prisma/seed.ts"
  },
  "dependencies": {
    "@prisma/client": "^5.14.0",
    "@radix-ui/react-alert-dialog": "^1.1.14",
    "@radix-ui/react-checkbox": "^1.3.2",
    "@radix-ui/react-popover": "^1.1.14",
    "@radix-ui/react-progress": "^1.1.7",
    "@radix-ui/react-select": "^2.2.5",
    "@radix-ui/react-tabs": "^1.1.12",
    "@tailwindcss/typography": "^0.5.16",
    "@tanstack/react-query": "^5.80.5",
    "@tanstack/react-query-devtools": "^5.80.5",
    "@types/pg": "^8.15.4",
    "@types/react-window": "^1.8.8",
    "@vercel/kv": "^1.0.1",
    "autoprefixer": "^10.4.21",
    "axios": "^1.9.0",
    "bcryptjs": "^2.4.3",
    "buffer": "^6.0.3",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "date-fns": "^3.6.0",
    "file-saver": "^2.0.5",
    "framer-motion": "^11.2.6",
    "ioredis": "^5.4.1",
    "jose": "^5.4.0",
    "jsonwebtoken": "^9.0.2",
    "jspdf": "^2.5.1",
    "jspdf-autotable": "^5.0.2",
    "lucide-react": "^0.379.0",
    "next": "14.2.3",
    "next-auth": "^4.24.11",
    "node-fetch": "^3.3.2",
    "openai": "^5.1.0",
    "pg": "^8.16.0",
    "process": "^0.11.10",
    "react": "^18",
    "react-day-picker": "^9.7.0",
    "react-dom": "^18",
    "react-hook-form": "^7.51.5",
    "react-hot-toast": "^2.5.2",
    "react-to-print": "^3.1.0",
    "react-virtualized-auto-sizer": "^1.0.26",
    "react-window": "^1.8.11",
    "react-window-infinite-loader": "^1.0.10",
    "recharts": "^2.15.3",
    "sonner": "^2.0.5",
    "tailwind-merge": "^2.3.0",
    "tailwindcss-animate": "^1.0.7",
    "use-debounce": "^10.0.4",
    "xlsx": "^0.18.5",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@playwright/test": "^1.40.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/bcryptjs": "^2.4.6",
    "@types/file-saver": "^2.0.7",
    "@types/jest": "^29.5.12",
    "@types/jsonwebtoken": "^9.0.9",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@typescript-eslint/eslint-plugin": "^8.34.1",
    "@typescript-eslint/parser": "^8.34.1",
    "artillery": "^2.0.0",
    "axe-playwright": "^2.1.0",
    "eslint": "^8",
    "eslint-config-next": "14.2.3",
    "husky": "^8.0.3",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-junit": "^16.0.0",
    "lighthouse": "^11.4.0",
    "lint-staged": "^15.2.0",
    "pa11y": "^8.0.0",
    "postcss": "^8",
    "prisma": "^5.14.0",
    "tailwindcss": "^3.4.1",
    "ts-node": "^10.9.2",
    "typescript": "^5"
  }
}
