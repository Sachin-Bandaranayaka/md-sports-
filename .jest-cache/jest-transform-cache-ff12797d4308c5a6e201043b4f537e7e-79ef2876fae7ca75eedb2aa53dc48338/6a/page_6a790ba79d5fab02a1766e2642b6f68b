  cov_1inllx6ad7().s[27]++;
  if (salesResponse.ok) {
    /* istanbul ignore next */
    cov_1inllx6ad7().b[5][0]++;
    const salesData =
    /* istanbul ignore next */
    (cov_1inllx6ad7().s[28]++, await salesResponse.json());
    /* istanbul ignore next */
    cov_1inllx6ad7().s[29]++;
    if (salesData.success) {
      /* istanbul ignore next */
      cov_1inllx6ad7().b[6][0]++;
      cov_1inllx6ad7().s[30]++;
      invoices = salesData.data.invoices;
      /* istanbul ignore next */
      cov_1inllx6ad7().s[31]++;
      salesMetrics = {
        daily: salesData.data.metrics.daily,
        weekly: salesData.data.metrics.weekly,
        monthly: salesData.data.metrics.monthly
      };
    } else
    /* istanbul ignore next */
    {
      cov_1inllx6ad7().b[6][1]++;
    }
  } else
  /* istanbul ignore next */
  {
    cov_1inllx6ad7().b[5][1]++;
  }
  // Transform the API response to match our ProductDetail interface
  cov_1inllx6ad7().s[32]++;
  return {
    id: productData.id.toString(),
    name: productData.name,
    category:
    /* istanbul ignore next */
    (cov_1inllx6ad7().b[7][0]++, productData.category_name) ||
    /* istanbul ignore next */
    (cov_1inllx6ad7().b[7][1]++, "Uncategorized"),
    stock: totalStock,
    retailPrice: productData.price,
    wholesalePrice:
    /* istanbul ignore next */
    (cov_1inllx6ad7().b[8][0]++, productData.wholesalePrice) ||
    /* istanbul ignore next */
    (cov_1inllx6ad7().b[8][1]++, productData.price * 0.85),
    weightedAverageCost:
    /* istanbul ignore next */
    (cov_1inllx6ad7().b[9][0]++, productData.weightedAverageCost) ||
    /* istanbul ignore next */
    (cov_1inllx6ad7().b[9][1]++, 0),
    status: totalStock > 0 ?
    /* istanbul ignore next */
    (cov_1inllx6ad7().b[10][0]++, totalStock < 10 ?
    /* istanbul ignore next */
    (cov_1inllx6ad7().b[11][0]++, "Low Stock") :
    /* istanbul ignore next */
    (cov_1inllx6ad7().b[11][1]++, "In Stock")) :
    /* istanbul ignore next */
    (cov_1inllx6ad7().b[10][1]++, "Out of Stock"),
    branchStock,
    invoices,
    sales: salesMetrics
  };
};
