    }
    cov_torxfj5iv().s[38]++;
    return hasAnyPermission(["invoice:manage", _permissions.PERMISSIONS.SALES_DELETE]);
  };
  /* istanbul ignore next */
  cov_torxfj5iv().s[39]++;
  const canManageUsers = () => {
    /* istanbul ignore next */
    cov_torxfj5iv().f[11]++;
    cov_torxfj5iv().s[40]++;
    return hasAnyPermission([_permissions.PERMISSIONS.USERS_CREATE, _permissions.PERMISSIONS.USERS_EDIT, _permissions.PERMISSIONS.USERS_DELETE]);
  };
  /* istanbul ignore next */
  cov_torxfj5iv().s[41]++;
  const canManageInventory = () => {
    /* istanbul ignore next */
    cov_torxfj5iv().f[12]++;
    cov_torxfj5iv().s[42]++;
    return hasAnyPermission([_permissions.PERMISSIONS.INVENTORY_CREATE, _permissions.PERMISSIONS.INVENTORY_EDIT, _permissions.PERMISSIONS.INVENTORY_DELETE, _permissions.PERMISSIONS.INVENTORY_MANAGE]);
  };
  /* istanbul ignore next */
  cov_torxfj5iv().s[43]++;
  const canViewReports = () => {
    /* istanbul ignore next */
    cov_torxfj5iv().f[13]++;
    cov_torxfj5iv().s[44]++;
    return hasPermission(_permissions.PERMISSIONS.REPORTS_VIEW);
  };
  /* istanbul ignore next */
  cov_torxfj5iv().s[45]++;
  const canViewDashboard = () => {
    /* istanbul ignore next */
    cov_torxfj5iv().f[14]++;
    cov_torxfj5iv().s[46]++;
    if (!user) {
      /* istanbul ignore next */
      cov_torxfj5iv().b[6][0]++;
      cov_torxfj5iv().s[47]++;
      return false;
    } else
    /* istanbul ignore next */
    {
      cov_torxfj5iv().b[6][1]++;
    }
    // Shop staff are not allowed to see the main dashboard
    cov_torxfj5iv().s[48]++;
    if (user.roleName?.toLowerCase() === "shop staff") {
      /* istanbul ignore next */
      cov_torxfj5iv().b[7][0]++;
      cov_torxfj5iv().s[49]++;
      return false;
    } else
    /* istanbul ignore next */
    {
      cov_torxfj5iv().b[7][1]++;
    }
    // For other roles, check for the required permission
    cov_torxfj5iv().s[50]++;
    return hasPermission(_permissions.PERMISSIONS.DASHBOARD_VIEW);
  };
  /* istanbul ignore next */
  cov_torxfj5iv().s[51]++;
  const canAccessRoute = route => {
    /* istanbul ignore next */
    cov_torxfj5iv().f[15]++;
    const requiredPermission =
    /* istanbul ignore next */
    (cov_torxfj5iv().s[52]++, routePermissions[route]);
    /* istanbul ignore next */
    cov_torxfj5iv().s[53]++;
    if (!requiredPermission) {
      /* istanbul ignore next */
      cov_torxfj5iv().b[8][0]++;
      cov_torxfj5iv().s[54]++;
      return true; // No specific permission required
    } else
    /* istanbul ignore next */
    {
      cov_torxfj5iv().b[8][1]++;
    }
    cov_torxfj5iv().s[55]++;
    return hasPermission(requiredPermission);
  };
  // Check if user can view quotations
  /* istanbul ignore next */
  cov_torxfj5iv().s[56]++;
  const canViewQuotations = () => {
    /* istanbul ignore next */
    cov_torxfj5iv().f[16]++;
    cov_torxfj5iv().s[57]++;
    if (!user) {
      /* istanbul ignore next */
      cov_torxfj5iv().b[9][0]++;
      cov_torxfj5iv().s[58]++;
      return false;
    } else
    /* istanbul ignore next */
    {
      cov_torxfj5iv().b[9][1]++;
    }
    // Allow viewing if user has general sales view, specific quotation view permission, or is a Shop Staff
    cov_torxfj5iv().s[59]++;
    return /* istanbul ignore next */(cov_torxfj5iv().b[10][0]++, hasPermission(_permissions.PERMISSIONS.SALES_VIEW)) ||
    /* istanbul ignore next */
    (cov_torxfj5iv().b[10][1]++, hasPermission(_permissions.PERMISSIONS.QUOTATION_VIEW)) ||
    /* istanbul ignore next */
    (cov_torxfj5iv().b[10][2]++, user.roleName?.toLowerCase() === "shop staff");
  };
  // Check if user can create quotations
  /* istanbul ignore next */
  cov_torxfj5iv().s[60]++;
  const canCreateQuotations = () => {
    /* istanbul ignore next */
    cov_torxfj5iv().f[17]++;
    cov_torxfj5iv().s[61]++;
    if (!user) {
      /* istanbul ignore next */
      cov_torxfj5iv().b[11][0]++;
      cov_torxfj5iv().s[62]++;
      return false;
    } else
    /* istanbul ignore next */
    {
      cov_torxfj5iv().b[11][1]++;
    }
    // Allow creation if user has manage or create permission
    cov_torxfj5iv().s[63]++;
    return /* istanbul ignore next */(cov_torxfj5iv().b[12][0]++, hasPermission(_permissions.PERMISSIONS.QUOTATION_MANAGE)) ||
    /* istanbul ignore next */
    (cov_torxfj5iv().b[12][1]++, hasPermission(_permissions.PERMISSIONS.QUOTATION_CREATE));
  };
  // Check if user can edit/delete quotations
  /* istanbul ignore next */
  cov_torxfj5iv().s[64]++;
  const canEditQuotations = () => {
    /* istanbul ignore next */
    cov_torxfj5iv().f[18]++;
    cov_torxfj5iv().s[65]++;
    if (!user) {
      /* istanbul ignore next */
      cov_torxfj5iv().b[13][0]++;
      cov_torxfj5iv().s[66]++;
      return false;
    } else
    /* istanbul ignore next */
    {
      cov_torxfj5iv().b[13][1]++;
    }
    cov_torxfj5iv().s[67]++;
    return /* istanbul ignore next */(cov_torxfj5iv().b[14][0]++, hasPermission("quotation:manage")) ||
    /* istanbul ignore next */
    (cov_torxfj5iv().b[14][1]++, hasPermission("admin:all")) ||
    /* istanbul ignore next */
    (cov_torxfj5iv().b[14][2]++, hasPermission("ALL")) ||
    /* istanbul ignore next */
    (cov_torxfj5iv().b[14][3]++, hasPermission("*"));
  };
  // Check if user can view cost data
  /* istanbul ignore next */
  cov_torxfj5iv().s[68]++;
  const canViewCosts = () => {
    /* istanbul ignore next */
    cov_torxfj5iv().f[19]++;
    cov_torxfj5iv().s[69]++;
    return /* istanbul ignore next */(cov_torxfj5iv().b[15][0]++, hasPermission("shop:view_costs")) ||
    /* istanbul ignore next */
    (cov_torxfj5iv().b[15][1]++, hasPermission("admin:all")) ||
    /* istanbul ignore next */
    (cov_torxfj5iv().b[15][2]++, hasPermission("ALL")) ||
    /* istanbul ignore next */
    (cov_torxfj5iv().b[15][3]++, hasPermission("*"));
  };
  // Check if user can record payments to specific accounts
  /* istanbul ignore next */
  cov_torxfj5iv().s[70]++;
  const canRecordPaymentToAccount = accountId => {
    /* istanbul ignore next */
    cov_torxfj5iv().f[20]++;
    cov_torxfj5iv().s[71]++;
    // Admin can record to any account
    if (
    /* istanbul ignore next */
    (cov_torxfj5iv().b[17][0]++, hasPermission("admin:all")) ||
    /* istanbul ignore next */
    (cov_torxfj5iv().b[17][1]++, hasPermission("ALL")) ||
    /* istanbul ignore next */
    (cov_torxfj5iv().b[17][2]++, hasPermission("*"))) {
      /* istanbul ignore next */
      cov_torxfj5iv().b[16][0]++;
      cov_torxfj5iv().s[72]++;
      return true;
    } else
    /* istanbul ignore next */
    {
      cov_torxfj5iv().b[16][1]++;
    }
    // Check if user has payment:record permission
    cov_torxfj5iv().s[73]++;
    if (
    /* istanbul ignore next */
    (cov_torxfj5iv().b[19][0]++, !hasPermission("payment:record")) &&
    /* istanbul ignore next */
    (cov_torxfj5iv().b[19][1]++, !hasPermission("invoice:create"))) {
      /* istanbul ignore next */
      cov_torxfj5iv().b[18][0]++;
      cov_torxfj5iv().s[74]++;
      return false;
    } else
    /* istanbul ignore next */
    {
      cov_torxfj5iv().b[18][1]++;
    }
    // Check if user has specific account permissions
    cov_torxfj5iv().s[75]++;
    if (
    /* istanbul ignore next */
    (cov_torxfj5iv().b[21][0]++, user?.allowedAccounts) &&
    /* istanbul ignore next */
    (cov_torxfj5iv().b[21][1]++, user.allowedAccounts.length > 0)) {
      /* istanbul ignore next */
      cov_torxfj5iv().b[20][0]++;
      cov_torxfj5iv().s[76]++;
      return user.allowedAccounts.includes(accountId.toString());
    } else
    /* istanbul ignore next */
    {
      cov_torxfj5iv().b[20][1]++;
    }
    // For users without specific account restrictions, allow all income/asset accounts
    // This maintains backward compatibility
    cov_torxfj5iv().s[77]++;
    return true;
  };
  // Get list of accounts user can record payments to
  /* istanbul ignore next */
  cov_torxfj5iv().s[78]++;
  const getAllowedAccountIds = () => {
    /* istanbul ignore next */
    cov_torxfj5iv().f[21]++;
    cov_torxfj5iv().s[79]++;
    // Admin can use any account
    if (
    /* istanbul ignore next */
    (cov_torxfj5iv().b[23][0]++, hasPermission("admin:all")) ||
    /* istanbul ignore next */
    (cov_torxfj5iv().b[23][1]++, hasPermission("ALL")) ||
    /* istanbul ignore next */
    (cov_torxfj5iv().b[23][2]++, hasPermission("*"))) {
      /* istanbul ignore next */
      cov_torxfj5iv().b[22][0]++;
      cov_torxfj5iv().s[80]++;
      return [];
    } else
    /* istanbul ignore next */
    {
      cov_torxfj5iv().b[22][1]++;
    }
    // Return user's allowed accounts or empty array if no restrictions
    cov_torxfj5iv().s[81]++;
    return /* istanbul ignore next */(cov_torxfj5iv().b[24][0]++, user?.allowedAccounts) ||
    /* istanbul ignore next */
    (cov_torxfj5iv().b[24][1]++, []);
  };
  // Check if user has permission to access the current route
  /* istanbul ignore next */
  cov_torxfj5iv().s[82]++;
  const checkRoutePermission = () => {
    /* istanbul ignore next */
    cov_torxfj5iv().f[22]++;
    cov_torxfj5iv().s[83]++;
    // Default to dashboard for root path
    if (pathname === "/") {
      /* istanbul ignore next */
      cov_torxfj5iv().b[25][0]++;
      cov_torxfj5iv().s[84]++;
      return true;
    } else
    /* istanbul ignore next */
    {
      cov_torxfj5iv().b[25][1]++;
    }
    // Login page is always accessible
    cov_torxfj5iv().s[85]++;
    if (pathname === "/login") {
      /* istanbul ignore next */
      cov_torxfj5iv().b[26][0]++;
      cov_torxfj5iv().s[86]++;
      return true;
    } else
    /* istanbul ignore next */
    {
      cov_torxfj5iv().b[26][1]++;
    }
    // For exact route matches
    const requiredPermission =
    /* istanbul ignore next */
    (cov_torxfj5iv().s[87]++, routePermissions[pathname]);
    /* istanbul ignore next */
    cov_torxfj5iv().s[88]++;
    if (requiredPermission !== undefined) {
      /* istanbul ignore next */
      cov_torxfj5iv().b[27][0]++;
      cov_torxfj5iv().s[89]++;
      return hasPermission(requiredPermission);
    } else
    /* istanbul ignore next */
    {
      cov_torxfj5iv().b[27][1]++;
    }
    // For routes that don't have an exact match, check parent routes
    cov_torxfj5iv().s[90]++;
    for (const route in routePermissions) {
      /* istanbul ignore next */
      cov_torxfj5iv().s[91]++;
      if (pathname.startsWith(route + "/")) {
        /* istanbul ignore next */
        cov_torxfj5iv().b[28][0]++;
        cov_torxfj5iv().s[92]++;
        return hasPermission(routePermissions[route]);
      } else
      /* istanbul ignore next */
      {
        cov_torxfj5iv().b[28][1]++;
      }
    }
    // If no matching route found, allow access by default
    /* istanbul ignore next */
    cov_torxfj5iv().s[93]++;
    return true;
  };
  /* istanbul ignore next */
  cov_torxfj5iv().s[94]++;
  return {
    // Core permission functions
    hasPermission,
    hasAnyPermission,
    hasAllPermissions,
    isAdmin,
    hasShopAccess,
    getAccessibleShopIds,
    // Specific business logic functions
    canCreateInvoices,
    canManageInvoices,
    canDeleteInvoices,
    canManageUsers,
    canManageInventory,
    canViewReports,
    canViewDashboard,
    canAccessRoute,
    checkRoutePermission,
    canViewQuotations,
    canCreateQuotations,
    canEditQuotations,
    canViewCosts,
    canRecordPaymentToAccount,
    getAllowedAccountIds,
    // Legacy support
    routePermissions
  };
}
