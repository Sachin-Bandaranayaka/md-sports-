    }
  };
  // Helper function to validate distributions
  /* istanbul ignore next */
  cov_18emoeox3d().s[142]++;
  const validateDistributions = () => {
    /* istanbul ignore next */
    cov_18emoeox3d().f[29]++;
    cov_18emoeox3d().s[143]++;
    if (
    /* istanbul ignore next */
