      })()
    })), /*#__PURE__*/(0, _jsxruntime.jsxs)("div", {
      className: "flex justify-end space-x-3 mt-8",
      children: [/*#__PURE__*/(0, _jsxruntime.jsx)(_Button.Button, {
        type: "button",
        variant: "outline",
        disabled: submitting,
        onClick: isModal ?
        /* istanbul ignore next */
