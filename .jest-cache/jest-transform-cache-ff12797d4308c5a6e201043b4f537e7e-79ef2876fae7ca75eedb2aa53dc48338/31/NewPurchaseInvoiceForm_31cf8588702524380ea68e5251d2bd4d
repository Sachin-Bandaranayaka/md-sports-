      /* istanbul ignore next */
      {
        cov_18emoeox3d().b[33][1]++;
      }
      const createdProduct =
      /* istanbul ignore next */
      (cov_18emoeox3d().s[123]++, await response.json());
      // Refetch products to get updated list
      /* istanbul ignore next */
      cov_18emoeox3d().s[124]++;
      await refetchProducts();
      /* istanbul ignore next */
      cov_18emoeox3d().s[125]++;
      setProducts(prev => {
        /* istanbul ignore next */
        cov_18emoeox3d().f[27]++;
        cov_18emoeox3d().s[126]++;
