      });
      /* istanbul ignore next */
      cov_18emoeox3d().s[127]++;
      setFilteredProducts(prev => {
        /* istanbul ignore next */
        cov_18emoeox3d().f[28]++;
        cov_18emoeox3d().s[128]++;
        return [...prev, createdProduct.data];
      });
      /* istanbul ignore next */
      cov_18emoeox3d().s[129]++;
      setShowNewProductModal(false);
      /* istanbul ignore next */
      cov_18emoeox3d().s[130]++;
      setNewProductData({
        name: "",
        sku: "",
        description: "",
        price: 0,
        weightedAverageCost: 0,
        categoryId: ""
      });
      // Optionally, add the new product directly to the current invoice items
    } catch (err) {
      /* istanbul ignore next */
      cov_18emoeox3d().s[131]++;
      setNewProductModalError(err.message);
    } finally {
