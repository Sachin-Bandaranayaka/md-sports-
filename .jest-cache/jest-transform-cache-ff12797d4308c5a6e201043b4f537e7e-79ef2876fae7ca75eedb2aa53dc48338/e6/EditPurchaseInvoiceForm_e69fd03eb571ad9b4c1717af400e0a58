    (cov_sdv2kyoyg().b[29][0]++, formData.items) ||
    /* istanbul ignore next */
    (cov_sdv2kyoyg().b[29][1]++, []))]);
    const currentItem =
    /* istanbul ignore next */
    (cov_sdv2kyoyg().s[95]++, {
      ...newItems[index]
    });
    /* istanbul ignore next */
    cov_sdv2kyoyg().s[96]++;
    if (name === "productId") {
      /* istanbul ignore next */
      cov_sdv2kyoyg().b[30][0]++;
      const selectedProduct =
      /* istanbul ignore next */
      (cov_sdv2kyoyg().s[97]++, products.find(p => {
        /* istanbul ignore next */
        cov_sdv2kyoyg().f[22]++;
        cov_sdv2kyoyg().s[98]++;
        return p.id.toString() === value;
      }));
      /* istanbul ignore next */
      cov_sdv2kyoyg().s[99]++;
      if (selectedProduct) {
        /* istanbul ignore next */
        cov_sdv2kyoyg().b[31][0]++;
        cov_sdv2kyoyg().s[100]++;
        currentItem.productId = selectedProduct.id.toString();
        /* istanbul ignore next */
        cov_sdv2kyoyg().s[101]++;
        currentItem.productName = selectedProduct.name;
        /* istanbul ignore next */
        cov_sdv2kyoyg().s[102]++;
        currentItem.price =
        /* istanbul ignore next */
        (cov_sdv2kyoyg().b[32][0]++, selectedProduct.weightedAverageCost) ||
        /* istanbul ignore next */
        (cov_sdv2kyoyg().b[32][1]++, 0);
      } else {
        /* istanbul ignore next */
        cov_sdv2kyoyg().b[31][1]++;
        cov_sdv2kyoyg().s[103]++;
        currentItem.productId = "";
        /* istanbul ignore next */
        cov_sdv2kyoyg().s[104]++;
        currentItem.productName = "";
        /* istanbul ignore next */
        cov_sdv2kyoyg().s[105]++;
        currentItem.price = 0;
      }
    } else {
      /* istanbul ignore next */
      cov_sdv2kyoyg().b[30][1]++;
      cov_sdv2kyoyg().s[106]++;
      if (
      /* istanbul ignore next */
      (cov_sdv2kyoyg().b[34][0]++, name === "quantity") ||
      /* istanbul ignore next */
      (cov_sdv2kyoyg().b[34][1]++, name === "price")) {
        /* istanbul ignore next */
        cov_sdv2kyoyg().b[33][0]++;
        cov_sdv2kyoyg().s[107]++;
        currentItem[name] = value === "" ?
        /* istanbul ignore next */
        (cov_sdv2kyoyg().b[35][0]++, "") :
        /* istanbul ignore next */
        (cov_sdv2kyoyg().b[35][1]++,
        /* istanbul ignore next */
        (cov_sdv2kyoyg().b[36][0]++, parseFloat(value)) ||
        /* istanbul ignore next */
        (cov_sdv2kyoyg().b[36][1]++, 0));
      } else {
        /* istanbul ignore next */
        cov_sdv2kyoyg().b[33][1]++;
        cov_sdv2kyoyg().s[108]++;
        currentItem[name] = value;
      }
    }
    /* istanbul ignore next */
    cov_sdv2kyoyg().s[109]++;
    newItems[index] = currentItem;
    /* istanbul ignore next */
    cov_sdv2kyoyg().s[110]++;
    setFormData(prev => {
      /* istanbul ignore next */
      cov_sdv2kyoyg().f[23]++;
      cov_sdv2kyoyg().s[111]++;
      return {
        ...prev,
        items: newItems
      };
    });
  };
  /* istanbul ignore next */
  cov_sdv2kyoyg().s[112]++;
  const handleRemoveItem = index => {
    /* istanbul ignore next */
    cov_sdv2kyoyg().f[24]++;
    cov_sdv2kyoyg().s[113]++;
    setFormData(prev => {
      /* istanbul ignore next */
      cov_sdv2kyoyg().f[25]++;
      cov_sdv2kyoyg().s[114]++;
      return {
        ...prev,
        items: (
        /* istanbul ignore next */
        (cov_sdv2kyoyg().b[37][0]++, prev.items) ||
        /* istanbul ignore next */
        (cov_sdv2kyoyg().b[37][1]++, [])).filter((_, i) => {
          /* istanbul ignore next */
          cov_sdv2kyoyg().f[26]++;
          cov_sdv2kyoyg().s[115]++;
          return i !== index;
        })
      };
    });
    // Properly remove the distribution at the specified index and shift remaining distributions
    /* istanbul ignore next */
    cov_sdv2kyoyg().s[116]++;
    setItemDistributions(prevDist => {
      /* istanbul ignore next */
      cov_sdv2kyoyg().f[27]++;
      const newDist =
      /* istanbul ignore next */
      (cov_sdv2kyoyg().s[117]++, prevDist.filter((_, i) => {
        /* istanbul ignore next */
        cov_sdv2kyoyg().f[28]++;
        cov_sdv2kyoyg().s[118]++;
        return i !== index;
      }));
      /* istanbul ignore next */
      cov_sdv2kyoyg().s[119]++;
      return newDist;
    });
    // Also update previousItemQuantities to maintain sync
    /* istanbul ignore next */
    cov_sdv2kyoyg().s[120]++;
    setPreviousItemQuantities(prev => {
      /* istanbul ignore next */
      cov_sdv2kyoyg().f[29]++;
      cov_sdv2kyoyg().s[121]++;
      return prev.filter((_, i) => {
        /* istanbul ignore next */
        cov_sdv2kyoyg().f[30]++;
        cov_sdv2kyoyg().s[122]++;
        return i !== index;
      });
    });
  };
  /* istanbul ignore next */
  cov_sdv2kyoyg().s[123]++;
  const handleOpenDistributionModal = itemIndex => {
    /* istanbul ignore next */
    cov_sdv2kyoyg().f[31]++;
    cov_sdv2kyoyg().s[124]++;
    setSelectedItemIndexForDistribution(itemIndex);
    /* istanbul ignore next */
    cov_sdv2kyoyg().s[125]++;
    setShowDistributionModal(true);
  };
  /* istanbul ignore next */
  cov_sdv2kyoyg().s[126]++;
  const handleDistributionChange = (shopId, quantityStr) => {
    /* istanbul ignore next */
    cov_sdv2kyoyg().f[32]++;
    cov_sdv2kyoyg().s[127]++;
    if (selectedItemIndexForDistribution === null) {
      /* istanbul ignore next */
      cov_sdv2kyoyg().b[38][0]++;
      cov_sdv2kyoyg().s[128]++;
      return;
    } else
    /* istanbul ignore next */
    {
      cov_sdv2kyoyg().b[38][1]++;
    }
    const quantity =
    /* istanbul ignore next */
    (cov_sdv2kyoyg().s[129]++, quantityStr === "" ?
    /* istanbul ignore next */
    (cov_sdv2kyoyg().b[39][0]++, 0) :
    /* istanbul ignore next */
    (cov_sdv2kyoyg().b[39][1]++,
    /* istanbul ignore next */
    (cov_sdv2kyoyg().b[40][0]++, parseInt(quantityStr)) ||
    /* istanbul ignore next */
    (cov_sdv2kyoyg().b[40][1]++, 0)));
    /* istanbul ignore next */
    cov_sdv2kyoyg().s[130]++;
    setItemDistributions(prevDists => {
      /* istanbul ignore next */
      cov_sdv2kyoyg().f[33]++;
      const newDists =
      /* istanbul ignore next */
      (cov_sdv2kyoyg().s[131]++, [...prevDists]);
      const currentItemDist =
      /* istanbul ignore next */
      (cov_sdv2kyoyg().s[132]++,
      /* istanbul ignore next */
      (cov_sdv2kyoyg().b[41][0]++, {
        ...newDists[selectedItemIndexForDistribution]
      }) ||
      /* istanbul ignore next */
      (cov_sdv2kyoyg().b[41][1]++, {}));
      /* istanbul ignore next */
      cov_sdv2kyoyg().s[133]++;
      if (quantity > 0) {
        /* istanbul ignore next */
        cov_sdv2kyoyg().b[42][0]++;
        cov_sdv2kyoyg().s[134]++;
        currentItemDist[shopId] = quantity;
      } else {
        /* istanbul ignore next */
        cov_sdv2kyoyg().b[42][1]++;
        cov_sdv2kyoyg().s[135]++;
        delete currentItemDist[shopId];
      }
      /* istanbul ignore next */
      cov_sdv2kyoyg().s[136]++;
      newDists[selectedItemIndexForDistribution] = currentItemDist;
      /* istanbul ignore next */
      cov_sdv2kyoyg().s[137]++;
      return newDists;
    });
  };
  /* istanbul ignore next */
  cov_sdv2kyoyg().s[138]++;
  const getTotalDistributedForItem = itemIndex => {
    /* istanbul ignore next */
    cov_sdv2kyoyg().f[34]++;
    cov_sdv2kyoyg().s[139]++;
    if (
    /* istanbul ignore next */
    (cov_sdv2kyoyg().b[44][0]++, itemIndex === null) ||
    /* istanbul ignore next */
    (cov_sdv2kyoyg().b[44][1]++, itemIndex < 0) ||
    /* istanbul ignore next */
    (cov_sdv2kyoyg().b[44][2]++, itemIndex >= itemDistributions.length)) {
      /* istanbul ignore next */
      cov_sdv2kyoyg().b[43][0]++;
      cov_sdv2kyoyg().s[140]++;
      return 0;
    } else
    /* istanbul ignore next */
    {
      cov_sdv2kyoyg().b[43][1]++;
    }
    cov_sdv2kyoyg().s[141]++;
    if (
    /* istanbul ignore next */
    (cov_sdv2kyoyg().b[46][0]++, !itemDistributions[itemIndex]) ||
    /* istanbul ignore next */
    (cov_sdv2kyoyg().b[46][1]++, typeof itemDistributions[itemIndex] !== "object")) {
      /* istanbul ignore next */
      cov_sdv2kyoyg().b[45][0]++;
      cov_sdv2kyoyg().s[142]++;
      return 0;
    } else
    /* istanbul ignore next */
    {
      cov_sdv2kyoyg().b[45][1]++;
    }
    cov_sdv2kyoyg().s[143]++;
    return Object.values(itemDistributions[itemIndex]).reduce((sum, qty) => {
      /* istanbul ignore next */
      cov_sdv2kyoyg().f[35]++;
      cov_sdv2kyoyg().s[144]++;
      return sum + (
      /* istanbul ignore next */
      (cov_sdv2kyoyg().b[47][0]++, Number(qty)) ||
      /* istanbul ignore next */
      (cov_sdv2kyoyg().b[47][1]++, 0));
    }, 0);
  };
  // New helper to get shop options for combobox, ensuring shop.id is string
  const shopOptions =
  /* istanbul ignore next */
  (cov_sdv2kyoyg().s[145]++, shops.map(shop => {
    /* istanbul ignore next */
    cov_sdv2kyoyg().f[36]++;
    cov_sdv2kyoyg().s[146]++;
    return {
      value: String(shop.id),
      label: shop.name
    };
  }));
  /* istanbul ignore next */
  cov_sdv2kyoyg().s[147]++;
  const handleCreateNewProduct = async () => {
    /* istanbul ignore next */
    cov_sdv2kyoyg().f[37]++;
    cov_sdv2kyoyg().s[148]++;
    if (
    /* istanbul ignore next */
    (cov_sdv2kyoyg().b[49][0]++, !newProductData.name) ||
    /* istanbul ignore next */
    (cov_sdv2kyoyg().b[49][1]++, !newProductData.categoryId) ||
    /* istanbul ignore next */
    (cov_sdv2kyoyg().b[49][2]++, newProductData.price <= 0) ||
    /* istanbul ignore next */
    (cov_sdv2kyoyg().b[49][3]++, newProductData.weightedAverageCost <= 0)) {
      /* istanbul ignore next */
      cov_sdv2kyoyg().b[48][0]++;
      cov_sdv2kyoyg().s[149]++;
      setNewProductModalError("Name, Category, Purchase Price, and Retail Price are required.");
      /* istanbul ignore next */
      cov_sdv2kyoyg().s[150]++;
      return;
    } else
    /* istanbul ignore next */
    {
      cov_sdv2kyoyg().b[48][1]++;
    }
    cov_sdv2kyoyg().s[151]++;
    setNewProductSubmitting(true);
    /* istanbul ignore next */
    cov_sdv2kyoyg().s[152]++;
    setNewProductModalError(null);
    /* istanbul ignore next */
    cov_sdv2kyoyg().s[153]++;
    try {
      const response =
      /* istanbul ignore next */
      (cov_sdv2kyoyg().s[154]++, await fetch("/api/products", {
        method: "POST",
        headers: {
          "Content-Type": "application/json"
        },
        body: JSON.stringify(newProductData)
      }));
      /* istanbul ignore next */
      cov_sdv2kyoyg().s[155]++;
      if (!response.ok) {
        /* istanbul ignore next */
        cov_sdv2kyoyg().b[50][0]++;
        const errData =
        /* istanbul ignore next */
        (cov_sdv2kyoyg().s[156]++, await response.json());
        /* istanbul ignore next */
        cov_sdv2kyoyg().s[157]++;
        throw new Error(
        /* istanbul ignore next */
        (cov_sdv2kyoyg().b[51][0]++, errData.message) ||
        /* istanbul ignore next */
        (cov_sdv2kyoyg().b[51][1]++, "Failed to create product"));
      } else
      /* istanbul ignore next */
      {
        cov_sdv2kyoyg().b[50][1]++;
      }
      const createdProduct =
      /* istanbul ignore next */
      (cov_sdv2kyoyg().s[158]++, await response.json());
      /* istanbul ignore next */
      cov_sdv2kyoyg().s[159]++;
      setProducts(prev => {
        /* istanbul ignore next */
        cov_sdv2kyoyg().f[38]++;
        cov_sdv2kyoyg().s[160]++;
        return [...prev, createdProduct.data];
      });
      /* istanbul ignore next */
      cov_sdv2kyoyg().s[161]++;
      setFilteredProducts(prev => {
        /* istanbul ignore next */
        cov_sdv2kyoyg().f[39]++;
        cov_sdv2kyoyg().s[162]++;
        return [...prev, createdProduct.data];
      });
      /* istanbul ignore next */
      cov_sdv2kyoyg().s[163]++;
      setShowNewProductModal(false);
      /* istanbul ignore next */
      cov_sdv2kyoyg().s[164]++;
      setNewProductData({
        name: "",
        sku: "",
        description: "",
        price: 0,
        weightedAverageCost: 0,
        categoryId: ""
      });
    } catch (err) {
      /* istanbul ignore next */
      cov_sdv2kyoyg().s[165]++;
      setNewProductModalError(err.message);
    } finally {
      /* istanbul ignore next */
      cov_sdv2kyoyg().s[166]++;
      setNewProductSubmitting(false);
    }
  };
  /* istanbul ignore next */
  cov_sdv2kyoyg().s[167]++;
  const handleSubmit = async e => {
    /* istanbul ignore next */
    cov_sdv2kyoyg().f[40]++;
    cov_sdv2kyoyg().s[168]++;
    e.preventDefault();
    /* istanbul ignore next */
    cov_sdv2kyoyg().s[169]++;
    setError(null);
    /* istanbul ignore next */
    cov_sdv2kyoyg().s[170]++;
    if (!formData.supplierId) {
      /* istanbul ignore next */
      cov_sdv2kyoyg().b[52][0]++;
      cov_sdv2kyoyg().s[171]++;
      setError("Please select a supplier.");
      /* istanbul ignore next */
      cov_sdv2kyoyg().s[172]++;
      return;
    } else
    /* istanbul ignore next */
    {
      cov_sdv2kyoyg().b[52][1]++;
    }
    cov_sdv2kyoyg().s[173]++;
    if (
    /* istanbul ignore next */
    (cov_sdv2kyoyg().b[54][0]++, !formData.items) ||
    /* istanbul ignore next */
    (cov_sdv2kyoyg().b[54][1]++, formData.items.length === 0)) {
      /* istanbul ignore next */
      cov_sdv2kyoyg().b[53][0]++;
      cov_sdv2kyoyg().s[174]++;
      setError("Please add at least one item to the invoice.");
      /* istanbul ignore next */
      cov_sdv2kyoyg().s[175]++;
      return;
    } else
    /* istanbul ignore next */
    {
      cov_sdv2kyoyg().b[53][1]++;
    }
    cov_sdv2kyoyg().s[176]++;
    for (let i =
    /* istanbul ignore next */
    (cov_sdv2kyoyg().s[177]++, 0); i < formData.items.length; i++) {
      const item =
      /* istanbul ignore next */
      (cov_sdv2kyoyg().s[178]++, formData.items[i]);
      /* istanbul ignore next */
      cov_sdv2kyoyg().s[179]++;
      if (
      /* istanbul ignore next */
      (cov_sdv2kyoyg().b[56][0]++, !item.productId) ||
      /* istanbul ignore next */
      (cov_sdv2kyoyg().b[56][1]++, Number(item.quantity) <= 0) ||
      /* istanbul ignore next */
      (cov_sdv2kyoyg().b[56][2]++, Number(item.price) < 0)) {
        /* istanbul ignore next */
        cov_sdv2kyoyg().b[55][0]++;
        cov_sdv2kyoyg().s[180]++;
        setError(`Please ensure all fields for item #${i + 1} (Product, valid Quantity, valid Price) are correctly filled.`);
        /* istanbul ignore next */
        cov_sdv2kyoyg().s[181]++;
        return;
      } else
      /* istanbul ignore next */
      {
        cov_sdv2kyoyg().b[55][1]++;
      }
      // Ensure we have a valid distribution object for this item index
      const distributedAmount =
      /* istanbul ignore next */
      (cov_sdv2kyoyg().s[182]++,
      /* istanbul ignore next */
      (cov_sdv2kyoyg().b[58][0]++, itemDistributions[i]) &&
      /* istanbul ignore next */
      (cov_sdv2kyoyg().b[58][1]++, typeof itemDistributions[i] === "object") ?
      /* istanbul ignore next */
      (cov_sdv2kyoyg().b[57][0]++, getTotalDistributedForItem(i)) :
      /* istanbul ignore next */
      (cov_sdv2kyoyg().b[57][1]++, 0));
      /* istanbul ignore next */
      cov_sdv2kyoyg().s[183]++;
      if (distributedAmount > Number(item.quantity)) {
        /* istanbul ignore next */
        cov_sdv2kyoyg().b[59][0]++;
        cov_sdv2kyoyg().s[184]++;
        setError(`Distributed quantity for ${
        /* istanbul ignore next */
        (cov_sdv2kyoyg().b[60][0]++, item.productName) ||
        /* istanbul ignore next */
        (cov_sdv2kyoyg().b[60][1]++, `item #${i + 1}`)} (${distributedAmount}) exceeds item quantity (${item.quantity}).`);
        /* istanbul ignore next */
        cov_sdv2kyoyg().s[185]++;
        return;
      } else
      /* istanbul ignore next */
      {
        cov_sdv2kyoyg().b[59][1]++;
      }
      // Auto-distribute to single shop if no distribution is set and only one shop has this product
      cov_sdv2kyoyg().s[186]++;
      if (
      /* istanbul ignore next */
      (cov_sdv2kyoyg().b[62][0]++, distributedAmount === 0) &&
      /* istanbul ignore next */
      (cov_sdv2kyoyg().b[62][1]++, Number(item.quantity) > 0)) {
        /* istanbul ignore next */
        cov_sdv2kyoyg().b[61][0]++;
        cov_sdv2kyoyg().s[187]++;
        // This will be handled by the backend logic, but we could add a warning here
        console.warn(`Item ${i + 1} (${item.productName}) has no distribution set. Backend will attempt to infer target shop.`);
      } else
      /* istanbul ignore next */
      {
        cov_sdv2kyoyg().b[61][1]++;
      }
    }
    /* istanbul ignore next */
    cov_sdv2kyoyg().s[188]++;
    setSubmitting(true);
    // Ensure distributions array matches items array length
    const syncedDistributions =
    /* istanbul ignore next */
    (cov_sdv2kyoyg().s[189]++, formData.items.map((_, index) => {
      /* istanbul ignore next */
      cov_sdv2kyoyg().f[41]++;
      cov_sdv2kyoyg().s[190]++;
      return /* istanbul ignore next */(cov_sdv2kyoyg().b[63][0]++, itemDistributions[index]) ||
      /* istanbul ignore next */
      (cov_sdv2kyoyg().b[63][1]++, {});
    }));
    const submissionData =
    /* istanbul ignore next */
    (cov_sdv2kyoyg().s[191]++, {
      ...formData,
      items: formData.items.map(item => {
        /* istanbul ignore next */
        cov_sdv2kyoyg().f[42]++;
        cov_sdv2kyoyg().s[192]++;
        return {
          productId: item.productId,
          quantity: Number(item.quantity),
          price: Number(item.price),
          id:
          /* istanbul ignore next */
          (cov_sdv2kyoyg().b[64][0]++, item.id) ||
          /* istanbul ignore next */
          (cov_sdv2kyoyg().b[64][1]++, undefined) // Keep item ID if it exists for update
        };
      }),
      distributions: syncedDistributions,
      totalAmount:
      /* istanbul ignore next */
      (cov_sdv2kyoyg().b[65][0]++, formData.totalAmount) ||
      /* istanbul ignore next */
      (cov_sdv2kyoyg().b[65][1]++, 0),
      paidAmount:
      /* istanbul ignore next */
      (cov_sdv2kyoyg().b[66][0]++, formData.paidAmount) ||
      /* istanbul ignore next */
      (cov_sdv2kyoyg().b[66][1]++, 0)
    });
    /* istanbul ignore next */
    cov_sdv2kyoyg().s[193]++;
    console.log("Submitting purchase invoice update with synced distributions:", {
      itemsCount: submissionData.items.length,
      distributionsCount: submissionData.distributions.length,
      distributions: submissionData.distributions
    });
    /* istanbul ignore next */
    cov_sdv2kyoyg().s[194]++;
    try {
      const response =
      /* istanbul ignore next */
      (cov_sdv2kyoyg().s[195]++, await fetch(`/api/purchases/${initialInvoice.id}`, {
        method: "PUT",
        headers: {
          "Content-Type": "application/json"
        },
        body: JSON.stringify(submissionData)
      }));
      /* istanbul ignore next */
      cov_sdv2kyoyg().s[196]++;
      if (!response.ok) {
        /* istanbul ignore next */
        cov_sdv2kyoyg().b[67][0]++;
        const errorData =
        /* istanbul ignore next */
        (cov_sdv2kyoyg().s[197]++, await response.json());
        /* istanbul ignore next */
        cov_sdv2kyoyg().s[198]++;
        throw new Error(
        /* istanbul ignore next */
        (cov_sdv2kyoyg().b[68][0]++, errorData.error) ||
        /* istanbul ignore next */
        (cov_sdv2kyoyg().b[68][1]++, errorData.message) ||
        /* istanbul ignore next */
        (cov_sdv2kyoyg().b[68][2]++, "Failed to update purchase invoice"));
      } else
      /* istanbul ignore next */
      {
        cov_sdv2kyoyg().b[67][1]++;
      }
      cov_sdv2kyoyg().s[199]++;
      router.push("/purchases?status=success&action=update");
    } catch (err) {
      /* istanbul ignore next */
      cov_sdv2kyoyg().s[200]++;
      setError(
      /* istanbul ignore next */
      (cov_sdv2kyoyg().b[69][0]++, err.message) ||
      /* istanbul ignore next */
      (cov_sdv2kyoyg().b[69][1]++, "An unexpected error occurred."));
    } finally {
      /* istanbul ignore next */
      cov_sdv2kyoyg().s[201]++;
      setSubmitting(false);
    }
  };
  /* istanbul ignore next */
  cov_sdv2kyoyg().s[202]++;
  const handleProductSearch = query => {
    /* istanbul ignore next */
    cov_sdv2kyoyg().f[43]++;
    cov_sdv2kyoyg().s[203]++;
    if (!query) {
      /* istanbul ignore next */
      cov_sdv2kyoyg().b[70][0]++;
      cov_sdv2kyoyg().s[204]++;
      setFilteredProducts(products);
      /* istanbul ignore next */
      cov_sdv2kyoyg().s[205]++;
      return;
