    }), /*#__PURE__*/(0, _jsxruntime.jsx)("div", {
      className: "max-w-6xl mx-auto px-6 py-4",
      children: /*#__PURE__*/(0, _jsxruntime.jsxs)("div", {
        className: "bg-gray-50 border border-gray-200 rounded-lg p-4",
        children: [/*#__PURE__*/(0, _jsxruntime.jsx)("h4", {
          className: "text-sm font-medium text-gray-800 mb-2",
          children: "\uD83D\uDCDD Edit Guidelines:"
        }), /*#__PURE__*/(0, _jsxruntime.jsxs)("ul", {
          className: "text-sm text-gray-600 space-y-1",
          children: [/*#__PURE__*/(0, _jsxruntime.jsx)("li", {
            children: " Changes are automatically validated in real-time"
          }), /*#__PURE__*/(0, _jsxruntime.jsx)("li", {
            children: " Inventory levels will be updated based on status changes"
          }), /*#__PURE__*/(0, _jsxruntime.jsx)("li", {
            children: " All modifications are logged for audit purposes"
          }), /*#__PURE__*/(0, _jsxruntime.jsx)("li", {
            children: " Use caution when editing paid invoices"
          })]
        })]
      })
    })]
  });
}
