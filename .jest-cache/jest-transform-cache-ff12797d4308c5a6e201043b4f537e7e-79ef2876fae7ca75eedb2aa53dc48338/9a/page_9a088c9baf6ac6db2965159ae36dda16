                }), /*#__PURE__*/(0, _jsxruntime.jsx)("input", {
                  type: "number",
                  name: "creditPeriod",
                  value: quickCustomerData.creditPeriod,
                  onChange: handleQuickCustomerInputChange,
                  className: "w-full rounded-md border border-gray-300 p-2.5 text-sm text-gray-900",
                  min: "0"
                })]
              })]
            })), /*#__PURE__*/(0, _jsxruntime.jsxs)("div", {
              className: "flex justify-end gap-3 pt-4",
              children: [/*#__PURE__*/(0, _jsxruntime.jsx)(_Button.Button, {
                type: "button",
                variant: "outline",
                onClick: () => {
                  /* istanbul ignore next */
                  cov_y2tbfk2n7().f[63]++;
                  cov_y2tbfk2n7().s[298]++;
                  setShowQuickCustomerModal(false);
                  /* istanbul ignore next */
                  cov_y2tbfk2n7().s[299]++;
                  setQuickCustomerData({
                    name: "",
                    phone: "",
                    address: "",
                    customerType: "Retail",
                    creditLimit: 0,
                    creditPeriod: 0
                  });
                },
                size: "sm",
                children: "Cancel"
              }), /*#__PURE__*/(0, _jsxruntime.jsx)(_Button.Button, {
                type: "submit",
                variant: "primary",
                isLoading: isCreatingCustomer,
                disabled: !quickCustomerData.name.trim(),
                size: "sm",
                children: "Create Customer"
              })]
            })]
          })]
        })
      }))]
    })
  });
}
