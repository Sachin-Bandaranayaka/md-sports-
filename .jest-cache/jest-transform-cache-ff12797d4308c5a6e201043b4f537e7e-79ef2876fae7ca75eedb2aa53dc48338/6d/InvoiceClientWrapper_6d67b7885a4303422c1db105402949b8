        const response =
        /* istanbul ignore next */
        (cov_inz4lgakk().s[173]++, await fetch(`/api/invoices/${invoiceId}`, {
          method: "DELETE",
          headers: {
            "Authorization": accessToken ?
            /* istanbul ignore next */
            (cov_inz4lgakk().b[37][0]++, `Bearer ${accessToken}`) :
            /* istanbul ignore next */
