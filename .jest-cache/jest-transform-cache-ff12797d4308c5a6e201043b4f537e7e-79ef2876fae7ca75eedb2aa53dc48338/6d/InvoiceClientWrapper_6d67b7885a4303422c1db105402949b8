  /* istanbul ignore next */
  (cov_inz4lgakk().b[9][1]++, undefined)));
  const [sortBy, setSortBy] =
  /* istanbul ignore next */
  (cov_inz4lgakk().s[55]++, (0, _react.useState)(
  /* istanbul ignore next */
  (cov_inz4lgakk().b[10][0]++, searchParams.get("sortBy")) ||
  /* istanbul ignore next */
  (cov_inz4lgakk().b[10][1]++, "newest")));
  const [dateSortOrder, setDateSortOrder] =
  /* istanbul ignore next */
  (cov_inz4lgakk().s[56]++, (0, _react.useState)("desc")); // desc = newest first, asc = oldest first
  const [dueStatusSortOrder, setDueStatusSortOrder] =
  /* istanbul ignore next */
  (cov_inz4lgakk().s[57]++, (0, _react.useState)("asc")); // asc = overdue first, desc = current first
  // Selection state for bulk operations
  const [selectedInvoices, setSelectedInvoices] =
  /* istanbul ignore next */
  (cov_inz4lgakk().s[58]++, (0, _react.useState)(new Set()));
  const [selectAll, setSelectAll] =
  /* istanbul ignore next */
  (cov_inz4lgakk().s[59]++, (0, _react.useState)(false));
  // Selection handlers
  const handleToggleSelection =
  /* istanbul ignore next */
  (cov_inz4lgakk().s[60]++, (0, _react.useCallback)(invoiceId => {
    /* istanbul ignore next */
    cov_inz4lgakk().f[5]++;
    cov_inz4lgakk().s[61]++;
    setSelectedInvoices(prev => {
      /* istanbul ignore next */
      cov_inz4lgakk().f[6]++;
      const newSet =
      /* istanbul ignore next */
      (cov_inz4lgakk().s[62]++, new Set(prev));
      /* istanbul ignore next */
      cov_inz4lgakk().s[63]++;
