      /* istanbul ignore next */
      cov_inz4lgakk().s[188]++;
      return;
    } else
    /* istanbul ignore next */
    {
      cov_inz4lgakk().b[41][1]++;
    }
    cov_inz4lgakk().s[189]++;
    if (!confirm(`Are you sure you want to delete ${selectedInvoices.size} selected invoice(s)?`)) {
      /* istanbul ignore next */
      cov_inz4lgakk().b[42][0]++;
      cov_inz4lgakk().s[190]++;
      return;
    } else
