        /* istanbul ignore next */
        {
          cov_inz4lgakk().b[38][1]++;
        }
        // Remove from selection if it was selected
        cov_inz4lgakk().s[177]++;
        setSelectedInvoices(prev => {
          /* istanbul ignore next */
          cov_inz4lgakk().f[29]++;
          const newSet =
          /* istanbul ignore next */
          (cov_inz4lgakk().s[178]++, new Set(prev));
          /* istanbul ignore next */
