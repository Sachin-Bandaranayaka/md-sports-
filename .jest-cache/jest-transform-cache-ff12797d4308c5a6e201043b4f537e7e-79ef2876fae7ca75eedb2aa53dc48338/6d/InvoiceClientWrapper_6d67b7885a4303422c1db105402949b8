  const handleFilterChange = () => {
    /* istanbul ignore next */
    cov_inz4lgakk().f[24]++;
    const params =
    /* istanbul ignore next */
    (cov_inz4lgakk().s[127]++, new URLSearchParams(searchParams));
    /* istanbul ignore next */
    cov_inz4lgakk().s[128]++;
    if (searchQuery) {
      /* istanbul ignore next */
      cov_inz4lgakk().b[28][0]++;
      cov_inz4lgakk().s[129]++;
