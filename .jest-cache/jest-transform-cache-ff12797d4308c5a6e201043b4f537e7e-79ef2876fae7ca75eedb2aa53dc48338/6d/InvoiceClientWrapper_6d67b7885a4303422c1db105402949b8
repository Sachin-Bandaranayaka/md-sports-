          /* istanbul ignore next */
          (cov_inz4lgakk().s[175]++, await response.json());
          /* istanbul ignore next */
          cov_inz4lgakk().s[176]++;
          throw new Error(
          /* istanbul ignore next */
          (cov_inz4lgakk().b[39][0]++, errData.message) ||
          /* istanbul ignore next */
          (cov_inz4lgakk().b[39][1]++, "Failed to delete invoice"));
