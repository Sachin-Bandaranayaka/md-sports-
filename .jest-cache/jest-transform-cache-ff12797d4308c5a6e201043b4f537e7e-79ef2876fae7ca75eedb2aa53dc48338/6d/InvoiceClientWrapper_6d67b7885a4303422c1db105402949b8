          /* istanbul ignore next */
          cov_inz4lgakk().s[180]++;
          return newSet;
        });
        /* istanbul ignore next */
        cov_inz4lgakk().s[181]++;
        router.refresh(); // Refresh data
      } catch (err) {
        /* istanbul ignore next */
        cov_inz4lgakk().s[182]++;
        console.error("Error deleting invoice:", err);
        /* istanbul ignore next */
        cov_inz4lgakk().s[183]++;
        setError(
        /* istanbul ignore next */
        (cov_inz4lgakk().b[40][0]++, err.message) ||
        /* istanbul ignore next */
        (cov_inz4lgakk().b[40][1]++, "Failed to delete invoice. Please try again."));
      } finally {
        /* istanbul ignore next */
        cov_inz4lgakk().s[184]++;
        setLoading(false);
      }
    } else
    /* istanbul ignore next */
    {
      cov_inz4lgakk().b[36][1]++;
    }
  };
  // Bulk delete handler
  /* istanbul ignore next */
  cov_inz4lgakk().s[185]++;
  const handleBulkDelete = async () => {
    /* istanbul ignore next */
    cov_inz4lgakk().f[30]++;
    cov_inz4lgakk().s[186]++;
    if (selectedInvoices.size === 0) {
      /* istanbul ignore next */
      cov_inz4lgakk().b[41][0]++;
      cov_inz4lgakk().s[187]++;
