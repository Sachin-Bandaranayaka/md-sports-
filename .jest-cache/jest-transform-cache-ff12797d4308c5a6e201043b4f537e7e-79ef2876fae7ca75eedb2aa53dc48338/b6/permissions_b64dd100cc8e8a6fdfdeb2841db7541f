  /* istanbul ignore next */
  cov_djshe31a4().f[6]++;
  const validPatterns =
  /* istanbul ignore next */
  (cov_djshe31a4().s[11]++, [/^[a-z]+:[a-z]+$/, /^[a-z]+:[a-z]+:[a-z]+$/, /^[a-z]+:[a-z_]+$/, /^[a-z_]+:[a-z_]+$/, /^[a-z_]+$/, /^[a-z]+:[a-z]+$/, /^\*$/, /^admin:all$/, /^ALL$/ // legacy admin permission
  ]);
  /* istanbul ignore next */
  cov_djshe31a4().s[12]++;
  return validPatterns.some(pattern => {
