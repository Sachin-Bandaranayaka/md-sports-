function Loading() {
  /* istanbul ignore next */
  cov_tdd3mgs7s().f[17]++;
  cov_tdd3mgs7s().s[87]++;
  return /*#__PURE__*/(0, _jsxruntime.jsx)(_MainLayout.default, {
    children: /*#__PURE__*/(0, _jsxruntime.jsx)("div", {
      className: "container max-w-md mx-auto py-8 px-4",
      children: /*#__PURE__*/(0, _jsxruntime.jsxs)("div", {
