        /* istanbul ignore next */
        cov_otzpkwz9a().s[199]++;
        _sonner.toast.error("Failed to parse CSV file");
      }
    };
    /* istanbul ignore next */
    cov_otzpkwz9a().s[200]++;
    reader.readAsText(file);
    /* istanbul ignore next */
    cov_otzpkwz9a().s[201]++;
    event.target.value = ""; // Reset input
  }, [products]));
