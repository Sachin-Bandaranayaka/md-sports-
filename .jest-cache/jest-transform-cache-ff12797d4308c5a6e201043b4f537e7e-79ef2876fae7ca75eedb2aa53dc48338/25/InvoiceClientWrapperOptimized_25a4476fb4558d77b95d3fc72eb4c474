      /* istanbul ignore next */
      cov_1lkrlxx5yj().b[11][0]++;
      cov_1lkrlxx5yj().s[59]++;
      params.append(key, String(value));
    } else
    /* istanbul ignore next */
    {
      cov_1lkrlxx5yj().b[11][1]++;
    }
  });
  const response =
  /* istanbul ignore next */
  (cov_1lkrlxx5yj().s[60]++, await fetch(`/api/invoices?${params}`));
  /* istanbul ignore next */
  cov_1lkrlxx5yj().s[61]++;
