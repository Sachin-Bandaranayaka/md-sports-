", "confirm", "mutate", "id", "handleRecordPayment", "handleCreateSuccess", "setTimeout", "refetchQueries", "handleEditSuccess", "listData", "p", "message", "h3", "totalOutstanding", "paidThisMonth", "overdueCount", "totalCreditSales", "totalNonCreditSales", "h1", "Plus", "Search", "input", "type", "placeholder", "defaultValue", "onChange", "e", "target", "select", "option", "Loader2", "length", "FileText", "List", "height", "itemSize", "itemData", "disabled", "InvoiceCreateModal", "isOpen", "onClose", "InvoiceEditModal", "InvoiceViewModal"],
