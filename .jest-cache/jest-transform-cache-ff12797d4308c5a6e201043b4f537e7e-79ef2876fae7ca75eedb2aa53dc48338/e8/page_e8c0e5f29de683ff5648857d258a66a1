                      }),
                      /* istanbul ignore next */
                      (cov_o6p580jko().b[68][0]++, canViewCosts) &&
                      /* istanbul ignore next */
                      (cov_o6p580jko().b[68][1]++, /*#__PURE__*/(0, _jsxruntime.jsx)("td", {
                        className: "px-4 py-3 whitespace-nowrap text-sm text-black",
                        children: /*#__PURE__*/(0, _jsxruntime.jsxs)("div", {
                          className: "space-y-1",
                          children: [/*#__PURE__*/(0, _jsxruntime.jsxs)("div", {
                            className: "text-xs text-gray-600",
                            children: ["WAC: Rs. ", product.weightedAverageCost.toLocaleString()]
                          }),
                          /* istanbul ignore next */
                          (cov_o6p580jko().b[69][0]++, shopStock) &&
                          /* istanbul ignore next */
                          (cov_o6p580jko().b[69][1]++, shopStock.shopSpecificCost > 0) &&
                          /* istanbul ignore next */
                          (cov_o6p580jko().b[69][2]++, /*#__PURE__*/(0, _jsxruntime.jsxs)("div", {
                            className: "text-xs text-blue-600",
                            children: ["Shop Cost: Rs. ", shopStock.shopSpecificCost.toLocaleString()]
                          }))]
                        })
                      })), /*#__PURE__*/(0, _jsxruntime.jsxs)("td", {
                        className: "px-4 py-3 whitespace-nowrap text-sm text-black",
                        children: ["Rs. ", product.retailPrice.toLocaleString()]
                      }), /*#__PURE__*/(0, _jsxruntime.jsxs)("td", {
                        className: "px-4 py-3 whitespace-nowrap text-sm text-black",
                        children: ["Rs. ", retailValue.toLocaleString()]
                      }),
                      /* istanbul ignore next */
                      (cov_o6p580jko().b[70][0]++, canViewCosts) &&
                      /* istanbul ignore next */
                      (cov_o6p580jko().b[70][1]++, /*#__PURE__*/(0, _jsxruntime.jsxs)("td", {
                        className: "px-4 py-3 whitespace-nowrap text-sm font-semibold text-green-600",
                        children: ["Rs. ", totalShopCost.toLocaleString()]
                      })), /*#__PURE__*/(0, _jsxruntime.jsx)("td", {
                        className: "px-4 py-3 whitespace-nowrap text-sm text-center",
                        children: /*#__PURE__*/(0, _jsxruntime.jsx)(_Button.Button, {
                          variant: "outline",
                          size: "sm",
                          onClick: () => {
                            /* istanbul ignore next */
                            cov_o6p580jko().f[44]++;
                            cov_o6p580jko().s[152]++;
                            return goToProductDetails(product.id);
                          },
                          className: "text-black",
                          children: "View"
                        })
                      })]
                    }, product.id);
                  })
                })]
              }),
              /* istanbul ignore next */
              (cov_o6p580jko().b[71][0]++, sortedProducts.filter(product => {
                /* istanbul ignore next */
                cov_o6p580jko().f[45]++;
                cov_o6p580jko().s[153]++;
                return product.branchStock.some(branch => {
                  /* istanbul ignore next */
                  cov_o6p580jko().f[46]++;
                  cov_o6p580jko().s[154]++;
                  return branch.shopId === shop.id;
                });
              }).length === 0) &&
              /* istanbul ignore next */
              (cov_o6p580jko().b[71][1]++, /*#__PURE__*/(0, _jsxruntime.jsx)("div", {
                className: "text-center py-4 text-gray-500",
                children: "No products found in this shop matching your filters."
              }))]
            })]
          }, shop.id);
        })) :
        /* istanbul ignore next */
        (/*#__PURE__*/cov_o6p580jko().b[64][1]++, (0, _jsxruntime.jsxs)("div", {
          className: "bg-white p-8 rounded-xl shadow-sm border border-gray-200 text-center",
          children: [/*#__PURE__*/(0, _jsxruntime.jsx)(_lucidereact.Package, {
            className: "h-12 w-12 mx-auto text-gray-400 mb-4"
          }), /*#__PURE__*/(0, _jsxruntime.jsx)("h3", {
            className: "text-lg font-medium text-black mb-2",
            children: "No shops found"
          }), /*#__PURE__*/(0, _jsxruntime.jsx)("p", {
            className: "text-gray-500 mb-4",
            children: "There are no shops in the system or you don't have access to any shops."
          })]
        }))
      }))]
    })
  });
}
