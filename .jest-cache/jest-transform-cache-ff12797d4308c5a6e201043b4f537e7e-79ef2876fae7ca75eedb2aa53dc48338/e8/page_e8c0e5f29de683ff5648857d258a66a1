                shopId: inv.shop_id,
                shopName: inv.shop_name,
                quantity: inv.quantity,
                shopSpecificCost:
                /* istanbul ignore next */
                (cov_o6p580jko().b[9][0]++, parseFloat(inv.shop_specific_cost)) ||
                /* istanbul ignore next */
                (cov_o6p580jko().b[9][1]++, 0)
              });
            });
          } else
          /* istanbul ignore next */
          {
            cov_o6p580jko().b[7][1]++;
          }
          cov_o6p580jko().s[50]++;
          transformedProducts.push({
            id: product.id,
            name: product.name,
            sku:
            /* istanbul ignore next */
            (cov_o6p580jko().b[10][0]++, product.sku) ||
            /* istanbul ignore next */
            (cov_o6p580jko().b[10][1]++, ""),
