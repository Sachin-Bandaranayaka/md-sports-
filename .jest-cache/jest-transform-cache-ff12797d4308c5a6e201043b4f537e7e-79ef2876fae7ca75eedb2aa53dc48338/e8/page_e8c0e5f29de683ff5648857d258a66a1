            /* istanbul ignore next */
            (cov_o6p580jko().b[11][0]++, parseFloat(product.price)) ||
            /* istanbul ignore next */
            (cov_o6p580jko().b[11][1]++, 0),
            weightedAverageCost:
            /* istanbul ignore next */
            (cov_o6p580jko().b[12][0]++, parseFloat(product.weightedAverageCost)) ||
            /* istanbul ignore next */
            (cov_o6p580jko().b[12][1]++, 0),
            totalStock,
            branchStock
          });
        } else
        /* istanbul ignore next */
        {
          cov_o6p580jko().b[6][1]++;
        }
