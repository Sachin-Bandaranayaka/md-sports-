            inventoryData.data.inventory.forEach(inv => {
              /* istanbul ignore next */
              cov_o6p580jko().f[5]++;
              cov_o6p580jko().s[46]++;
              totalStock += inv.quantity;
              // Add shop to the map if not already present
              /* istanbul ignore next */
