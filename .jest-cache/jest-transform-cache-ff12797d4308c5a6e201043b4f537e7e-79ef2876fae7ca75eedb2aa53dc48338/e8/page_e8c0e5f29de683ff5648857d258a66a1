          /* istanbul ignore next */
          cov_o6p580jko().b[6][0]++;
          const branchStock =
          /* istanbul ignore next */
          (cov_o6p580jko().s[42]++, []);
          let totalStock =
          /* istanbul ignore next */
          (cov_o6p580jko().s[43]++, 0);
          /* istanbul ignore next */
          cov_o6p580jko().s[44]++;
          if (inventoryData.data.inventory) {
            /* istanbul ignore next */
            cov_o6p580jko().b[7][0]++;
