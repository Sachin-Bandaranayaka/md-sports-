            cov_qig5kn2rc().s[124]++;
            dueDate = dueDateObj.toISOString().split("T")[0];
            /* istanbul ignore next */
            cov_qig5kn2rc().s[125]++;
            status = "pending";
          } else
          /* istanbul ignore next */
          {
            cov_qig5kn2rc().b[35][1]++;
          }
        } else
        /* istanbul ignore next */
        {
          cov_qig5kn2rc().b[31][1]++;
        }
      } else
      /* istanbul ignore next */
      {
        cov_qig5kn2rc().b[30][1]++;
      }
    } catch (error) {
      /* istanbul ignore next */
      cov_qig5kn2rc().s[126]++;
      console.error("Error fetching customer details:", error);
    }
    // Update form data with customer info and calculated dates/status
    /* istanbul ignore next */
    cov_qig5kn2rc().s[127]++;
    setFormData({
      ...formData,
      customerId: customer.id,
      customerName: customer.name,
      invoiceDate: invoiceDate,
      dueDate: dueDate,
      status: status
    });
    /* istanbul ignore next */
    cov_qig5kn2rc().s[128]++;
    setCustomerSearch("");
    /* istanbul ignore next */
    cov_qig5kn2rc().s[129]++;
    setShowCustomerDropdown(false);
  };
  // Handle product selection for adding to line items
  /* istanbul ignore next */
  cov_qig5kn2rc().s[130]++;
  const handleSelectProduct = async product => {
    /* istanbul ignore next */
    cov_qig5kn2rc().f[18]++;
    cov_qig5kn2rc().s[131]++;
    // Fetch full product details to get weightedAverageCost for newly added items
    if (!formData.shopId) {
      /* istanbul ignore next */
      cov_qig5kn2rc().b[37][0]++;
      cov_qig5kn2rc().s[132]++;
      alert("Please select a shop first before adding products.");
      /* istanbul ignore next */
      cov_qig5kn2rc().s[133]++;
      return;
    } else
    /* istanbul ignore next */
    {
      cov_qig5kn2rc().b[37][1]++;
    }
    cov_qig5kn2rc().s[134]++;
    try {
      const response =
      /* istanbul ignore next */
      (cov_qig5kn2rc().s[135]++, await fetch(`/api/products/${product.id}`));
      /* istanbul ignore next */
      cov_qig5kn2rc().s[136]++;
