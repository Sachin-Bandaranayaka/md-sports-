    cov_qig5kn2rc().s[177]++;
    setSelectedProduct(null);
    /* istanbul ignore next */
    cov_qig5kn2rc().s[178]++;
    setProductStock(null);
    /* istanbul ignore next */
    cov_qig5kn2rc().s[179]++;
    setQuantity(1);
    /* istanbul ignore next */
    cov_qig5kn2rc().s[180]++;
    setCustomPrice(0);
  };
