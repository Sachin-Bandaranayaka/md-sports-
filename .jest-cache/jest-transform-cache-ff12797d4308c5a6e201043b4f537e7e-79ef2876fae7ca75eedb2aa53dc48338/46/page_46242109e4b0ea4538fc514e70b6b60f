            /* istanbul ignore next */
            cov_qig5kn2rc().f[19]++;
            cov_qig5kn2rc().s[142]++;
            return invItem.shop_id === formData.shopId;
          } // Assuming shopId on inventoryItem is number
          )?.quantity) ||
          /* istanbul ignore next */
          (cov_qig5kn2rc().b[42][1]++, 0));
          /* istanbul ignore next */
          cov_qig5kn2rc().s[143]++;
          setProductStock(shopStock);
          /* istanbul ignore next */
          cov_qig5kn2rc().s[144]++;
          setCustomPrice(0); // Reset custom price when product is selected
        } else {
          /* istanbul ignore next */
          cov_qig5kn2rc().b[39][1]++;
          cov_qig5kn2rc().s[145]++;
          setSelectedProduct(product);
          /* istanbul ignore next */
          cov_qig5kn2rc().s[146]++;
          setCurrentProductCost(
          /* istanbul ignore next */
          (cov_qig5kn2rc().b[43][0]++, product.weightedAverageCost) ||
          /* istanbul ignore next */
          (cov_qig5kn2rc().b[43][1]++, 0));
          /* istanbul ignore next */
          cov_qig5kn2rc().s[147]++;
          setProductStock(null);
          /* istanbul ignore next */
          cov_qig5kn2rc().s[148]++;
          setCustomPrice(0); // Reset custom price when product is selected
        }
      } else {
        /* istanbul ignore next */
        cov_qig5kn2rc().b[38][1]++;
        cov_qig5kn2rc().s[149]++;
        setSelectedProduct(product);
        /* istanbul ignore next */
        cov_qig5kn2rc().s[150]++;
        setCurrentProductCost(
        /* istanbul ignore next */
        (cov_qig5kn2rc().b[44][0]++, product.weightedAverageCost) ||
        /* istanbul ignore next */
        (cov_qig5kn2rc().b[44][1]++, 0));
        /* istanbul ignore next */
        cov_qig5kn2rc().s[151]++;
        setProductStock(null);
        /* istanbul ignore next */
        cov_qig5kn2rc().s[152]++;
        setCustomPrice(0); // Reset custom price when product is selected
      }
