    /* istanbul ignore next */
    cov_qig5kn2rc().f[23]++;
    cov_qig5kn2rc().s[185]++;
    console.log("Updating quantity:", itemId, newQuantity);
    /* istanbul ignore next */
    cov_qig5kn2rc().s[186]++;
    if (newQuantity <= 0) {
      /* istanbul ignore next */
      cov_qig5kn2rc().b[52][0]++;
      cov_qig5kn2rc().s[187]++;
      // If new quantity is invalid, find the item and revert its input to the current quantity
      setFormData(prevData => {
        /* istanbul ignore next */
        cov_qig5kn2rc().f[24]++;
        const currentItem =
        /* istanbul ignore next */
        (cov_qig5kn2rc().s[188]++, prevData.items.find(item => {
          /* istanbul ignore next */
          cov_qig5kn2rc().f[25]++;
          cov_qig5kn2rc().s[189]++;
          return item.id === itemId;
        }));
        /* istanbul ignore next */
        cov_qig5kn2rc().s[190]++;
        if (currentItem) {
          /* istanbul ignore next */
          cov_qig5kn2rc().b[53][0]++;
          // Trigger a re-render with the existing quantity to reset the input
          const updatedItems =
          /* istanbul ignore next */
          (cov_qig5kn2rc().s[191]++, prevData.items.map(item => {
            /* istanbul ignore next */
            cov_qig5kn2rc().f[26]++;
            cov_qig5kn2rc().s[192]++;
            if (item.id === itemId) {
              /* istanbul ignore next */
              cov_qig5kn2rc().b[54][0]++;
              cov_qig5kn2rc().s[193]++;
              return {
                ...item,
                quantity: currentItem.quantity
              };
            } else
            /* istanbul ignore next */
            {
              cov_qig5kn2rc().b[54][1]++;
            }
            cov_qig5kn2rc().s[194]++;
            return item;
          }));
          /* istanbul ignore next */
          cov_qig5kn2rc().s[195]++;
          return {
            ...prevData,
            items: updatedItems
          };
        } else
        /* istanbul ignore next */
        {
          cov_qig5kn2rc().b[53][1]++;
        }
        cov_qig5kn2rc().s[196]++;
        return prevData;
      });
      /* istanbul ignore next */
      cov_qig5kn2rc().s[197]++;
      return;
    } else
    /* istanbul ignore next */
    {
      cov_qig5kn2rc().b[52][1]++;
    }
    cov_qig5kn2rc().s[198]++;
    setFormData(prevData => {
      /* istanbul ignore next */
      cov_qig5kn2rc().f[27]++;
      cov_qig5kn2rc().s[199]++;
      console.log("Previous formData:", prevData);
      const updatedItems =
      /* istanbul ignore next */
      (cov_qig5kn2rc().s[200]++, prevData.items.map(item => {
        /* istanbul ignore next */
        cov_qig5kn2rc().f[28]++;
        cov_qig5kn2rc().s[201]++;
        if (item.id === itemId) {
          /* istanbul ignore next */
          cov_qig5kn2rc().b[55][0]++;
          cov_qig5kn2rc().s[202]++;
          console.log("Updating item:", item.id, "from", item.quantity, "to", newQuantity);
          /* istanbul ignore next */
          cov_qig5kn2rc().s[203]++;
          return {
            ...item,
            quantity: newQuantity,
            total: item.price * newQuantity
          };
        } else
        /* istanbul ignore next */
        {
          cov_qig5kn2rc().b[55][1]++;
        }
        cov_qig5kn2rc().s[204]++;
        return item;
      }));
      /* istanbul ignore next */
      cov_qig5kn2rc().s[205]++;
      console.log("Updated items:", updatedItems);
      /* istanbul ignore next */
      cov_qig5kn2rc().s[206]++;
      return {
        ...prevData,
        items: updatedItems
      };
    });
  };
  // Handle form submission
  /* istanbul ignore next */
  cov_qig5kn2rc().s[207]++;
  const handleSubmit = async e => {
    /* istanbul ignore next */
    cov_qig5kn2rc().f[29]++;
    cov_qig5kn2rc().s[208]++;
    e.preventDefault();
    /* istanbul ignore next */
    cov_qig5kn2rc().s[209]++;
    setIsSubmitting(true);
    /* istanbul ignore next */
    cov_qig5kn2rc().s[210]++;
    try {
      // Transform data for API
      const invoiceData =
      /* istanbul ignore next */
      (cov_qig5kn2rc().s[211]++, {
        customerId: formData.customerId,
        total: invoiceTotal,
        status: formData.status,
        paymentMethod: formData.paymentMethod,
