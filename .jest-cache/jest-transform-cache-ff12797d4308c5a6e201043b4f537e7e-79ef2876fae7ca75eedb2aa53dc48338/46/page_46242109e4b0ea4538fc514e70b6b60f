    /* istanbul ignore next */
    (cov_qig5kn2rc().s[172]++, customPrice > 0 ?
    /* istanbul ignore next */
    (cov_qig5kn2rc().b[51][0]++, customPrice) :
    /* istanbul ignore next */
    (cov_qig5kn2rc().b[51][1]++, selectedProduct.price)); // Use custom price if set, otherwise default price
    const costPrice =
    /* istanbul ignore next */
    (cov_qig5kn2rc().s[173]++, currentProductCost); // Cost for the new item
    const itemTotal =
    /* istanbul ignore next */
    (cov_qig5kn2rc().s[174]++, sellingPrice * quantity);
    const newItem =
    /* istanbul ignore next */
    (cov_qig5kn2rc().s[175]++, {
      id: Date.now().toString(),
      productId: selectedProduct.id,
      productName: selectedProduct.name,
      quantity: quantity,
      price: sellingPrice,
      costPrice: costPrice,
      total: itemTotal
    });
