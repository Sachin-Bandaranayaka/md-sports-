  /* istanbul ignore next */
  cov_qig5kn2rc().s[181]++;
  const handleRemoveLineItem = itemId => {
    /* istanbul ignore next */
    cov_qig5kn2rc().f[21]++;
    cov_qig5kn2rc().s[182]++;
