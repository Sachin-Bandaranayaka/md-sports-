      cov_qig5kn2rc().s[153]++;
      setSelectedProduct(product);
      /* istanbul ignore next */
      cov_qig5kn2rc().s[154]++;
      setCurrentProductCost(
      /* istanbul ignore next */
      (cov_qig5kn2rc().b[45][0]++, product.weightedAverageCost) ||
      /* istanbul ignore next */
      (cov_qig5kn2rc().b[45][1]++, 0));
