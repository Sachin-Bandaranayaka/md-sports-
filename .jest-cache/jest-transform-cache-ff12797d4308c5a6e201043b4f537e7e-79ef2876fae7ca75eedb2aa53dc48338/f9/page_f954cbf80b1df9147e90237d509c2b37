const StatusBadge = ({
  status
}) => {
  /* istanbul ignore next */
  cov_5dsp9jajn().f[5]++;
  cov_5dsp9jajn().s[35]++;
  const getStatusBadgeClass = status => {
    /* istanbul ignore next */
    cov_5dsp9jajn().f[6]++;
    cov_5dsp9jajn().s[36]++;
    switch (status) {
      case "paid":
        /* istanbul ignore next */
        cov_5dsp9jajn().b[10][0]++;
        cov_5dsp9jajn().s[37]++;
        return "bg-green-100 text-green-800 border-green-200";
      case "partial":
        /* istanbul ignore next */
        cov_5dsp9jajn().b[10][1]++;
        cov_5dsp9jajn().s[38]++;
        return "bg-yellow-100 text-yellow-800 border-yellow-200";
      case "unpaid":
        /* istanbul ignore next */
        cov_5dsp9jajn().b[10][2]++;
        cov_5dsp9jajn().s[39]++;
        return "bg-red-100 text-red-800 border-red-200";
      case "overdue":
        /* istanbul ignore next */
        cov_5dsp9jajn().b[10][3]++;
        cov_5dsp9jajn().s[40]++;
        return "bg-red-100 text-red-800 border-red-200";
      default:
        /* istanbul ignore next */
        cov_5dsp9jajn().b[10][4]++;
        cov_5dsp9jajn().s[41]++;
        return "bg-blue-100 text-blue-800 border-blue-200";
    }
  };
  /* istanbul ignore next */
  cov_5dsp9jajn().s[42]++;
  return /*#__PURE__*/(0, _jsxruntime.jsx)("span", {
    className: `px-3 py-1 rounded-full text-sm font-medium border ${getStatusBadgeClass(status)}`,
    children: status?.charAt(0).toUpperCase() + (
    /* istanbul ignore next */
    (cov_5dsp9jajn().b[11][0]++, status?.slice(1)) ||
    /* istanbul ignore next */
    (cov_5dsp9jajn().b[11][1]++, ""))
  });
};
