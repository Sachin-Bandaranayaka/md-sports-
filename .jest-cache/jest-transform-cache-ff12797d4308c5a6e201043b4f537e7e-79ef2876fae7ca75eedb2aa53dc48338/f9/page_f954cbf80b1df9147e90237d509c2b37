  cov_5dsp9jajn().f[4]++;
  cov_5dsp9jajn().s[25]++;
  try {
    const response =
    /* istanbul ignore next */
    (cov_5dsp9jajn().s[26]++, await fetch(`${baseUrl}/api/shops`, {
      cache: "no-store",
      headers: {
        "Authorization": "Bearer dev-token"
      }
    }));
    /* istanbul ignore next */
    cov_5dsp9jajn().s[27]++;
    if (!response.ok) {
      /* istanbul ignore next */
      cov_5dsp9jajn().b[8][0]++;
      cov_5dsp9jajn().s[28]++;
      console.error("Failed to fetch shops:", response.status);
      /* istanbul ignore next */
      cov_5dsp9jajn().s[29]++;
      return [];
    } else
    /* istanbul ignore next */
    {
      cov_5dsp9jajn().b[8][1]++;
    }
    const data =
    /* istanbul ignore next */
    (cov_5dsp9jajn().s[30]++, await response.json());
    /* istanbul ignore next */
    cov_5dsp9jajn().s[31]++;
    return data.success ?
    /* istanbul ignore next */
    (cov_5dsp9jajn().b[9][0]++, data.data) :
    /* istanbul ignore next */
    (cov_5dsp9jajn().b[9][1]++, data);
  } catch (error) {
    /* istanbul ignore next */
    cov_5dsp9jajn().s[32]++;
    console.error("Error fetching shops:", error);
    /* istanbul ignore next */
    cov_5dsp9jajn().s[33]++;
    return [];
  }
}
