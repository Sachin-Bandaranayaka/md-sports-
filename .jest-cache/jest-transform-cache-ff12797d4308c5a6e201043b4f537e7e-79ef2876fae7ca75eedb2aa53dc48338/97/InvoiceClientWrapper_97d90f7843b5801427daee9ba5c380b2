      cov_inz4lgakk().b[0][5]++;
      cov_inz4lgakk().s[22]++;
      return "bg-gray-100 text-gray-800";
  }
};
// Shop-based row colors
/* istanbul ignore next */
cov_inz4lgakk().s[23]++;
const getShopRowClass = shopName => {
  /* istanbul ignore next */
  cov_inz4lgakk().f[2]++;
  cov_inz4lgakk().s[24]++;
  if (!shopName) {
    /* istanbul ignore next */
    cov_inz4lgakk().b[1][0]++;
    cov_inz4lgakk().s[25]++;
    return "hover:bg-gray-50";
  } else
  /* istanbul ignore next */
  {
    cov_inz4lgakk().b[1][1]++;
  }
  cov_inz4lgakk().s[26]++;
  switch (shopName.toLowerCase()) {
    case "mba":
      /* istanbul ignore next */
      cov_inz4lgakk().b[2][0]++;
    case "mba branch":
      /* istanbul ignore next */
      cov_inz4lgakk().b[2][1]++;
      cov_inz4lgakk().s[27]++;
      return "bg-blue-50 hover:bg-blue-100 border-l-4 border-blue-400";
    case "zymantra":
      /* istanbul ignore next */
      cov_inz4lgakk().b[2][2]++;
    case "zymantra branch":
      /* istanbul ignore next */
      cov_inz4lgakk().b[2][3]++;
    case "zimantra":
      /* istanbul ignore next */
      cov_inz4lgakk().b[2][4]++;
    case "zimantra branch":
      /* istanbul ignore next */
      cov_inz4lgakk().b[2][5]++;
      cov_inz4lgakk().s[28]++;
      return "bg-green-50 hover:bg-green-100 border-l-4 border-green-400";
    default:
      /* istanbul ignore next */
      cov_inz4lgakk().b[2][6]++;
      cov_inz4lgakk().s[29]++;
      return "hover:bg-gray-50";
  }
};
function InvoiceClientWrapper({
  initialInvoices,
  initialTotalPages,
  initialCurrentPage,
  initialStatistics,
  shops
}) {
  /* istanbul ignore next */
  cov_inz4lgakk().f[3]++;
  const router =
  /* istanbul ignore next */
  (cov_inz4lgakk().s[30]++, (0, _navigation.useRouter)());
  const searchParams =
  /* istanbul ignore next */
  (cov_inz4lgakk().s[31]++, (0, _navigation.useSearchParams)());
  const {
    accessToken
  } =
  /* istanbul ignore next */
  (cov_inz4lgakk().s[32]++, (0, _useAuth.useAuth)());
  const {
    canEditInvoices
  } =
  /* istanbul ignore next */
  (cov_inz4lgakk().s[33]++, (0, _usePermission.usePermission)());
  const [invoices, setInvoices] =
  /* istanbul ignore next */
  (cov_inz4lgakk().s[34]++, (0, _react.useState)(initialInvoices));
  const [totalPages, setTotalPages] =
  /* istanbul ignore next */
  (cov_inz4lgakk().s[35]++, (0, _react.useState)(initialTotalPages));
  const [currentPage, setCurrentPage] =
  /* istanbul ignore next */
  (cov_inz4lgakk().s[36]++, (0, _react.useState)(initialCurrentPage));
  const [statistics, setStatistics] =
  /* istanbul ignore next */
  (cov_inz4lgakk().s[37]++, (0, _react.useState)(initialStatistics));
  const [loading, setLoading] =
  /* istanbul ignore next */
  (cov_inz4lgakk().s[38]++, (0, _react.useState)(false)); // For client-side actions like payment, delete
  const [error, setError] =
  /* istanbul ignore next */
  (cov_inz4lgakk().s[39]++, (0, _react.useState)(null));
  const [customers, setCustomers] =
  /* istanbul ignore next */
  (cov_inz4lgakk().s[40]++, (0, _react.useState)([]));
  const [products, setProducts] =
  /* istanbul ignore next */
  (cov_inz4lgakk().s[41]++, (0, _react.useState)([]));
  const [shopsState, setShopsState] =
  /* istanbul ignore next */
  (cov_inz4lgakk().s[42]++, (0, _react.useState)(shops.map(shop => {
    /* istanbul ignore next */
    cov_inz4lgakk().f[4]++;
    cov_inz4lgakk().s[43]++;
    return {
      ...shop,
      id: shop.id.toString()
    };
  })));
  // Modal states
  const [isCreateModalOpen, setIsCreateModalOpen] =
  /* istanbul ignore next */
  (cov_inz4lgakk().s[44]++, (0, _react.useState)(false));
  const [isEditModalOpen, setIsEditModalOpen] =
  /* istanbul ignore next */
  (cov_inz4lgakk().s[45]++, (0, _react.useState)(false));
  const [isViewModalOpen, setIsViewModalOpen] =
  /* istanbul ignore next */
  (cov_inz4lgakk().s[46]++, (0, _react.useState)(false));
  const [selectedInvoice, setSelectedInvoice] =
  /* istanbul ignore next */
  (cov_inz4lgakk().s[47]++, (0, _react.useState)(null));
  // Filters state - initialized from URL search params if present
  const [statusFilter, setStatusFilter] =
  /* istanbul ignore next */
  (cov_inz4lgakk().s[48]++, (0, _react.useState)(
  /* istanbul ignore next */
  (cov_inz4lgakk().b[3][0]++, searchParams.get("status")) ||
  /* istanbul ignore next */
  (cov_inz4lgakk().b[3][1]++, "")));
  const [paymentMethodFilter, setPaymentMethodFilter] =
  /* istanbul ignore next */
  (cov_inz4lgakk().s[49]++, (0, _react.useState)(
  /* istanbul ignore next */
  (cov_inz4lgakk().b[4][0]++, searchParams.get("paymentMethod")) ||
  /* istanbul ignore next */
  (cov_inz4lgakk().b[4][1]++, "")));
  const [shopFilter, setShopFilter] =
  /* istanbul ignore next */
  (cov_inz4lgakk().s[50]++, (0, _react.useState)(
  /* istanbul ignore next */
  (cov_inz4lgakk().b[5][0]++, searchParams.get("shopId")) ||
  /* istanbul ignore next */
  (cov_inz4lgakk().b[5][1]++, "")));
  const [searchQuery, setSearchQuery] =
  /* istanbul ignore next */
  (cov_inz4lgakk().s[51]++, (0, _react.useState)(
  /* istanbul ignore next */
  (cov_inz4lgakk().b[6][0]++, searchParams.get("search")) ||
  /* istanbul ignore next */
  (cov_inz4lgakk().b[6][1]++, "")));
  const [dateFrom, setDateFrom] =
  /* istanbul ignore next */
  (cov_inz4lgakk().s[52]++, (0, _react.useState)(searchParams.get("dateFrom") ?
  /* istanbul ignore next */
  (cov_inz4lgakk().b[7][0]++, new Date(searchParams.get("dateFrom"))) :
  /* istanbul ignore next */
  (cov_inz4lgakk().b[7][1]++, undefined)));
  const [dateTo, setDateTo] =
  /* istanbul ignore next */
  (cov_inz4lgakk().s[53]++, (0, _react.useState)(searchParams.get("dateTo") ?
  /* istanbul ignore next */
  (cov_inz4lgakk().b[8][0]++, new Date(searchParams.get("dateTo"))) :
  /* istanbul ignore next */
  (cov_inz4lgakk().b[8][1]++, undefined)));
  const [sortBy, setSortBy] =
  /* istanbul ignore next */
  (cov_inz4lgakk().s[54]++, (0, _react.useState)(
  /* istanbul ignore next */
  (cov_inz4lgakk().b[9][0]++, searchParams.get("sortBy")) ||
  /* istanbul ignore next */
  (cov_inz4lgakk().b[9][1]++, "newest")));
  const [dateSortOrder, setDateSortOrder] =
  /* istanbul ignore next */
  (cov_inz4lgakk().s[55]++, (0, _react.useState)("desc")); // desc = newest first, asc = oldest first
  const [dueStatusSortOrder, setDueStatusSortOrder] =
  /* istanbul ignore next */
  (cov_inz4lgakk().s[56]++, (0, _react.useState)("asc")); // asc = overdue first, desc = current first
  // Selection state for bulk operations
  const [selectedInvoices, setSelectedInvoices] =
  /* istanbul ignore next */
  (cov_inz4lgakk().s[57]++, (0, _react.useState)(new Set()));
  const [selectAll, setSelectAll] =
  /* istanbul ignore next */
  (cov_inz4lgakk().s[58]++, (0, _react.useState)(false));
  // Selection handlers
  const handleToggleSelection =
  /* istanbul ignore next */
  (cov_inz4lgakk().s[59]++, (0, _react.useCallback)(invoiceId => {
    /* istanbul ignore next */
    cov_inz4lgakk().f[5]++;
    cov_inz4lgakk().s[60]++;
    setSelectedInvoices(prev => {
      /* istanbul ignore next */
      cov_inz4lgakk().f[6]++;
      const newSet =
      /* istanbul ignore next */
      (cov_inz4lgakk().s[61]++, new Set(prev));
      /* istanbul ignore next */
      cov_inz4lgakk().s[62]++;
      if (newSet.has(invoiceId)) {
        /* istanbul ignore next */
        cov_inz4lgakk().b[10][0]++;
        cov_inz4lgakk().s[63]++;
        newSet.delete(invoiceId);
      } else {
        /* istanbul ignore next */
        cov_inz4lgakk().b[10][1]++;
        cov_inz4lgakk().s[64]++;
        newSet.add(invoiceId);
      }
      /* istanbul ignore next */
      cov_inz4lgakk().s[65]++;
      return newSet;
    });
  }, []));
  const handleSelectAll =
  /* istanbul ignore next */
  (cov_inz4lgakk().s[66]++, (0, _react.useCallback)(() => {
    /* istanbul ignore next */
    cov_inz4lgakk().f[7]++;
    cov_inz4lgakk().s[67]++;
    if (selectAll) {
      /* istanbul ignore next */
      cov_inz4lgakk().b[11][0]++;
      cov_inz4lgakk().s[68]++;
      setSelectedInvoices(new Set());
      /* istanbul ignore next */
      cov_inz4lgakk().s[69]++;
      setSelectAll(false);
    } else {
      /* istanbul ignore next */
      cov_inz4lgakk().b[11][1]++;
      const allIds =
      /* istanbul ignore next */
      (cov_inz4lgakk().s[70]++, new Set(invoices.map(invoice => {
        /* istanbul ignore next */
        cov_inz4lgakk().f[8]++;
        cov_inz4lgakk().s[71]++;
        return String(invoice.id);
      })));
      /* istanbul ignore next */
      cov_inz4lgakk().s[72]++;
      setSelectedInvoices(allIds);
      /* istanbul ignore next */
      cov_inz4lgakk().s[73]++;
      setSelectAll(true);
    }
  }, [selectAll, invoices]));
  const handleClearSelection =
  /* istanbul ignore next */
  (cov_inz4lgakk().s[74]++, (0, _react.useCallback)(() => {
    /* istanbul ignore next */
    cov_inz4lgakk().f[9]++;
    cov_inz4lgakk().s[75]++;
    setSelectedInvoices(new Set());
    /* istanbul ignore next */
    cov_inz4lgakk().s[76]++;
    setSelectAll(false);
  }, []));
  // Handle date column sorting
  const handleDateSort =
  /* istanbul ignore next */
  (cov_inz4lgakk().s[77]++, (0, _react.useCallback)(() => {
    /* istanbul ignore next */
    cov_inz4lgakk().f[10]++;
    const newOrder =
    /* istanbul ignore next */
    (cov_inz4lgakk().s[78]++, dateSortOrder === "desc" ?
    /* istanbul ignore next */
    (cov_inz4lgakk().b[12][0]++, "asc") :
    /* istanbul ignore next */
    (cov_inz4lgakk().b[12][1]++, "desc"));
    /* istanbul ignore next */
    cov_inz4lgakk().s[79]++;
    setDateSortOrder(newOrder);
    /* istanbul ignore next */
    cov_inz4lgakk().s[80]++;
    setSortBy(newOrder === "desc" ?
    /* istanbul ignore next */
    (cov_inz4lgakk().b[13][0]++, "newest") :
    /* istanbul ignore next */
    (cov_inz4lgakk().b[13][1]++, "oldest"));
  }, [dateSortOrder]));
  // Handle due days column sorting
  const handleDueStatusSort =
  /* istanbul ignore next */
  (cov_inz4lgakk().s[81]++, (0, _react.useCallback)(() => {
    /* istanbul ignore next */
    cov_inz4lgakk().f[11]++;
    const newOrder =
    /* istanbul ignore next */
    (cov_inz4lgakk().s[82]++, dueStatusSortOrder === "asc" ?
    /* istanbul ignore next */
    (cov_inz4lgakk().b[14][0]++, "desc") :
    /* istanbul ignore next */
    (cov_inz4lgakk().b[14][1]++, "asc"));
    /* istanbul ignore next */
    cov_inz4lgakk().s[83]++;
    setDueStatusSortOrder(newOrder);
    /* istanbul ignore next */
    cov_inz4lgakk().s[84]++;
    setSortBy(newOrder === "asc" ?
    /* istanbul ignore next */
    (cov_inz4lgakk().b[15][0]++, "due-date") :
    /* istanbul ignore next */
    (cov_inz4lgakk().b[15][1]++, "due-date-desc"));
  }, [dueStatusSortOrder]));
  /* istanbul ignore next */
  cov_inz4lgakk().s[85]++;
  (0, _react.useEffect)(() => {
    /* istanbul ignore next */
    cov_inz4lgakk().f[12]++;
    cov_inz4lgakk().s[86]++;
    setInvoices(initialInvoices);
    /* istanbul ignore next */
    cov_inz4lgakk().s[87]++;
    setTotalPages(initialTotalPages);
    /* istanbul ignore next */
    cov_inz4lgakk().s[88]++;
    setCurrentPage(initialCurrentPage);
    /* istanbul ignore next */
    cov_inz4lgakk().s[89]++;
    setStatistics(initialStatistics);
  }, [initialInvoices, initialTotalPages, initialCurrentPage, initialStatistics]);
  // Handle search query changes
  /* istanbul ignore next */
  cov_inz4lgakk().s[90]++;
  (0, _react.useEffect)(() => {
    /* istanbul ignore next */
    cov_inz4lgakk().f[13]++;
    const timeoutId =
    /* istanbul ignore next */
    (cov_inz4lgakk().s[91]++, setTimeout(() => {
      /* istanbul ignore next */
      cov_inz4lgakk().f[14]++;
      cov_inz4lgakk().s[92]++;
      handleFilterChange();
    }, 300)); // Debounce search by 300ms
    /* istanbul ignore next */
    cov_inz4lgakk().s[93]++;
    return () => {
      /* istanbul ignore next */
      cov_inz4lgakk().f[15]++;
      cov_inz4lgakk().s[94]++;
      return clearTimeout(timeoutId);
    };
  }, [searchQuery]);
  // Handle status, payment method, and shop filter changes
  /* istanbul ignore next */
  cov_inz4lgakk().s[95]++;
  (0, _react.useEffect)(() => {
    /* istanbul ignore next */
    cov_inz4lgakk().f[16]++;
    cov_inz4lgakk().s[96]++;
    handleFilterChange();
  }, [statusFilter, paymentMethodFilter, shopFilter]);
  // Handle date range filter changes
  /* istanbul ignore next */
  cov_inz4lgakk().s[97]++;
  (0, _react.useEffect)(() => {
    /* istanbul ignore next */
    cov_inz4lgakk().f[17]++;
    cov_inz4lgakk().s[98]++;
    if (
    /* istanbul ignore next */
    (cov_inz4lgakk().b[17][0]++, dateFrom) ||
    /* istanbul ignore next */
    (cov_inz4lgakk().b[17][1]++, dateTo)) {
      /* istanbul ignore next */
      cov_inz4lgakk().b[16][0]++;
      cov_inz4lgakk().s[99]++;
      handleFilterChange();
    } else
    /* istanbul ignore next */
    {
      cov_inz4lgakk().b[16][1]++;
    }
  }, [dateFrom, dateTo]);
  // Handle sort changes
  /* istanbul ignore next */
  cov_inz4lgakk().s[100]++;
  (0, _react.useEffect)(() => {
    /* istanbul ignore next */
    cov_inz4lgakk().f[18]++;
    cov_inz4lgakk().s[101]++;
    if (sortBy !== "newest") {
      /* istanbul ignore next */
      cov_inz4lgakk().b[18][0]++;
      cov_inz4lgakk().s[102]++;
      handleFilterChange();
    } else
    /* istanbul ignore next */
    {
      cov_inz4lgakk().b[18][1]++;
    }
  }, [sortBy, dateSortOrder, dueStatusSortOrder]);
  // Fetch customers and products on component mount for better performance
  /* istanbul ignore next */
  cov_inz4lgakk().s[103]++;
  (0, _react.useEffect)(() => {
    /* istanbul ignore next */
    cov_inz4lgakk().f[19]++;
    cov_inz4lgakk().s[104]++;
    const fetchCustomersAndProducts = async () => {
      /* istanbul ignore next */
      cov_inz4lgakk().f[20]++;
      cov_inz4lgakk().s[105]++;
      if (!accessToken) {
        /* istanbul ignore next */
        cov_inz4lgakk().b[19][0]++;
        cov_inz4lgakk().s[106]++;
        return;
      } else
      /* istanbul ignore next */
      {
        cov_inz4lgakk().b[19][1]++;
      }
      cov_inz4lgakk().s[107]++;
      try {
        // Fetch customers (let the fetch interceptor handle auth headers)
        const customersResponse =
        /* istanbul ignore next */
        (cov_inz4lgakk().s[108]++, await fetch("/api/customers"));
        /* istanbul ignore next */
        cov_inz4lgakk().s[109]++;
        if (customersResponse.ok) {
          /* istanbul ignore next */
          cov_inz4lgakk().b[20][0]++;
          const customersData =
          /* istanbul ignore next */
          (cov_inz4lgakk().s[110]++, await customersResponse.json());
          // Add default customerType if not present
          const customersWithType =
          /* istanbul ignore next */
          (cov_inz4lgakk().s[111]++, customersData.map(customer => {
            /* istanbul ignore next */
            cov_inz4lgakk().f[21]++;
            cov_inz4lgakk().s[112]++;
            return {
              ...customer,
              customerType:
              /* istanbul ignore next */
              (cov_inz4lgakk().b[21][0]++, customer.customerType) ||
              /* istanbul ignore next */
              (cov_inz4lgakk().b[21][1]++, "retail")
            };
          }));
          /* istanbul ignore next */
          cov_inz4lgakk().s[113]++;
          setCustomers(customersWithType);
        } else
        /* istanbul ignore next */
        {
          cov_inz4lgakk().b[20][1]++;
        }
        // Fetch products
        const productsResponse =
        /* istanbul ignore next */
        (cov_inz4lgakk().s[114]++, await fetch("/api/products"));
        /* istanbul ignore next */
        cov_inz4lgakk().s[115]++;
        if (productsResponse.ok) {
          /* istanbul ignore next */
          cov_inz4lgakk().b[22][0]++;
          const productsData =
          /* istanbul ignore next */
          (cov_inz4lgakk().s[116]++, await productsResponse.json());
          // Handle the API response structure
          /* istanbul ignore next */
          cov_inz4lgakk().s[117]++;
          if (
          /* istanbul ignore next */
          (cov_inz4lgakk().b[24][0]++, productsData.success) &&
          /* istanbul ignore next */
          (cov_inz4lgakk().b[24][1]++, productsData.data)) {
            /* istanbul ignore next */
            cov_inz4lgakk().b[23][0]++;
            cov_inz4lgakk().s[118]++;
            setProducts(productsData.data);
          } else {
            /* istanbul ignore next */
            cov_inz4lgakk().b[23][1]++;
            cov_inz4lgakk().s[119]++;
            setProducts(productsData);
          }
        } else
        /* istanbul ignore next */
        {
          cov_inz4lgakk().b[22][1]++;
        }
      } catch (err) {
        /* istanbul ignore next */
        cov_inz4lgakk().s[120]++;
        console.error("Error fetching customers and products:", err);
      }
    };
    /* istanbul ignore next */
    cov_inz4lgakk().s[121]++;
    fetchCustomersAndProducts();
  }, [accessToken]); // Run when accessToken becomes available
  /* istanbul ignore next */
  cov_inz4lgakk().s[122]++;
  const handleFilterChange = () => {
    /* istanbul ignore next */
    cov_inz4lgakk().f[22]++;
    const params =
    /* istanbul ignore next */
    (cov_inz4lgakk().s[123]++, new URLSearchParams(searchParams));
    /* istanbul ignore next */
    cov_inz4lgakk().s[124]++;
    if (searchQuery) {
      /* istanbul ignore next */
      cov_inz4lgakk().b[25][0]++;
      cov_inz4lgakk().s[125]++;
      params.set("search", searchQuery);
    } else {
      /* istanbul ignore next */
      cov_inz4lgakk().b[25][1]++;
      cov_inz4lgakk().s[126]++;
      params.delete("search");
    }
    /* istanbul ignore next */
    cov_inz4lgakk().s[127]++;
    if (statusFilter) {
      /* istanbul ignore next */
      cov_inz4lgakk().b[26][0]++;
      cov_inz4lgakk().s[128]++;
      params.set("status", statusFilter);
    } else {
      /* istanbul ignore next */
      cov_inz4lgakk().b[26][1]++;
      cov_inz4lgakk().s[129]++;
      params.delete("status");
    }
    /* istanbul ignore next */
    cov_inz4lgakk().s[130]++;
    if (paymentMethodFilter) {
      /* istanbul ignore next */
      cov_inz4lgakk().b[27][0]++;
      cov_inz4lgakk().s[131]++;
      params.set("paymentMethod", paymentMethodFilter);
    } else {
      /* istanbul ignore next */
      cov_inz4lgakk().b[27][1]++;
      cov_inz4lgakk().s[132]++;
      params.delete("paymentMethod");
    }
    /* istanbul ignore next */
    cov_inz4lgakk().s[133]++;
    if (shopFilter) {
      /* istanbul ignore next */
      cov_inz4lgakk().b[28][0]++;
      cov_inz4lgakk().s[134]++;
      params.set("shopId", shopFilter);
    } else {
      /* istanbul ignore next */
      cov_inz4lgakk().b[28][1]++;
      cov_inz4lgakk().s[135]++;
      params.delete("shopId");
    }
    /* istanbul ignore next */
    cov_inz4lgakk().s[136]++;
    if (dateFrom) {
      /* istanbul ignore next */
      cov_inz4lgakk().b[29][0]++;
      cov_inz4lgakk().s[137]++;
      params.set("dateFrom", dateFrom.toISOString().split("T")[0]);
    } else {
      /* istanbul ignore next */
      cov_inz4lgakk().b[29][1]++;
      cov_inz4lgakk().s[138]++;
      params.delete("dateFrom");
    }
    /* istanbul ignore next */
    cov_inz4lgakk().s[139]++;
    if (dateTo) {
      /* istanbul ignore next */
      cov_inz4lgakk().b[30][0]++;
      cov_inz4lgakk().s[140]++;
      params.set("dateTo", dateTo.toISOString().split("T")[0]);
    } else {
      /* istanbul ignore next */
      cov_inz4lgakk().b[30][1]++;
      cov_inz4lgakk().s[141]++;
      params.delete("dateTo");
    }
    /* istanbul ignore next */
    cov_inz4lgakk().s[142]++;
    if (
    /* istanbul ignore next */
    (cov_inz4lgakk().b[32][0]++, sortBy) &&
    /* istanbul ignore next */
    (cov_inz4lgakk().b[32][1]++, sortBy !== "newest")) {
      /* istanbul ignore next */
      cov_inz4lgakk().b[31][0]++;
      cov_inz4lgakk().s[143]++;
      params.set("sortBy", sortBy);
    } else {
      /* istanbul ignore next */
      cov_inz4lgakk().b[31][1]++;
      cov_inz4lgakk().s[144]++;
      params.delete("sortBy");
    }
    /* istanbul ignore next */
    cov_inz4lgakk().s[145]++;
    params.set("page", "1"); // Reset to page 1 on new filter
    /* istanbul ignore next */
    cov_inz4lgakk().s[146]++;
    router.push(`/invoices?${params.toString()}`);
  };
  /* istanbul ignore next */
  cov_inz4lgakk().s[147]++;
  const clearFilters = () => {
    /* istanbul ignore next */
    cov_inz4lgakk().f[23]++;
    cov_inz4lgakk().s[148]++;
    setSearchQuery("");
    /* istanbul ignore next */
    cov_inz4lgakk().s[149]++;
    setStatusFilter("");
    /* istanbul ignore next */
    cov_inz4lgakk().s[150]++;
    setPaymentMethodFilter("");
    /* istanbul ignore next */
    cov_inz4lgakk().s[151]++;
    setShopFilter("");
    /* istanbul ignore next */
    cov_inz4lgakk().s[152]++;
    setDateFrom(undefined);
    /* istanbul ignore next */
