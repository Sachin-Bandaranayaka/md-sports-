                    /* istanbul ignore next */
                    cov_inz4lgakk().b[100][0]++;
                    cov_inz4lgakk().s[317]++;
                    return /*#__PURE__*/(0, _jsxruntime.jsx)("span", {
                      className: "relative inline-flex items-center px-4 py-2 text-sm font-semibold text-gray-700 ring-1 ring-inset ring-gray-300 focus:outline-offset-0",
                      children: "..."
                    }, page);
                  } else
                  /* istanbul ignore next */
                  {
                    cov_inz4lgakk().b[100][1]++;
                  }
                  cov_inz4lgakk().s[318]++;
                  return /*#__PURE__*/(0, _jsxruntime.jsx)(_Button.Button, {
                    variant: currentPage === page ?
                    /* istanbul ignore next */
                    (cov_inz4lgakk().b[101][0]++, "primary") :
                    /* istanbul ignore next */
                    (cov_inz4lgakk().b[101][1]++, "outline"),
                    size: "sm",
                    onClick: () => {
                      /* istanbul ignore next */
                      cov_inz4lgakk().f[59]++;
                      cov_inz4lgakk().s[319]++;
                      return handlePageChange(page);
                    },
                    disabled: loading,
                    className: `relative inline-flex items-center px-4 py-2 text-sm font-semibold ${currentPage === page ?
                    /* istanbul ignore next */
                    (cov_inz4lgakk().b[102][0]++, "z-10 bg-indigo-600 text-white focus-visible:outline focus-visible:outline-2 focus-visible:outline-offset-2 focus-visible:outline-indigo-600") :
                    /* istanbul ignore next */
                    (cov_inz4lgakk().b[102][1]++, "text-gray-900 ring-1 ring-inset ring-gray-300 hover:bg-gray-50 focus:z-20 focus:outline-offset-0")}`,
                    children: page
                  }, page);
                });
              })(), /*#__PURE__*/(0, _jsxruntime.jsxs)(_Button.Button, {
                variant: "outline",
                size: "sm",
                onClick: () => {
                  /* istanbul ignore next */
                  cov_inz4lgakk().f[60]++;
                  cov_inz4lgakk().s[320]++;
                  return handlePageChange(currentPage + 1);
                },
                disabled:
                /* istanbul ignore next */
                (cov_inz4lgakk().b[103][0]++, currentPage === totalPages) ||
                /* istanbul ignore next */
                (cov_inz4lgakk().b[103][1]++, loading),
                className: "relative inline-flex items-center rounded-r-md px-2 py-2 text-gray-400 ring-1 ring-inset ring-gray-300 hover:bg-gray-50 focus:z-20 focus:outline-offset-0",
                children: [/*#__PURE__*/(0, _jsxruntime.jsx)("span", {
                  className: "sr-only",
                  children: "Next"
                }), /*#__PURE__*/(0, _jsxruntime.jsx)("svg", {
                  className: "h-5 w-5",
                  viewBox: "0 0 20 20",
                  fill: "currentColor",
                  "aria-hidden": "true",
                  children: /*#__PURE__*/(0, _jsxruntime.jsx)("path", {
                    fillRule: "evenodd",
                    d: "M7.21 14.77a.75.75 0 01.02-1.06L11.168 10 7.23 6.29a.75.75 0 111.04-1.08l4.5 4.25a.75.75 0 010 1.08l-4.5 4.25a.75.75 0 01-1.06-.02z",
                    clipRule: "evenodd"
                  })
                })]
              })]
            })
          })]
        })]
      }),
      /* istanbul ignore next */
      (cov_inz4lgakk().b[104][0]++, totalPages > 10) &&
      /* istanbul ignore next */
      (cov_inz4lgakk().b[104][1]++, /*#__PURE__*/(0, _jsxruntime.jsxs)("div", {
        className: "mt-3 flex items-center justify-center space-x-2",
        children: [/*#__PURE__*/(0, _jsxruntime.jsx)("span", {
          className: "text-sm text-gray-700",
          children: "Jump to page:"
        }), /*#__PURE__*/(0, _jsxruntime.jsx)("input", {
          type: "number",
          min: "1",
          max: totalPages,
          className: "w-16 px-2 py-1 text-sm border border-gray-300 rounded focus:ring-indigo-500 focus:border-indigo-500",
          onKeyPress: e => {
            /* istanbul ignore next */
            cov_inz4lgakk().f[61]++;
            cov_inz4lgakk().s[321]++;
            if (e.key === "Enter") {
              /* istanbul ignore next */
              cov_inz4lgakk().b[105][0]++;
              const page =
              /* istanbul ignore next */
              (cov_inz4lgakk().s[322]++, parseInt(e.target.value));
              /* istanbul ignore next */
              cov_inz4lgakk().s[323]++;
              if (
              /* istanbul ignore next */
              (cov_inz4lgakk().b[107][0]++, page >= 1) &&
              /* istanbul ignore next */
              (cov_inz4lgakk().b[107][1]++, page <= totalPages)) {
                /* istanbul ignore next */
                cov_inz4lgakk().b[106][0]++;
                cov_inz4lgakk().s[324]++;
                handlePageChange(page);
                /* istanbul ignore next */
                cov_inz4lgakk().s[325]++;
                e.target.value = "";
              } else
              /* istanbul ignore next */
              {
                cov_inz4lgakk().b[106][1]++;
              }
            } else
            /* istanbul ignore next */
            {
              cov_inz4lgakk().b[105][1]++;
            }
          },
          placeholder: currentPage.toString()
        }), /*#__PURE__*/(0, _jsxruntime.jsxs)("span", {
          className: "text-sm text-gray-500",
          children: ["of ", totalPages]
        })]
      }))]
    })), /*#__PURE__*/(0, _jsxruntime.jsx)(_invoices.InvoiceCreateModal, {
      isOpen: isCreateModalOpen,
      onClose: handleCloseModals,
      onSave: handleCreateSuccess,
      customers: customers,
      products: products,
      shops: shopsState,
      isLoading: loading
    }), /*#__PURE__*/(0, _jsxruntime.jsx)(_invoices.InvoiceEditModal, {
      isOpen: isEditModalOpen,
      onClose: handleCloseModals,
      onSave: handleEditSuccess,
      customers: customers,
      products: products,
      initialData: selectedInvoice,
      isLoading: loading
    }), /*#__PURE__*/(0, _jsxruntime.jsx)(_invoices.InvoiceViewModal, {
      isOpen: isViewModalOpen,
      onClose: handleCloseModals,
      onEdit: () => {
        /* istanbul ignore next */
