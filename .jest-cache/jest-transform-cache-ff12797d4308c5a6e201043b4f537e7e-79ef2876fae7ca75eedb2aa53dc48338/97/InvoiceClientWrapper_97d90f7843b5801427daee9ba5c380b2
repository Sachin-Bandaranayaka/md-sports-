                      pages.push(1);
                      /* istanbul ignore next */
                      cov_inz4lgakk().s[309]++;
                      pages.push("ellipsis1");
                      /* istanbul ignore next */
                      cov_inz4lgakk().s[310]++;
                      for (let i =
                      /* istanbul ignore next */
                      (cov_inz4lgakk().s[311]++, currentPage - 1); i <= currentPage + 1; i++) {
                        /* istanbul ignore next */
                        cov_inz4lgakk().s[312]++;
                        pages.push(i);
                      }
                      /* istanbul ignore next */
                      cov_inz4lgakk().s[313]++;
                      pages.push("ellipsis2");
                      /* istanbul ignore next */
                      cov_inz4lgakk().s[314]++;
                      pages.push(totalPages);
                    }
                  }
                }
                /* istanbul ignore next */
                cov_inz4lgakk().s[315]++;
                return pages.map((page, index) => {
                  /* istanbul ignore next */
                  cov_inz4lgakk().f[58]++;
