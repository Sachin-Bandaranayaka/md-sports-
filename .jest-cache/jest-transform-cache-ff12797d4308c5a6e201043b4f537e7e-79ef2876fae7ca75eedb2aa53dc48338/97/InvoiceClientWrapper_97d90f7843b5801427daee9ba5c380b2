                  } else {
                    /* istanbul ignore next */
                    cov_inz4lgakk().b[96][1]++;
                    cov_inz4lgakk().s[301]++;
                    if (currentPage >= totalPages - 3) {
                      /* istanbul ignore next */
                      cov_inz4lgakk().b[98][0]++;
                      cov_inz4lgakk().s[302]++;
                      // Show first page + ellipsis + last 5 pages
                      pages.push(1);
                      /* istanbul ignore next */
                      cov_inz4lgakk().s[303]++;
