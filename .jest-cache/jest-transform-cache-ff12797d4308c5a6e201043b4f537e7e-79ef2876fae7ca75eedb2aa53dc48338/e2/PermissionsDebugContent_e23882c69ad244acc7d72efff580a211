        /* istanbul ignore next */
        (cov_11qerv0qle().b[12][0]++, `User HAS the permission: ${permissionToCheck}`) :
        /* istanbul ignore next */
        (cov_11qerv0qle().b[12][1]++, `User DOES NOT HAVE the permission: ${permissionToCheck}`)
