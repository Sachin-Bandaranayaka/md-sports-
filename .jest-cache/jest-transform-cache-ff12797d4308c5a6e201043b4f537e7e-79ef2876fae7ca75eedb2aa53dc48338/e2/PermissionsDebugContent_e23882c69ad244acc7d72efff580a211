            children: perm
          }, perm);
        })
      }))]
    }), /*#__PURE__*/(0, _jsxruntime.jsxs)("div", {
      className: "mb-6",
      children: [/*#__PURE__*/(0, _jsxruntime.jsx)("h2", {
        className: "text-xl font-semibold mb-2",
        children: "Check Specific Permission"
      }), /*#__PURE__*/(0, _jsxruntime.jsxs)("div", {
        className: "flex mb-2",
        children: [/*#__PURE__*/(0, _jsxruntime.jsx)("input", {
          type: "text",
          value: permissionToCheck,
          onChange: e => {
            /* istanbul ignore next */
            cov_11qerv0qle().f[8]++;
            cov_11qerv0qle().s[42]++;
            return setPermissionToCheck(e.target.value);
          },
          placeholder: "Enter permission (e.g., category:delete)",
          className: "p-2 border rounded mr-2 flex-grow"
        }), /*#__PURE__*/(0, _jsxruntime.jsx)("button", {
          onClick: checkPermission,
          disabled: !permissionToCheck,
          className: "bg-blue-500 hover:bg-blue-600 text-white py-2 px-4 rounded disabled:bg-blue-300",
          children: "Check"
        })]
      }),
      /* istanbul ignore next */
      (cov_11qerv0qle().b[10][0]++, hasPermissionResult !== null) &&
      /* istanbul ignore next */
      (cov_11qerv0qle().b[10][1]++, /*#__PURE__*/(0, _jsxruntime.jsx)("div", {
