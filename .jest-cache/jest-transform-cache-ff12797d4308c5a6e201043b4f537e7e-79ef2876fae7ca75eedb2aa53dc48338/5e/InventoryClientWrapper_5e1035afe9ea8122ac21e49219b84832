  /* istanbul ignore next */
  (cov_hnpup662y().s[24]++, _getRequireWildcardCache(nodeInterop));
  /* istanbul ignore next */
  cov_hnpup662y().s[25]++;
  if (
  /* istanbul ignore next */
  (cov_hnpup662y().b[9][0]++, cache) &&
  /* istanbul ignore next */
  (cov_hnpup662y().b[9][1]++, cache.has(obj))) {
    /* istanbul ignore next */
    cov_hnpup662y().b[8][0]++;
    cov_hnpup662y().s[26]++;
