function _interop_require_wildcard(obj, nodeInterop) {
  /* istanbul ignore next */
  cov_hnpup662y().f[4]++;
  cov_hnpup662y().s[20]++;
  if (
  /* istanbul ignore next */
  (cov_hnpup662y().b[5][0]++, !nodeInterop) &&
  /* istanbul ignore next */
