    /* istanbul ignore next */
    {
      cov_hnpup662y().b[27][1]++;
    }
    const operationId =
    /* istanbul ignore next */
    (cov_hnpup662y().s[127]++, `delete-product-${productId}`);
    /* istanbul ignore next */
    cov_hnpup662y().s[128]++;
    setDeleteLoading(productId);
    /* istanbul ignore next */
    cov_hnpup662y().s[129]++;
    setPendingOperations(prev => {
      /* istanbul ignore next */
      cov_hnpup662y().f[24]++;
      cov_hnpup662y().s[130]++;
      return new Set(prev).add(operationId);
    });
    // Temporarily disable auto-refresh during deletion
    const wasAutoRefreshEnabled =
    /* istanbul ignore next */
    (cov_hnpup662y().s[131]++, autoRefreshEnabled);
    /* istanbul ignore next */
    cov_hnpup662y().s[132]++;
    setAutoRefreshEnabled(false);
    /* istanbul ignore next */
    cov_hnpup662y().s[133]++;
    try {
      // Optimistic update - immediately remove from UI
      const originalItems =
      /* istanbul ignore next */
      (cov_hnpup662y().s[134]++, localInventoryItems);
      /* istanbul ignore next */
      cov_hnpup662y().s[135]++;
      setInventoryItems(prevItems => {
        /* istanbul ignore next */
        cov_hnpup662y().f[25]++;
        cov_hnpup662y().s[136]++;
        return prevItems.filter(item => {
          /* istanbul ignore next */
          cov_hnpup662y().f[26]++;
          cov_hnpup662y().s[137]++;
          return item.id !== productId;
        });
      });
      const response =
      /* istanbul ignore next */
      (cov_hnpup662y().s[138]++, await (0, _api.authDelete)(`/api/products/${productId}`));
      const data =
      /* istanbul ignore next */
      (cov_hnpup662y().s[139]++, await response.json());
      /* istanbul ignore next */
      cov_hnpup662y().s[140]++;
      if (
      /* istanbul ignore next */
      (cov_hnpup662y().b[29][0]++, response.ok) &&
      /* istanbul ignore next */
      (cov_hnpup662y().b[29][1]++, data.success)) {
        /* istanbul ignore next */
        cov_hnpup662y().b[28][0]++;
        cov_hnpup662y().s[141]++;
        // Success - keep the item removed
        console.log(`Product ${productId} deleted successfully`);
        // Wait a bit before re-enabling auto-refresh to ensure DB consistency
        /* istanbul ignore next */
        cov_hnpup662y().s[142]++;
        setTimeout(() => {
          /* istanbul ignore next */
          cov_hnpup662y().f[27]++;
          cov_hnpup662y().s[143]++;
          setAutoRefreshEnabled(wasAutoRefreshEnabled);
        }, 2000);
      } else {
        /* istanbul ignore next */
        cov_hnpup662y().b[28][1]++;
        cov_hnpup662y().s[144]++;
        // Rollback optimistic update on failure
        setInventoryItems(originalItems);
        // Enhanced error message for related records
        let errorMessage =
        /* istanbul ignore next */
        (cov_hnpup662y().s[145]++,
        /* istanbul ignore next */
        (cov_hnpup662y().b[30][0]++, data.message) ||
        /* istanbul ignore next */
        (cov_hnpup662y().b[30][1]++, "Failed to delete product"));
        /* istanbul ignore next */
        cov_hnpup662y().s[146]++;
        if (data.relatedRecords) {
          /* istanbul ignore next */
          cov_hnpup662y().b[31][0]++;
          const {
            purchaseInvoiceItems,
            salesInvoiceItems,
            quotationItems
          } =
          /* istanbul ignore next */
          (cov_hnpup662y().s[147]++, data.relatedRecords);
          const details =
          /* istanbul ignore next */
          (cov_hnpup662y().s[148]++, []);
          /* istanbul ignore next */
          cov_hnpup662y().s[149]++;
          if (purchaseInvoiceItems > 0) {
            /* istanbul ignore next */
            cov_hnpup662y().b[32][0]++;
            cov_hnpup662y().s[150]++;
            details.push(`${purchaseInvoiceItems} purchase invoice(s)`);
          } else
          /* istanbul ignore next */
          {
            cov_hnpup662y().b[32][1]++;
          }
          cov_hnpup662y().s[151]++;
          if (salesInvoiceItems > 0) {
            /* istanbul ignore next */
            cov_hnpup662y().b[33][0]++;
            cov_hnpup662y().s[152]++;
            details.push(`${salesInvoiceItems} sales invoice(s)`);
          } else
          /* istanbul ignore next */
          {
            cov_hnpup662y().b[33][1]++;
          }
          cov_hnpup662y().s[153]++;
          if (quotationItems > 0) {
            /* istanbul ignore next */
            cov_hnpup662y().b[34][0]++;
            cov_hnpup662y().s[154]++;
            details.push(`${quotationItems} quotation(s)`);
          } else
          /* istanbul ignore next */
          {
            cov_hnpup662y().b[34][1]++;
          }
          cov_hnpup662y().s[155]++;
          if (details.length > 0) {
            /* istanbul ignore next */
            cov_hnpup662y().b[35][0]++;
            cov_hnpup662y().s[156]++;
            errorMessage += `\n\nThis product is currently used in:\n• ${details.join("\n• ")}`;
            /* istanbul ignore next */
            cov_hnpup662y().s[157]++;
            errorMessage += "\n\nPlease remove these references before deleting the product.";
          } else
          /* istanbul ignore next */
          {
            cov_hnpup662y().b[35][1]++;
          }
        } else
        /* istanbul ignore next */
        {
          cov_hnpup662y().b[31][1]++;
        }
        cov_hnpup662y().s[158]++;
        setLocalError(errorMessage);
        /* istanbul ignore next */
        cov_hnpup662y().s[159]++;
        setAutoRefreshEnabled(wasAutoRefreshEnabled); // Re-enable immediately on error
      }
    } catch (err) {
      /* istanbul ignore next */
      cov_hnpup662y().s[160]++;
      console.error("Error deleting product:", err);
      // Rollback optimistic update on error
      /* istanbul ignore next */
      cov_hnpup662y().s[161]++;
      setInventoryItems(inventoryItems);
      /* istanbul ignore next */
      cov_hnpup662y().s[162]++;
      setLocalError("Failed to delete product. Please try again later.");
      /* istanbul ignore next */
      cov_hnpup662y().s[163]++;
      setAutoRefreshEnabled(wasAutoRefreshEnabled); // Re-enable immediately on error
    } finally {
      /* istanbul ignore next */
      cov_hnpup662y().s[164]++;
      setDeleteLoading(null);
      /* istanbul ignore next */
      cov_hnpup662y().s[165]++;
      setPendingOperations(prev => {
        /* istanbul ignore next */
        cov_hnpup662y().f[28]++;
        const newSet =
        /* istanbul ignore next */
        (cov_hnpup662y().s[166]++, new Set(prev));
        /* istanbul ignore next */
        cov_hnpup662y().s[167]++;
        newSet.delete(operationId);
        /* istanbul ignore next */
        cov_hnpup662y().s[168]++;
        return newSet;
      });
    }
  };
  // Selection handlers
  const handleToggleSelection =
  /* istanbul ignore next */
  (cov_hnpup662y().s[169]++, (0, _react.useCallback)(itemId => {
    /* istanbul ignore next */
    cov_hnpup662y().f[29]++;
    cov_hnpup662y().s[170]++;
    setSelectedItems(prev => {
      /* istanbul ignore next */
      cov_hnpup662y().f[30]++;
      const newSet =
      /* istanbul ignore next */
      (cov_hnpup662y().s[171]++, new Set(prev));
      /* istanbul ignore next */
      cov_hnpup662y().s[172]++;
      if (newSet.has(itemId)) {
        /* istanbul ignore next */
        cov_hnpup662y().b[36][0]++;
        cov_hnpup662y().s[173]++;
        newSet.delete(itemId);
      } else {
        /* istanbul ignore next */
        cov_hnpup662y().b[36][1]++;
        cov_hnpup662y().s[174]++;
        newSet.add(itemId);
      }
      /* istanbul ignore next */
      cov_hnpup662y().s[175]++;
      return newSet;
    });
  }, []));
  const handleSelectAll =
  /* istanbul ignore next */
  (cov_hnpup662y().s[176]++, (0, _react.useCallback)(() => {
    /* istanbul ignore next */
    cov_hnpup662y().f[31]++;
    const currentItems =
    /* istanbul ignore next */
    (cov_hnpup662y().s[177]++,
    /* istanbul ignore next */
    (cov_hnpup662y().b[37][0]++, inventoryData?.data) ||
    /* istanbul ignore next */
    (cov_hnpup662y().b[37][1]++, localInventoryItems));
    /* istanbul ignore next */
    cov_hnpup662y().s[178]++;
    if (selectAll) {
      /* istanbul ignore next */
      cov_hnpup662y().b[38][0]++;
      cov_hnpup662y().s[179]++;
      setSelectedItems(new Set());
    } else {
      /* istanbul ignore next */
      cov_hnpup662y().b[38][1]++;
      cov_hnpup662y().s[180]++;
      setSelectedItems(new Set(currentItems.map(item => {
        /* istanbul ignore next */
        cov_hnpup662y().f[32]++;
        cov_hnpup662y().s[181]++;
        return item.id;
      })));
    }
    /* istanbul ignore next */
    cov_hnpup662y().s[182]++;
    setSelectAll(!selectAll);
  }, [selectAll, inventoryData?.data, localInventoryItems]));
  const handleClearSelection =
  /* istanbul ignore next */
  (cov_hnpup662y().s[183]++, (0, _react.useCallback)(() => {
    /* istanbul ignore next */
    cov_hnpup662y().f[33]++;
    cov_hnpup662y().s[184]++;
    setSelectedItems(new Set());
    /* istanbul ignore next */
    cov_hnpup662y().s[185]++;
    setSelectAll(false);
  }, []));
  // Bulk delete handler
  const handleBulkDelete =
  /* istanbul ignore next */
  (cov_hnpup662y().s[186]++, (0, _react.useCallback)(async () => {
    /* istanbul ignore next */
    cov_hnpup662y().f[34]++;
    cov_hnpup662y().s[187]++;
    if (selectedItems.size === 0) {
      /* istanbul ignore next */
      cov_hnpup662y().b[39][0]++;
      cov_hnpup662y().s[188]++;
      return;
    } else
    /* istanbul ignore next */
    {
      cov_hnpup662y().b[39][1]++;
    }
    const confirmMessage =
    /* istanbul ignore next */
    (cov_hnpup662y().s[189]++, `Are you sure you want to delete ${selectedItems.size} selected item(s)? This action cannot be undone.`);
    /* istanbul ignore next */
    cov_hnpup662y().s[190]++;
    if (!confirm(confirmMessage)) {
      /* istanbul ignore next */
      cov_hnpup662y().b[40][0]++;
      cov_hnpup662y().s[191]++;
      return;
    } else
    /* istanbul ignore next */
    {
      cov_hnpup662y().b[40][1]++;
    }
    cov_hnpup662y().s[192]++;
    setBulkDeleteLoading(true);
    const wasAutoRefreshEnabled =
    /* istanbul ignore next */
    (cov_hnpup662y().s[193]++, autoRefreshEnabled);
    /* istanbul ignore next */
    cov_hnpup662y().s[194]++;
    setAutoRefreshEnabled(false);
    /* istanbul ignore next */
    cov_hnpup662y().s[195]++;
    try {
      const deletePromises =
      /* istanbul ignore next */
      (cov_hnpup662y().s[196]++, Array.from(selectedItems).map(async itemId => {
        /* istanbul ignore next */
        cov_hnpup662y().f[35]++;
        const response =
        /* istanbul ignore next */
        (cov_hnpup662y().s[197]++, await (0, _api.authDelete)(`/api/products/${itemId}`));
        const data =
        /* istanbul ignore next */
        (cov_hnpup662y().s[198]++, await response.json());
        /* istanbul ignore next */
        cov_hnpup662y().s[199]++;
        return {
          itemId,
          success:
          /* istanbul ignore next */
          (cov_hnpup662y().b[41][0]++, response.ok) &&
          /* istanbul ignore next */
          (cov_hnpup662y().b[41][1]++, data.success),
          error: data.message
        };
      }));
      const results =
      /* istanbul ignore next */
      (cov_hnpup662y().s[200]++, await Promise.all(deletePromises));
      const successfulDeletes =
      /* istanbul ignore next */
      (cov_hnpup662y().s[201]++, results.filter(r => {
        /* istanbul ignore next */
        cov_hnpup662y().f[36]++;
        cov_hnpup662y().s[202]++;
        return r.success;
      }).map(r => {
        /* istanbul ignore next */
        cov_hnpup662y().f[37]++;
        cov_hnpup662y().s[203]++;
        return r.itemId;
      }));
      const failedDeletes =
      /* istanbul ignore next */
      (cov_hnpup662y().s[204]++, results.filter(r => {
        /* istanbul ignore next */
        cov_hnpup662y().f[38]++;
        cov_hnpup662y().s[205]++;
        return !r.success;
      }));
      /* istanbul ignore next */
      cov_hnpup662y().s[206]++;
      if (successfulDeletes.length > 0) {
        /* istanbul ignore next */
        cov_hnpup662y().b[42][0]++;
        cov_hnpup662y().s[207]++;
        // Remove successfully deleted items from local state
        setInventoryItems(prev => {
          /* istanbul ignore next */
          cov_hnpup662y().f[39]++;
          cov_hnpup662y().s[208]++;
          return prev.filter(item => {
            /* istanbul ignore next */
            cov_hnpup662y().f[40]++;
            cov_hnpup662y().s[209]++;
            return !successfulDeletes.includes(item.id);
          });
        });
        /* istanbul ignore next */
        cov_hnpup662y().s[210]++;
        console.log(`Successfully deleted ${successfulDeletes.length} items`);
      } else
      /* istanbul ignore next */
      {
        cov_hnpup662y().b[42][1]++;
      }
      cov_hnpup662y().s[211]++;
      if (failedDeletes.length > 0) {
        /* istanbul ignore next */
        cov_hnpup662y().b[43][0]++;
        const errorMessage =
        /* istanbul ignore next */
        (cov_hnpup662y().s[212]++, `Failed to delete ${failedDeletes.length} item(s). Some items may be referenced in invoices or other records.`);
        /* istanbul ignore next */
        cov_hnpup662y().s[213]++;
        setLocalError(errorMessage);
      } else
      /* istanbul ignore next */
      {
        cov_hnpup662y().b[43][1]++;
      }
      // Clear selection
      cov_hnpup662y().s[214]++;
      setSelectedItems(new Set());
      /* istanbul ignore next */
      cov_hnpup662y().s[215]++;
      setSelectAll(false);
      // Wait before re-enabling auto-refresh
      /* istanbul ignore next */
      cov_hnpup662y().s[216]++;
      setTimeout(() => {
        /* istanbul ignore next */
        cov_hnpup662y().f[41]++;
        cov_hnpup662y().s[217]++;
        setAutoRefreshEnabled(wasAutoRefreshEnabled);
      }, 2000);
    } catch (error) {
      /* istanbul ignore next */
      cov_hnpup662y().s[218]++;
      console.error("Error during bulk delete:", error);
      /* istanbul ignore next */
      cov_hnpup662y().s[219]++;
      setLocalError("Failed to delete selected items. Please try again later.");
      /* istanbul ignore next */
      cov_hnpup662y().s[220]++;
      setAutoRefreshEnabled(wasAutoRefreshEnabled);
    } finally {
      /* istanbul ignore next */
      cov_hnpup662y().s[221]++;
      setBulkDeleteLoading(false);
    }
  }, [selectedItems, autoRefreshEnabled, _api.authDelete]));
  // Update selectAll state when items change
  /* istanbul ignore next */
  cov_hnpup662y().s[222]++;
  (0, _react.useEffect)(() => {
    /* istanbul ignore next */
    cov_hnpup662y().f[42]++;
    const currentItems =
    /* istanbul ignore next */
    (cov_hnpup662y().s[223]++,
    /* istanbul ignore next */
    (cov_hnpup662y().b[44][0]++, inventoryData?.data) ||
    /* istanbul ignore next */
    (cov_hnpup662y().b[44][1]++, localInventoryItems));
    /* istanbul ignore next */
    cov_hnpup662y().s[224]++;
    if (currentItems.length === 0) {
      /* istanbul ignore next */
      cov_hnpup662y().b[45][0]++;
      cov_hnpup662y().s[225]++;
      setSelectAll(false);
    } else {
      /* istanbul ignore next */
      cov_hnpup662y().b[45][1]++;
      const allSelected =
      /* istanbul ignore next */
      (cov_hnpup662y().s[226]++, currentItems.every(item => {
        /* istanbul ignore next */
        cov_hnpup662y().f[43]++;
        cov_hnpup662y().s[227]++;
        return selectedItems.has(item.id);
      }));
      /* istanbul ignore next */
      cov_hnpup662y().s[228]++;
      setSelectAll(
      /* istanbul ignore next */
      (cov_hnpup662y().b[46][0]++, allSelected) &&
      /* istanbul ignore next */
      (cov_hnpup662y().b[46][1]++, selectedItems.size > 0));
    }
  }, [selectedItems, inventoryData?.data, localInventoryItems]);
  // Clear selection when filters change
  /* istanbul ignore next */
  cov_hnpup662y().s[229]++;
  (0, _react.useEffect)(() => {
    /* istanbul ignore next */
    cov_hnpup662y().f[44]++;
    cov_hnpup662y().s[230]++;
    setSelectedItems(new Set());
    /* istanbul ignore next */
    cov_hnpup662y().s[231]++;
    setSelectAll(false);
  }, [searchTerm, categoryFilter, statusFilter, currentPage]);
  // Handle inventory added
  /* istanbul ignore next */
  cov_hnpup662y().s[232]++;
  const handleInventoryAdded = () => {
    /* istanbul ignore next */
    cov_hnpup662y().f[45]++;
    cov_hnpup662y().s[233]++;
    console.log("Inventory added, refreshing data");
    /* istanbul ignore next */
    cov_hnpup662y().s[234]++;
    refetch();
    /* istanbul ignore next */
    cov_hnpup662y().s[235]++;
    setLastRefreshed(new Date());
  };
  // Debounced search function
  const debouncedSearch =
  /* istanbul ignore next */
  (cov_hnpup662y().s[236]++, (0, _react.useCallback)((0, _utils.debounce)(searchValue => {
    /* istanbul ignore next */
    cov_hnpup662y().f[46]++;
    cov_hnpup662y().s[237]++;
    setCurrentPage(1);
    // Trigger refresh with new search term
    const params =
    /* istanbul ignore next */
    (cov_hnpup662y().s[238]++, new URLSearchParams(searchParams));
    /* istanbul ignore next */
    cov_hnpup662y().s[239]++;
    params.set("page", "1");
    /* istanbul ignore next */
    cov_hnpup662y().s[240]++;
    params.set("limit", itemsPerPage.toString());
    /* istanbul ignore next */
    cov_hnpup662y().s[241]++;
    if (searchValue) {
      /* istanbul ignore next */
      cov_hnpup662y().b[47][0]++;
      cov_hnpup662y().s[242]++;
      params.set("search", searchValue);
    } else {
      /* istanbul ignore next */
      cov_hnpup662y().b[47][1]++;
      cov_hnpup662y().s[243]++;
      params.delete("search");
    }
    /* istanbul ignore next */
    cov_hnpup662y().s[244]++;
    if (categoryFilter) {
      /* istanbul ignore next */
      cov_hnpup662y().b[48][0]++;
      cov_hnpup662y().s[245]++;
      params.set("category", categoryFilter);
    } else
    /* istanbul ignore next */
    {
      cov_hnpup662y().b[48][1]++;
    }
    cov_hnpup662y().s[246]++;
    if (statusFilter) {
      /* istanbul ignore next */
      cov_hnpup662y().b[49][0]++;
      cov_hnpup662y().s[247]++;
      params.set("status", statusFilter);
    } else
    /* istanbul ignore next */
    {
      cov_hnpup662y().b[49][1]++;
    }
    cov_hnpup662y().s[248]++;
    router.push(`${pathname}?${params.toString()}`);
  }, 300), [categoryFilter, statusFilter, itemsPerPage, pathname, router, searchParams]));
  // Optimized search handler
  const handleSearchChange =
  /* istanbul ignore next */
  (cov_hnpup662y().s[249]++, (0, _react.useCallback)(value => {
    /* istanbul ignore next */
    cov_hnpup662y().f[47]++;
    cov_hnpup662y().s[250]++;
    setSearchTerm(value);
    /* istanbul ignore next */
    cov_hnpup662y().s[251]++;
    debouncedSearch(value);
  }, [debouncedSearch]));
  // Function to manually refresh inventory data with smart refresh logic
  const refreshInventory =
  /* istanbul ignore next */
  (cov_hnpup662y().s[252]++, (0, _react.useCallback)(async (force =
  /* istanbul ignore next */
  (cov_hnpup662y().b[50][0]++, false)) => {
    /* istanbul ignore next */
    cov_hnpup662y().f[48]++;
    cov_hnpup662y().s[253]++;
    // Smart refresh logic - don't refresh if there are pending operations (unless forced)
    if (
    /* istanbul ignore next */
    (cov_hnpup662y().b[52][0]++, !force) &&
    /* istanbul ignore next */
    (cov_hnpup662y().b[52][1]++, pendingOperations.size > 0)) {
      /* istanbul ignore next */
      cov_hnpup662y().b[51][0]++;
      cov_hnpup662y().s[254]++;
      console.log("Skipping refresh due to pending operations:", Array.from(pendingOperations));
      /* istanbul ignore next */
      cov_hnpup662y().s[255]++;
      return;
    } else
    /* istanbul ignore next */
    {
      cov_hnpup662y().b[51][1]++;
    }
    cov_hnpup662y().s[256]++;
    try {
      /* istanbul ignore next */
      cov_hnpup662y().s[257]++;
      setLocalError(null);
      /* istanbul ignore next */
      cov_hnpup662y().s[258]++;
      console.log("Refreshing inventory data...", {
        page: currentPage,
        limit: itemsPerPage,
        search: searchTerm,
        category: categoryFilter,
        status: statusFilter,
        pendingOperations: Array.from(pendingOperations),
        forced: force
      });
      // Use React Query's refetch function instead of manual fetching
      /* istanbul ignore next */
      cov_hnpup662y().s[259]++;
      await refetch();
      // Update last refreshed timestamp
      /* istanbul ignore next */
      cov_hnpup662y().s[260]++;
      setLastRefreshed(new Date());
    } catch (err) {
      /* istanbul ignore next */
      cov_hnpup662y().s[261]++;
      console.error("Error refreshing inventory:", err);
      /* istanbul ignore next */
      cov_hnpup662y().s[262]++;
      setLocalError(err instanceof Error ?
      /* istanbul ignore next */
      (cov_hnpup662y().b[53][0]++, err.message) :
      /* istanbul ignore next */
      (cov_hnpup662y().b[53][1]++, "Failed to refresh inventory"));
    }
  }, [currentPage, itemsPerPage, searchTerm, categoryFilter, statusFilter, pendingOperations, refetch]));
  // Optimized auto-refresh with exponential backoff
  /* istanbul ignore next */
  cov_hnpup662y().s[263]++;
  (0, _react.useEffect)(() => {
    /* istanbul ignore next */
    cov_hnpup662y().f[49]++;
    cov_hnpup662y().s[264]++;
    if (!autoRefreshEnabled) {
      /* istanbul ignore next */
      cov_hnpup662y().b[54][0]++;
      cov_hnpup662y().s[265]++;
      return;
    } else
    /* istanbul ignore next */
    {
      cov_hnpup662y().b[54][1]++;
    }
    let retryCount =
    /* istanbul ignore next */
    (cov_hnpup662y().s[266]++, 0);
    const maxRetries =
    /* istanbul ignore next */
    (cov_hnpup662y().s[267]++, 3);
    const baseInterval =
    /* istanbul ignore next */
    (cov_hnpup662y().s[268]++, autoRefreshInterval * 1000);
    /* istanbul ignore next */
    cov_hnpup662y().s[269]++;
