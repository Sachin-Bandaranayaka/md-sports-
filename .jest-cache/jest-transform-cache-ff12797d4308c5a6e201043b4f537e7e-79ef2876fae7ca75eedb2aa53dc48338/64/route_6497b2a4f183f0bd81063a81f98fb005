    cov_8ko9wrib9().s[5]++;
    return PATCH;
  },
  dynamic: function () {
    /* istanbul ignore next */
    cov_8ko9wrib9().f[3]++;
    cov_8ko9wrib9().s[6]++;
    return dynamic;
  },
  maxDuration: function () {
    /* istanbul ignore next */
    cov_8ko9wrib9().f[4]++;
    cov_8ko9wrib9().s[7]++;
    return maxDuration;
  },
  runtime: function () {
    /* istanbul ignore next */
    cov_8ko9wrib9().f[5]++;
    cov_8ko9wrib9().s[8]++;
    return runtime;
  }
});
const _server =
