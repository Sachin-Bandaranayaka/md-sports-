    /* istanbul ignore next */
    cov_23w7eztupa().b[8][0]++;
    cov_23w7eztupa().s[26]++;
    return cache.get(obj);
  } else
  /* istanbul ignore next */
  {
    cov_23w7eztupa().b[8][1]++;
  }
  var newObj =
  /* istanbul ignore next */
  (cov_23w7eztupa().s[27]++, {
    __proto__: null
  });
  var hasPropertyDescriptor =
  /* istanbul ignore next */
  (cov_23w7eztupa().s[28]++,
