  {
    cov_23w7eztupa().b[6][1]++;
  }
  var cache =
  /* istanbul ignore next */
  (cov_23w7eztupa().s[24]++, _getRequireWildcardCache(nodeInterop));
  /* istanbul ignore next */
  cov_23w7eztupa().s[25]++;
