            const response = await fetch("/api/audit-trail/recover", {
                method: "POST",
                headers: {
                    "Content-Type": "application/json",
                    Authorization: `Bearer ${accessToken}`,
                    ...csrfToken && {
                        "X-CSRF-Token": csrfToken
                    }
                },
                body: JSON.stringify({
                    auditLogId
                })
