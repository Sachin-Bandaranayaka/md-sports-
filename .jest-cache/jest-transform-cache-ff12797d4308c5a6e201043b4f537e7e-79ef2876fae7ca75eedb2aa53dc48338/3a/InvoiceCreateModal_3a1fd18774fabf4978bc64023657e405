  /* istanbul ignore next */
  (cov_1zlzl48xi1().s[27]++, {
    __proto__: null
  });
  var hasPropertyDescriptor =
  /* istanbul ignore next */
  (cov_1zlzl48xi1().s[28]++,
  /* istanbul ignore next */
  (cov_1zlzl48xi1().b[10][0]++, Object.defineProperty) &&
  /* istanbul ignore next */
