            onClick: () => {
              /* istanbul ignore next */
              cov_1zlzl48xi1().f[124]++;
              cov_1zlzl48xi1().s[446]++;
              return setShowQuickCustomerModal(false);
            },
            disabled: isCreatingCustomer,
            children: "Cancel"
          }), /*#__PURE__*/(0, _jsxruntime.jsx)(_Button.Button, {
            onClick: handleQuickCustomerCreate,
            isLoading: isCreatingCustomer,
            disabled:
            /* istanbul ignore next */
            (cov_1zlzl48xi1().b[168][0]++, isCreatingCustomer) ||
            /* istanbul ignore next */
            (cov_1zlzl48xi1().b[168][1]++, !quickCustomerData.name.trim()),
            className: "bg-blue-500 hover:bg-blue-600 text-white",
            children: isCreatingCustomer ?
            /* istanbul ignore next */
