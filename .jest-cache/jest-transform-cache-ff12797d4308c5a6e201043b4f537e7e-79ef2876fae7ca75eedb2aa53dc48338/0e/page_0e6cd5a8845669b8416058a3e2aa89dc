          /* istanbul ignore next */
          cov_y2tbfk2n7().s[132]++;
          setProductStock(shopStock);
          /* istanbul ignore next */
          cov_y2tbfk2n7().s[133]++;
          setCustomPrice(detailedProductData.data.price); // Reset custom price to product's default
        } else {
          /* istanbul ignore next */
          cov_y2tbfk2n7().b[24][1]++;
          cov_y2tbfk2n7().s[134]++;
          // Fallback if detailed data fetch fails but we have basic product info
          setSelectedProduct(product); // Use the basic product info
          /* istanbul ignore next */
          cov_y2tbfk2n7().s[135]++;
          setCurrentProductCost(
          /* istanbul ignore next */
          (cov_y2tbfk2n7().b[28][0]++, product.weightedAverageCost) ||
          /* istanbul ignore next */
          (cov_y2tbfk2n7().b[28][1]++, 0)); // Use WAC from list if available
          /* istanbul ignore next */
          cov_y2tbfk2n7().s[136]++;
          setCustomPrice(product.price);
          /* istanbul ignore next */
          cov_y2tbfk2n7().s[137]++;
          setProductStock(null); // Can't determine shop-specific stock
          /* istanbul ignore next */
          cov_y2tbfk2n7().s[138]++;
          console.error("Failed to fetch detailed product data or data format incorrect.");
        }
      } else {
        /* istanbul ignore next */
        cov_y2tbfk2n7().b[23][1]++;
        cov_y2tbfk2n7().s[139]++;
        setSelectedProduct(product); // Use the basic product info on API error
        /* istanbul ignore next */
        cov_y2tbfk2n7().s[140]++;
        setCurrentProductCost(
        /* istanbul ignore next */
        (cov_y2tbfk2n7().b[29][0]++, product.weightedAverageCost) ||
        /* istanbul ignore next */
        (cov_y2tbfk2n7().b[29][1]++, 0));
        /* istanbul ignore next */
        cov_y2tbfk2n7().s[141]++;
        setCustomPrice(product.price);
        /* istanbul ignore next */
        cov_y2tbfk2n7().s[142]++;
        setProductStock(null);
        /* istanbul ignore next */
        cov_y2tbfk2n7().s[143]++;
        console.error("Error fetching product details for stock and cost:", await response.text());
      }
    } catch (error) {
      /* istanbul ignore next */
      cov_y2tbfk2n7().s[144]++;
      setSelectedProduct(product); // Use the basic product info on general error
      /* istanbul ignore next */
      cov_y2tbfk2n7().s[145]++;
      setCurrentProductCost(
      /* istanbul ignore next */
      (cov_y2tbfk2n7().b[30][0]++, product.weightedAverageCost) ||
      /* istanbul ignore next */
      (cov_y2tbfk2n7().b[30][1]++, 0));
      /* istanbul ignore next */
      cov_y2tbfk2n7().s[146]++;
      setCustomPrice(product.price);
      /* istanbul ignore next */
      cov_y2tbfk2n7().s[147]++;
      setProductStock(null);
      /* istanbul ignore next */
      cov_y2tbfk2n7().s[148]++;
      console.error("Error fetching product stock and cost:", error);
    }
    /* istanbul ignore next */
    cov_y2tbfk2n7().s[149]++;
    setProductSearch(""); // Clear search
    /* istanbul ignore next */
    cov_y2tbfk2n7().s[150]++;
    setShowProductDropdown(false); // Close dropdown
  };
  // Handle adding a line item to the invoice
  /* istanbul ignore next */
  cov_y2tbfk2n7().s[151]++;
  const handleAddLineItem = () => {
    /* istanbul ignore next */
    cov_y2tbfk2n7().f[19]++;
    cov_y2tbfk2n7().s[152]++;
    if (
    /* istanbul ignore next */
    (cov_y2tbfk2n7().b[32][0]++, !selectedProduct) ||
    /* istanbul ignore next */
    (cov_y2tbfk2n7().b[32][1]++, quantity <= 0)) {
      /* istanbul ignore next */
      cov_y2tbfk2n7().b[31][0]++;
      cov_y2tbfk2n7().s[153]++;
      alert("Please select a product and enter a valid quantity.");
      /* istanbul ignore next */
      cov_y2tbfk2n7().s[154]++;
      return;
    } else
    /* istanbul ignore next */
    {
      cov_y2tbfk2n7().b[31][1]++;
    }
    // Check if shop is selected
    cov_y2tbfk2n7().s[155]++;
    if (!formData.shopId) {
      /* istanbul ignore next */
      cov_y2tbfk2n7().b[33][0]++;
      cov_y2tbfk2n7().s[156]++;
      alert("Please select a shop first.");
      /* istanbul ignore next */
      cov_y2tbfk2n7().s[157]++;
      return;
    } else
    /* istanbul ignore next */
    {
      cov_y2tbfk2n7().b[33][1]++;
    }
    // Check if sufficient stock is available in the selected shop
    cov_y2tbfk2n7().s[158]++;
    if (
    /* istanbul ignore next */
    (cov_y2tbfk2n7().b[35][0]++, productStock !== null) &&
    /* istanbul ignore next */
    (cov_y2tbfk2n7().b[35][1]++, quantity > productStock)) {
      /* istanbul ignore next */
      cov_y2tbfk2n7().b[34][0]++;
      cov_y2tbfk2n7().s[159]++;
      alert(`Insufficient stock in the selected shop. Available: ${productStock}, Requested: ${quantity}`);
      /* istanbul ignore next */
      cov_y2tbfk2n7().s[160]++;
      return;
    } else
    /* istanbul ignore next */
    {
      cov_y2tbfk2n7().b[34][1]++;
    }
    // Use custom price if set, otherwise use product's default price
    const finalPrice =
    /* istanbul ignore next */
    (cov_y2tbfk2n7().s[161]++, customPrice > 0 ?
    /* istanbul ignore next */
    (cov_y2tbfk2n7().b[36][0]++, customPrice) :
    /* istanbul ignore next */
    (cov_y2tbfk2n7().b[36][1]++,
    /* istanbul ignore next */
    (cov_y2tbfk2n7().b[37][0]++, selectedProduct.price) ||
    /* istanbul ignore next */
    (cov_y2tbfk2n7().b[37][1]++, 0)));
    const costPrice =
    /* istanbul ignore next */
    (cov_y2tbfk2n7().s[162]++, currentProductCost); // Use the fetched cost for the selected product
    const itemTotal =
    /* istanbul ignore next */
    (cov_y2tbfk2n7().s[163]++, finalPrice * quantity);
    // Check credit limit for wholesale customers
    /* istanbul ignore next */
    cov_y2tbfk2n7().s[164]++;
    if (
    /* istanbul ignore next */
    (cov_y2tbfk2n7().b[39][0]++, selectedCustomer) &&
    /* istanbul ignore next */
    (cov_y2tbfk2n7().b[39][1]++, selectedCustomer.customerType === "wholesale") &&
    /* istanbul ignore next */
    (cov_y2tbfk2n7().b[39][2]++, selectedCustomer.creditLimit)) {
      /* istanbul ignore next */
      cov_y2tbfk2n7().b[38][0]++;
      const currentTotal =
      /* istanbul ignore next */
      (cov_y2tbfk2n7().s[165]++, formData.items.reduce((sum, item) => {
        /* istanbul ignore next */
        cov_y2tbfk2n7().f[20]++;
        cov_y2tbfk2n7().s[166]++;
        return sum + item.total;
      }, 0));
      /* istanbul ignore next */
      cov_y2tbfk2n7().s[167]++;
      if (currentTotal + itemTotal > selectedCustomer.creditLimit) {
        /* istanbul ignore next */
        cov_y2tbfk2n7().b[40][0]++;
        cov_y2tbfk2n7().s[168]++;
        alert(`Adding this item exceeds the customer\'s credit limit of ${selectedCustomer.creditLimit}.`);
        /* istanbul ignore next */
        cov_y2tbfk2n7().s[169]++;
        return;
      } else
      /* istanbul ignore next */
      {
        cov_y2tbfk2n7().b[40][1]++;
      }
    } else
    /* istanbul ignore next */
    {
      cov_y2tbfk2n7().b[38][1]++;
    }
    const newLineItem =
    /* istanbul ignore next */
    (cov_y2tbfk2n7().s[170]++, {
      id: Date.now().toString(),
      productId: selectedProduct.id,
      productName: selectedProduct.name,
      quantity: quantity,
      price: finalPrice,
      costPrice: costPrice,
      total: itemTotal
    });
    /* istanbul ignore next */
    cov_y2tbfk2n7().s[171]++;
    setFormData({
      ...formData,
      items: [...formData.items, newLineItem]
    });
    // Reset selection
    /* istanbul ignore next */
    cov_y2tbfk2n7().s[172]++;
    setSelectedProduct(null);
    /* istanbul ignore next */
    cov_y2tbfk2n7().s[173]++;
    setProductStock(null);
    /* istanbul ignore next */
    cov_y2tbfk2n7().s[174]++;
    setCurrentProductCost(0); // Reset current product cost
    /* istanbul ignore next */
    cov_y2tbfk2n7().s[175]++;
    setQuantity(1);
    /* istanbul ignore next */
    cov_y2tbfk2n7().s[176]++;
    setCustomPrice(0);
  };
  // Remove line item from invoice
  /* istanbul ignore next */
  cov_y2tbfk2n7().s[177]++;
  const handleRemoveLineItem = itemId => {
    /* istanbul ignore next */
    cov_y2tbfk2n7().f[21]++;
    cov_y2tbfk2n7().s[178]++;
    setFormData({
      ...formData,
      items: formData.items.filter(item => {
        /* istanbul ignore next */
        cov_y2tbfk2n7().f[22]++;
        cov_y2tbfk2n7().s[179]++;
        return item.id !== itemId;
      })
    });
  };
  // Handle changes to quantity or price of an existing line item
  /* istanbul ignore next */
  cov_y2tbfk2n7().s[180]++;
  const handleItemDetailChange = (itemId, field, value) => {
    /* istanbul ignore next */
    cov_y2tbfk2n7().f[23]++;
    const numericValue =
    /* istanbul ignore next */
    (cov_y2tbfk2n7().s[181]++, parseFloat(value));
    /* istanbul ignore next */
    cov_y2tbfk2n7().s[182]++;
    if (
    /* istanbul ignore next */
    (cov_y2tbfk2n7().b[42][0]++, isNaN(numericValue)) &&
    /* istanbul ignore next */
    (cov_y2tbfk2n7().b[42][1]++, value !== "")) {
      /* istanbul ignore next */
      cov_y2tbfk2n7().b[41][0]++;
      cov_y2tbfk2n7().s[183]++;
      return;
    } else
    /* istanbul ignore next */
    {
      cov_y2tbfk2n7().b[41][1]++;
    }
    cov_y2tbfk2n7().s[184]++;
    setFormData(prevFormData => {
      /* istanbul ignore next */
      cov_y2tbfk2n7().f[24]++;
      const updatedItems =
      /* istanbul ignore next */
      (cov_y2tbfk2n7().s[185]++, prevFormData.items.map(item => {
        /* istanbul ignore next */
        cov_y2tbfk2n7().f[25]++;
        cov_y2tbfk2n7().s[186]++;
        if (item.id === itemId) {
          /* istanbul ignore next */
          cov_y2tbfk2n7().b[43][0]++;
          const newQuantity =
          /* istanbul ignore next */
          (cov_y2tbfk2n7().s[187]++, field === "quantity" ?
          /* istanbul ignore next */
          (cov_y2tbfk2n7().b[44][0]++, numericValue >= 0 ?
          /* istanbul ignore next */
          (cov_y2tbfk2n7().b[45][0]++, numericValue) :
          /* istanbul ignore next */
          (cov_y2tbfk2n7().b[45][1]++, item.quantity)) :
          /* istanbul ignore next */
          (cov_y2tbfk2n7().b[44][1]++, item.quantity));
          const newPrice =
          /* istanbul ignore next */
          (cov_y2tbfk2n7().s[188]++, field === "price" ?
          /* istanbul ignore next */
          (cov_y2tbfk2n7().b[46][0]++, numericValue >= 0 ?
          /* istanbul ignore next */
          (cov_y2tbfk2n7().b[47][0]++, numericValue) :
          /* istanbul ignore next */
          (cov_y2tbfk2n7().b[47][1]++, item.price)) :
          /* istanbul ignore next */
          (cov_y2tbfk2n7().b[46][1]++, item.price));
          // item.costPrice is already set and should not change when user edits qty/price
          const newTotal =
          /* istanbul ignore next */
          (cov_y2tbfk2n7().s[189]++, newQuantity * newPrice);
          /* istanbul ignore next */
          cov_y2tbfk2n7().s[190]++;
          return {
            ...item,
            quantity: newQuantity,
            price: newPrice,
            total: newTotal
          };
        } else
        /* istanbul ignore next */
        {
          cov_y2tbfk2n7().b[43][1]++;
        }
        cov_y2tbfk2n7().s[191]++;
        return item;
      }));
      /* istanbul ignore next */
      cov_y2tbfk2n7().s[192]++;
      return {
        ...prevFormData,
        items: updatedItems
      };
    });
  };
  // Handle form submission
  /* istanbul ignore next */
  cov_y2tbfk2n7().s[193]++;
  const handleSubmit = async e => {
    /* istanbul ignore next */
    cov_y2tbfk2n7().f[26]++;
    cov_y2tbfk2n7().s[194]++;
    e.preventDefault();
    // Final credit limit check for wholesale customers
    /* istanbul ignore next */
    cov_y2tbfk2n7().s[195]++;
    if (
    /* istanbul ignore next */
    (cov_y2tbfk2n7().b[49][0]++, selectedCustomer) &&
    /* istanbul ignore next */
    (cov_y2tbfk2n7().b[49][1]++, selectedCustomer.customerType === "wholesale") &&
    /* istanbul ignore next */
    (cov_y2tbfk2n7().b[49][2]++, selectedCustomer.creditLimit)) {
      /* istanbul ignore next */
      cov_y2tbfk2n7().b[48][0]++;
      const finalTotal =
      /* istanbul ignore next */
      (cov_y2tbfk2n7().s[196]++, formData.items.reduce((sum, item) => {
        /* istanbul ignore next */
        cov_y2tbfk2n7().f[27]++;
        cov_y2tbfk2n7().s[197]++;
        return sum + item.total;
      }, 0));
      /* istanbul ignore next */
      cov_y2tbfk2n7().s[198]++;
      if (finalTotal > selectedCustomer.creditLimit) {
        /* istanbul ignore next */
        cov_y2tbfk2n7().b[50][0]++;
        cov_y2tbfk2n7().s[199]++;
        alert(`The total invoice amount of ${finalTotal} exceeds the customer\'s credit limit of ${selectedCustomer.creditLimit}. Please remove items or save as draft.`);
        // Optionally, allow saving as draft or prevent submission entirely
        // For now, we prevent submission by returning.
        /* istanbul ignore next */
        cov_y2tbfk2n7().s[200]++;
        return;
      } else
      /* istanbul ignore next */
      {
        cov_y2tbfk2n7().b[50][1]++;
      }
    } else
    /* istanbul ignore next */
    {
      cov_y2tbfk2n7().b[48][1]++;
    }
    cov_y2tbfk2n7().s[201]++;
    setIsSubmitting(true);
    /* istanbul ignore next */
    cov_y2tbfk2n7().s[202]++;
    try {
      // Transform data for API
      const invoiceData =
      /* istanbul ignore next */
      (cov_y2tbfk2n7().s[203]++, {
        invoiceNumber,
        customerId: formData.customerId,
        total: invoiceTotal,
        status: formData.status,
        paymentMethod: formData.paymentMethod,
        notes: formData.notes,
        invoiceDate: formData.invoiceDate,
        dueDate: formData.dueDate,
        items: formData.items.map(item => {
          /* istanbul ignore next */
          cov_y2tbfk2n7().f[28]++;
          cov_y2tbfk2n7().s[204]++;
          return {
            productId: item.productId,
            quantity: item.quantity,
            price: item.price,
            total: item.total
          };
        }),
        shopId: formData.shopId
      });
      // Create invoice via API
      const response =
      /* istanbul ignore next */
      (cov_y2tbfk2n7().s[205]++, await fetch("/api/invoices", {
        method: "POST",
        headers: {
          "Content-Type": "application/json",
          "Authorization": accessToken ?
          /* istanbul ignore next */
          (cov_y2tbfk2n7().b[51][0]++, `Bearer ${accessToken}`) :
          /* istanbul ignore next */
          (cov_y2tbfk2n7().b[51][1]++, "")
        },
        body: JSON.stringify(invoiceData)
      }));
      const responseData =
      /* istanbul ignore next */
      (cov_y2tbfk2n7().s[206]++, await response.json());
      /* istanbul ignore next */
      cov_y2tbfk2n7().s[207]++;
      if (!response.ok) {
        /* istanbul ignore next */
        cov_y2tbfk2n7().b[52][0]++;
        cov_y2tbfk2n7().s[208]++;
        throw new Error(
        /* istanbul ignore next */
        (cov_y2tbfk2n7().b[53][0]++, responseData.message) ||
        /* istanbul ignore next */
        (cov_y2tbfk2n7().b[53][1]++, responseData.error) ||
        /* istanbul ignore next */
        (cov_y2tbfk2n7().b[53][2]++, "Failed to create invoice"));
      } else
      /* istanbul ignore next */
      {
        cov_y2tbfk2n7().b[52][1]++;
      }
      // Check if the response explicitly indicates failure
      cov_y2tbfk2n7().s[209]++;
      if (responseData.success === false) {
        /* istanbul ignore next */
        cov_y2tbfk2n7().b[54][0]++;
        cov_y2tbfk2n7().s[210]++;
        throw new Error(
        /* istanbul ignore next */
        (cov_y2tbfk2n7().b[55][0]++, responseData.message) ||
        /* istanbul ignore next */
        (cov_y2tbfk2n7().b[55][1]++, responseData.error) ||
        /* istanbul ignore next */
        (cov_y2tbfk2n7().b[55][2]++, "Failed to create invoice"));
      } else
      /* istanbul ignore next */
      {
        cov_y2tbfk2n7().b[54][1]++;
      }
      // If we get here, the invoice was created successfully
      cov_y2tbfk2n7().s[211]++;
      router.push("/invoices");
      /* istanbul ignore next */
      cov_y2tbfk2n7().s[212]++;
      router.refresh();
    } catch (error) {
      /* istanbul ignore next */
      cov_y2tbfk2n7().s[213]++;
      console.error("Error creating invoice:", error);
      /* istanbul ignore next */
      cov_y2tbfk2n7().s[214]++;
      alert("Failed to create invoice. Please try again.");
    } finally {
      /* istanbul ignore next */
      cov_y2tbfk2n7().s[215]++;
      setIsSubmitting(false);
    }
  };
  /* istanbul ignore next */
  cov_y2tbfk2n7().s[216]++;
  const handleQuickCustomerSubmit = async e => {
    /* istanbul ignore next */
    cov_y2tbfk2n7().f[29]++;
    cov_y2tbfk2n7().s[217]++;
    e.preventDefault();
    /* istanbul ignore next */
    cov_y2tbfk2n7().s[218]++;
    setIsCreatingCustomer(true);
    /* istanbul ignore next */
    cov_y2tbfk2n7().s[219]++;
    try {
      const response =
      /* istanbul ignore next */
      (cov_y2tbfk2n7().s[220]++, await fetch("/api/customers", {
        method: "POST",
        headers: {
          "Content-Type": "application/json"
        },
        body: JSON.stringify({
          name: quickCustomerData.name,
          phone: quickCustomerData.phone,
          address: quickCustomerData.address,
          customerType: quickCustomerData.customerType,
          creditLimit: quickCustomerData.customerType === "Wholesale" ?
          /* istanbul ignore next */
          (cov_y2tbfk2n7().b[56][0]++, quickCustomerData.creditLimit) :
          /* istanbul ignore next */
          (cov_y2tbfk2n7().b[56][1]++, null),
          creditPeriod: quickCustomerData.customerType === "Wholesale" ?
          /* istanbul ignore next */
          (cov_y2tbfk2n7().b[57][0]++, quickCustomerData.creditPeriod) :
          /* istanbul ignore next */
          (cov_y2tbfk2n7().b[57][1]++, null)
        })
      }));
      /* istanbul ignore next */
      cov_y2tbfk2n7().s[221]++;
      if (response.ok) {
        /* istanbul ignore next */
        cov_y2tbfk2n7().b[58][0]++;
        const result =
        /* istanbul ignore next */
        (cov_y2tbfk2n7().s[222]++, await response.json());
        const newCustomer =
        /* istanbul ignore next */
        (cov_y2tbfk2n7().s[223]++, {
          id: result.data.id,
          name: result.data.name,
          email: result.data.email,
          phone: result.data.phone,
          customerType: result.data.customerType.toLowerCase(),
          creditLimit: result.data.creditLimit,
          creditPeriod: result.data.creditPeriod
        });
        // Add to customers list and select it
        /* istanbul ignore next */
        cov_y2tbfk2n7().s[224]++;
        setCustomers(prev => {
          /* istanbul ignore next */
          cov_y2tbfk2n7().f[30]++;
          cov_y2tbfk2n7().s[225]++;
          return [...prev, newCustomer];
        });
        /* istanbul ignore next */
        cov_y2tbfk2n7().s[226]++;
        handleSelectCustomer(newCustomer);
        // Reset modal
        /* istanbul ignore next */
        cov_y2tbfk2n7().s[227]++;
        setShowQuickCustomerModal(false);
        /* istanbul ignore next */
        cov_y2tbfk2n7().s[228]++;
        setQuickCustomerData({
          name: "",
          phone: "",
          address: "",
          customerType: "Retail",
          creditLimit: 0,
          creditPeriod: 0
        });
      } else {
        /* istanbul ignore next */
        cov_y2tbfk2n7().b[58][1]++;
        const errorData =
        /* istanbul ignore next */
        (cov_y2tbfk2n7().s[229]++, await response.json());
        /* istanbul ignore next */
        cov_y2tbfk2n7().s[230]++;
        alert(`Failed to create customer: ${
        /* istanbul ignore next */
        (cov_y2tbfk2n7().b[59][0]++, errorData.message) ||
        /* istanbul ignore next */
        (cov_y2tbfk2n7().b[59][1]++, "Unknown error")}`);
      }
    } catch (error) {
      /* istanbul ignore next */
      cov_y2tbfk2n7().s[231]++;
      console.error("Error creating customer:", error);
      /* istanbul ignore next */
      cov_y2tbfk2n7().s[232]++;
      alert("An error occurred while creating the customer.");
    } finally {
      /* istanbul ignore next */
      cov_y2tbfk2n7().s[233]++;
      setIsCreatingCustomer(false);
    }
  };
  /* istanbul ignore next */
  cov_y2tbfk2n7().s[234]++;
  const handleQuickCustomerInputChange = e => {
    /* istanbul ignore next */
    cov_y2tbfk2n7().f[31]++;
    const {
      name,
      value,
      type
    } =
    /* istanbul ignore next */
    (cov_y2tbfk2n7().s[235]++, e.target);
    /* istanbul ignore next */
    cov_y2tbfk2n7().s[236]++;
    setQuickCustomerData(prev => {
      /* istanbul ignore next */
      cov_y2tbfk2n7().f[32]++;
      cov_y2tbfk2n7().s[237]++;
      return {
        ...prev,
        [name]: type === "number" ?
        /* istanbul ignore next */
        (cov_y2tbfk2n7().b[60][0]++, value === "" ?
        /* istanbul ignore next */
        (cov_y2tbfk2n7().b[61][0]++, 0) :
        /* istanbul ignore next */
        (cov_y2tbfk2n7().b[61][1]++, Number(value))) :
        /* istanbul ignore next */
