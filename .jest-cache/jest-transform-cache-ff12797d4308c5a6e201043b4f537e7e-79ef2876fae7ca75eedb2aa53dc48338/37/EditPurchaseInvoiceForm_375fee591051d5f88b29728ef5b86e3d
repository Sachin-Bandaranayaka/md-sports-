    } else
    /* istanbul ignore next */
    {
      cov_sdv2kyoyg().b[63][1]++;
    }
    cov_sdv2kyoyg().s[187]++;
    setNewProductSubmitting(true);
    /* istanbul ignore next */
    cov_sdv2kyoyg().s[188]++;
    setNewProductModalError(null);
    /* istanbul ignore next */
    cov_sdv2kyoyg().s[189]++;
