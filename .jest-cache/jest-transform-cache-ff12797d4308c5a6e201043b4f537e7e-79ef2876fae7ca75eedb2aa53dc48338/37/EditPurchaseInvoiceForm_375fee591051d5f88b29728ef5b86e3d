    } else
    /* istanbul ignore next */
    {
      cov_sdv2kyoyg().b[39][1]++;
    }
    const quantity =
    /* istanbul ignore next */
    (cov_sdv2kyoyg().s[134]++, quantityStr === "" ?
    /* istanbul ignore next */
