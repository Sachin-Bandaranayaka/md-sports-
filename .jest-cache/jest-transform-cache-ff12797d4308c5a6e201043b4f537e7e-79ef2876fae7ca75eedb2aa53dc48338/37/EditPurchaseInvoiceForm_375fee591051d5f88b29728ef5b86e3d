        /* istanbul ignore next */
        cov_sdv2kyoyg().b[79][0]++;
        const errorData =
        /* istanbul ignore next */
        (cov_sdv2kyoyg().s[228]++, await response.json());
        /* istanbul ignore next */
        cov_sdv2kyoyg().s[229]++;
        throw new Error(
        /* istanbul ignore next */
        (cov_sdv2kyoyg().b[80][0]++, errorData.error) ||
        /* istanbul ignore next */
        (cov_sdv2kyoyg().b[80][1]++, errorData.message) ||
        /* istanbul ignore next */
        (cov_sdv2kyoyg().b[80][2]++, "Failed to update purchase invoice"));
      } else
      /* istanbul ignore next */
      {
        cov_sdv2kyoyg().b[79][1]++;
      }
      // Force immediate cache invalidation for this purchase invoice
      cov_sdv2kyoyg().s[230]++;
      try {
        /* istanbul ignore next */
        cov_sdv2kyoyg().s[231]++;
        await fetch("/api/revalidate", {
          method: "POST",
          headers: {
            "Content-Type": "application/json"
          },
          body: JSON.stringify({
            tags: ["purchase-invoices", `purchase-${initialInvoice.id}`],
            paths: [`/purchases/${initialInvoice.id}`, `/purchases/${initialInvoice.id}/edit`, "/purchases"]
          })
        });
      } catch (revalidateError) {
        /* istanbul ignore next */
        cov_sdv2kyoyg().s[232]++;
        console.warn("Failed to trigger revalidation:", revalidateError);
      }
      // Force router refresh to show updated data immediately
      /* istanbul ignore next */
      cov_sdv2kyoyg().s[233]++;
      router.refresh();
      // Short delay to ensure cache invalidation is processed
      /* istanbul ignore next */
      cov_sdv2kyoyg().s[234]++;
      await new Promise(resolve => {
        /* istanbul ignore next */
        cov_sdv2kyoyg().f[47]++;
        cov_sdv2kyoyg().s[235]++;
        return setTimeout(resolve, 100);
      });
      // Navigate with success message and force hard refresh
      /* istanbul ignore next */
      cov_sdv2kyoyg().s[236]++;
      router.push("/purchases?status=success&action=update");
      // If user navigates back to edit page, they'll see updated data
      /* istanbul ignore next */
      cov_sdv2kyoyg().s[237]++;
      if (typeof window !== "undefined") {
        /* istanbul ignore next */
        cov_sdv2kyoyg().b[81][0]++;
        cov_sdv2kyoyg().s[238]++;
        // Store a flag to force refresh when returning to edit page
        sessionStorage.setItem(`purchase-${initialInvoice.id}-updated`, Date.now().toString());
      } else
      /* istanbul ignore next */
      {
        cov_sdv2kyoyg().b[81][1]++;
      }
    } catch (err) {
      /* istanbul ignore next */
      cov_sdv2kyoyg().s[239]++;
      setError(
      /* istanbul ignore next */
      (cov_sdv2kyoyg().b[82][0]++, err.message) ||
      /* istanbul ignore next */
      (cov_sdv2kyoyg().b[82][1]++, "An unexpected error occurred."));
    } finally {
      /* istanbul ignore next */
      cov_sdv2kyoyg().s[240]++;
      setSubmitting(false);
    }
  };
  // Remove the global product search handler - let each Combobox handle its own filtering
  /* istanbul ignore next */
  cov_sdv2kyoyg().s[241]++;
  return /*#__PURE__*/(0, _jsxruntime.jsxs)("form", {
    onSubmit: handleSubmit,
    className: "space-y-6 pb-24",
    children: [
    /* istanbul ignore next */
    (cov_sdv2kyoyg().b[83][0]++, error) &&
    /* istanbul ignore next */
    (cov_sdv2kyoyg().b[83][1]++, /*#__PURE__*/(0, _jsxruntime.jsxs)(_framermotion.motion.div, {
