            /* istanbul ignore next */
            (cov_sdv2kyoyg().b[16][0]++, newQty !== oldQty) &&
            /* istanbul ignore next */
            (cov_sdv2kyoyg().b[16][1]++, oldQty > 0)) {
              /* istanbul ignore next */
              cov_sdv2kyoyg().b[15][0]++;
              // Proportionally adjust existing distributions
              const currentDist =
              /* istanbul ignore next */
              (cov_sdv2kyoyg().s[52]++,
              /* istanbul ignore next */
              (cov_sdv2kyoyg().b[17][0]++, newDists[itemIndex]) ||
              /* istanbul ignore next */
              (cov_sdv2kyoyg().b[17][1]++, {}));
              const totalDistributed =
              /* istanbul ignore next */
              (cov_sdv2kyoyg().s[53]++, Object.values(currentDist).reduce((sum, qty) => {
                /* istanbul ignore next */
                cov_sdv2kyoyg().f[13]++;
                cov_sdv2kyoyg().s[54]++;
                return sum + Number(qty);
              }, 0));
              /* istanbul ignore next */
              cov_sdv2kyoyg().s[55]++;
              if (totalDistributed > 0) {
                /* istanbul ignore next */
                cov_sdv2kyoyg().b[18][0]++;
                const ratio =
                /* istanbul ignore next */
                (cov_sdv2kyoyg().s[56]++, newQty / oldQty);
                const adjustedDist =
                /* istanbul ignore next */
                (cov_sdv2kyoyg().s[57]++, {});
                /* istanbul ignore next */
                cov_sdv2kyoyg().s[58]++;
                for (const [shopId, qty] of Object.entries(currentDist)) {
                  const adjustedQty =
                  /* istanbul ignore next */
                  (cov_sdv2kyoyg().s[59]++, Math.round(Number(qty) * ratio));
                  /* istanbul ignore next */
                  cov_sdv2kyoyg().s[60]++;
