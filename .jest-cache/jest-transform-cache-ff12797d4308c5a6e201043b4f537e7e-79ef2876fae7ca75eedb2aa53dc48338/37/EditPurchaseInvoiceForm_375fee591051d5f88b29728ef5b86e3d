          /* istanbul ignore next */
          (cov_sdv2kyoyg().s[48]++, [...prevDists]);
          /* istanbul ignore next */
          cov_sdv2kyoyg().s[49]++;
          currentQuantities.forEach((newQty, itemIndex) => {
            /* istanbul ignore next */
            cov_sdv2kyoyg().f[12]++;
            const oldQty =
            /* istanbul ignore next */
            (cov_sdv2kyoyg().s[50]++,
            /* istanbul ignore next */
