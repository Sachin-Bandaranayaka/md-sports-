  /* istanbul ignore next */
  cov_sdv2kyoyg().s[97]++;
  const handleItemChange = (e, index) => {
    /* istanbul ignore next */
    cov_sdv2kyoyg().f[22]++;
    const {
      name,
      value
    } =
    /* istanbul ignore next */
    (cov_sdv2kyoyg().s[98]++, e.target);
