        categoryId: ""
      });
    } catch (err) {
      /* istanbul ignore next */
      cov_sdv2kyoyg().s[199]++;
      setNewProductModalError(err.message);
    } finally {
      /* istanbul ignore next */
      cov_sdv2kyoyg().s[200]++;
      setNewProductSubmitting(false);
    }
  };
  /* istanbul ignore next */
  cov_sdv2kyoyg().s[201]++;
  const handleSubmit = async e => {
    /* istanbul ignore next */
    cov_sdv2kyoyg().f[43]++;
    cov_sdv2kyoyg().s[202]++;
    e.preventDefault();
    /* istanbul ignore next */
    cov_sdv2kyoyg().s[203]++;
    setError(null);
    /* istanbul ignore next */
    cov_sdv2kyoyg().s[204]++;
    if (!formData.supplierId) {
      /* istanbul ignore next */
      cov_sdv2kyoyg().b[67][0]++;
      cov_sdv2kyoyg().s[205]++;
      setError("Please select a supplier.");
      /* istanbul ignore next */
      cov_sdv2kyoyg().s[206]++;
      return;
    } else
    /* istanbul ignore next */
    {
      cov_sdv2kyoyg().b[67][1]++;
    }
    cov_sdv2kyoyg().s[207]++;
    if (
    /* istanbul ignore next */
    (cov_sdv2kyoyg().b[69][0]++, !formData.items) ||
    /* istanbul ignore next */
    (cov_sdv2kyoyg().b[69][1]++, formData.items.length === 0)) {
      /* istanbul ignore next */
      cov_sdv2kyoyg().b[68][0]++;
      cov_sdv2kyoyg().s[208]++;
