    setPreviousItemQuantities(prev => {
      /* istanbul ignore next */
      cov_sdv2kyoyg().f[30]++;
      cov_sdv2kyoyg().s[126]++;
      return prev.filter((_, i) => {
        /* istanbul ignore next */
        cov_sdv2kyoyg().f[31]++;
        cov_sdv2kyoyg().s[127]++;
        return i !== index;
      });
    });
  };
  /* istanbul ignore next */
  cov_sdv2kyoyg().s[128]++;
  const handleOpenDistributionModal = itemIndex => {
    /* istanbul ignore next */
    cov_sdv2kyoyg().f[32]++;
    cov_sdv2kyoyg().s[129]++;
    setSelectedItemIndexForDistribution(itemIndex);
    /* istanbul ignore next */
    cov_sdv2kyoyg().s[130]++;
