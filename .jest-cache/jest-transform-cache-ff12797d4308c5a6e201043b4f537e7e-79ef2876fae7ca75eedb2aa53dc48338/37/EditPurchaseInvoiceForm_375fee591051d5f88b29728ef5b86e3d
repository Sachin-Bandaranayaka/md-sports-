  }))));
  // Track previous item quantities to detect changes
  const [previousItemQuantities, setPreviousItemQuantities] =
  /* istanbul ignore next */
  (cov_sdv2kyoyg().s[32]++, (0, _react.useState)(initialInvoice.items.map(item => {
