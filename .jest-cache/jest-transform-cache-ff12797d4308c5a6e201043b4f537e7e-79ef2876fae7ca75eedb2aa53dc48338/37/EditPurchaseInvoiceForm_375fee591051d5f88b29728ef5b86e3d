      /* istanbul ignore next */
      {
        cov_sdv2kyoyg().b[25][1]++;
      }
    } else
    /* istanbul ignore next */
    {
      cov_sdv2kyoyg().b[24][1]++;
    }
  }, [fetchUpdatedProductData, initialInvoice.id]);
  /* istanbul ignore next */
  cov_sdv2kyoyg().s[87]++;
  const handleChange = e => {
    /* istanbul ignore next */
    cov_sdv2kyoyg().f[17]++;
    const {
      name,
      value
