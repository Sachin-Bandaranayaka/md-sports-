      isValid,
      error: validationError
    } =
    /* istanbul ignore next */
    (cov_sdv2kyoyg().s[215]++, validateDistributions());
    /* istanbul ignore next */
    cov_sdv2kyoyg().s[216]++;
    if (!isValid) {
      /* istanbul ignore next */
      cov_sdv2kyoyg().b[73][0]++;
      cov_sdv2kyoyg().s[217]++;
      setError(
      /* istanbul ignore next */
      (cov_sdv2kyoyg().b[74][0]++, validationError) ||
      /* istanbul ignore next */
      (cov_sdv2kyoyg().b[74][1]++, "An unexpected error occurred during distribution validation."));
      /* istanbul ignore next */
      cov_sdv2kyoyg().s[218]++;
      return;
    } else
    /* istanbul ignore next */
    {
      cov_sdv2kyoyg().b[73][1]++;
    }
    cov_sdv2kyoyg().s[219]++;
    setSubmitting(true);
    // Ensure distributions array matches items array length
    const syncedDistributions =
    /* istanbul ignore next */
    (cov_sdv2kyoyg().s[220]++, formData.items.map((_, index) => {
      /* istanbul ignore next */
      cov_sdv2kyoyg().f[45]++;
      cov_sdv2kyoyg().s[221]++;
      return /* istanbul ignore next */(cov_sdv2kyoyg().b[75][0]++, itemDistributions[index]) ||
      /* istanbul ignore next */
      (cov_sdv2kyoyg().b[75][1]++, {});
    }));
    const submissionData =
    /* istanbul ignore next */
    (cov_sdv2kyoyg().s[222]++, {
      ...formData,
      items: formData.items.map(item => {
        /* istanbul ignore next */
        cov_sdv2kyoyg().f[46]++;
        cov_sdv2kyoyg().s[223]++;
