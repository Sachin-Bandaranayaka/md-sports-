        /* istanbul ignore next */
        (cov_sdv2kyoyg().b[23][0]++, data.success) &&
        /* istanbul ignore next */
        (cov_sdv2kyoyg().b[23][1]++, data.data)) {
          /* istanbul ignore next */
          cov_sdv2kyoyg().b[22][0]++;
          cov_sdv2kyoyg().s[76]++;
          setProducts(data.data);
        } else
        /* istanbul ignore next */
        {
          cov_sdv2kyoyg().b[22][1]++;
        }
      } else
      /* istanbul ignore next */
      {
        cov_sdv2kyoyg().b[21][1]++;
      }
    } catch (error) {
      /* istanbul ignore next */
      cov_sdv2kyoyg().s[77]++;
      console.error("Error fetching updated product data:", error);
    }
  }, []));
  // Force refresh products on component mount to ensure latest data
  /* istanbul ignore next */
  cov_sdv2kyoyg().s[78]++;
  (0, _react.useEffect)(() => {
    /* istanbul ignore next */
    cov_sdv2kyoyg().f[15]++;
    cov_sdv2kyoyg().s[79]++;
    fetchUpdatedProductData();
    // Check if we just updated this invoice and need to refresh data
    /* istanbul ignore next */
    cov_sdv2kyoyg().s[80]++;
    if (typeof window !== "undefined") {
      /* istanbul ignore next */
      cov_sdv2kyoyg().b[24][0]++;
