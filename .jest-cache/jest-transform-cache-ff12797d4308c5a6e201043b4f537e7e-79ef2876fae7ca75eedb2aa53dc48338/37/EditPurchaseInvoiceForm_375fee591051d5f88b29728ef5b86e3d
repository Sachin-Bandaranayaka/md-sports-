  };
  /* istanbul ignore next */
  cov_sdv2kyoyg().s[131]++;
  const handleDistributionChange = (shopId, quantityStr) => {
    /* istanbul ignore next */
    cov_sdv2kyoyg().f[33]++;
    cov_sdv2kyoyg().s[132]++;
    if (selectedItemIndexForDistribution === null) {
      /* istanbul ignore next */
      cov_sdv2kyoyg().b[39][0]++;
