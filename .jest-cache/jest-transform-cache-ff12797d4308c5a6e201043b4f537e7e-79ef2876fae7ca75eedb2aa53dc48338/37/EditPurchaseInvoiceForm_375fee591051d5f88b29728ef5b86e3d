      /* istanbul ignore next */
      (cov_sdv2kyoyg().s[81]++, sessionStorage.getItem(`purchase-${initialInvoice.id}-updated`));
      /* istanbul ignore next */
      cov_sdv2kyoyg().s[82]++;
