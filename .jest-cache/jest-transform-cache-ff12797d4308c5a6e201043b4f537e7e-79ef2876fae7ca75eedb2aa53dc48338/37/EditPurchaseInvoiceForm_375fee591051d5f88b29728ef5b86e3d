    /* istanbul ignore next */
    (cov_sdv2kyoyg().b[64][3]++, newProductData.weightedAverageCost <= 0)) {
      /* istanbul ignore next */
      cov_sdv2kyoyg().b[63][0]++;
      cov_sdv2kyoyg().s[185]++;
