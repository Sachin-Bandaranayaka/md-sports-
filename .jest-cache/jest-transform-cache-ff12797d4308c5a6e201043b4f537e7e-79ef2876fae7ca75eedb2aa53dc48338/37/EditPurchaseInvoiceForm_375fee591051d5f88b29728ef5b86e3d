        // Clear the flag
        /* istanbul ignore next */
        cov_sdv2kyoyg().s[84]++;
        sessionStorage.removeItem(`purchase-${initialInvoice.id}-updated`);
        // Force a page reload to get fresh data
        /* istanbul ignore next */
        cov_sdv2kyoyg().s[85]++;
