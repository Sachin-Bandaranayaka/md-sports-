          /* istanbul ignore next */
          cov_sdv2kyoyg().s[67]++;
          return newDists;
        });
        /* istanbul ignore next */
        cov_sdv2kyoyg().s[68]++;
        setPreviousItemQuantities(currentQuantities);
      } else
      /* istanbul ignore next */
      {
        cov_sdv2kyoyg().b[13][1]++;
      }
    } else
    /* istanbul ignore next */
    {
      cov_sdv2kyoyg().b[10][1]++;
    }
  }, [formData.items, previousItemQuantities]);
  // Function to fetch updated product data
  const fetchUpdatedProductData =
  /* istanbul ignore next */
  (cov_sdv2kyoyg().s[69]++, (0, _react.useCallback)(async productId => {
    /* istanbul ignore next */
    cov_sdv2kyoyg().f[14]++;
    cov_sdv2kyoyg().s[70]++;
    try {
      // Add timestamp to ensure cache busting
      const timestamp =
      /* istanbul ignore next */
      (cov_sdv2kyoyg().s[71]++, Date.now());
      const response =
      /* istanbul ignore next */
      (cov_sdv2kyoyg().s[72]++, await fetch(`/api/products?t=${timestamp}&limit=10000`, {
        cache: "no-store",
