      /* istanbul ignore next */
      cov_sdv2kyoyg().s[209]++;
      return;
    } else
    /* istanbul ignore next */
    {
      cov_sdv2kyoyg().b[68][1]++;
    }
    // Validate all items have valid products and quantities
    const invalidItems =
    /* istanbul ignore next */
    (cov_sdv2kyoyg().s[210]++, (
    /* istanbul ignore next */
    (cov_sdv2kyoyg().b[70][0]++, formData.items) ||
    /* istanbul ignore next */
    (cov_sdv2kyoyg().b[70][1]++, [])).filter(item => {
      /* istanbul ignore next */
      cov_sdv2kyoyg().f[44]++;
      cov_sdv2kyoyg().s[211]++;
      return /* istanbul ignore next */(cov_sdv2kyoyg().b[71][0]++, !item.productId) ||
      /* istanbul ignore next */
