      /* istanbul ignore next */
      (cov_sdv2kyoyg().b[71][2]++, item.quantity <= 0) ||
      /* istanbul ignore next */
      (cov_sdv2kyoyg().b[71][3]++, !item.price) ||
      /* istanbul ignore next */
      (cov_sdv2kyoyg().b[71][4]++, item.price <= 0);
    }));
    /* istanbul ignore next */
    cov_sdv2kyoyg().s[212]++;
