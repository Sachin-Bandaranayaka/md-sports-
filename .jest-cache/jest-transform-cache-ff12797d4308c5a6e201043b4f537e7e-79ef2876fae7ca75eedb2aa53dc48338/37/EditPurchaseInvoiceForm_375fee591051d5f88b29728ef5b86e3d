        /* istanbul ignore next */
        cov_sdv2kyoyg().b[65][0]++;
        const errData =
        /* istanbul ignore next */
        (cov_sdv2kyoyg().s[192]++, await response.json());
        /* istanbul ignore next */
        cov_sdv2kyoyg().s[193]++;
        throw new Error(
        /* istanbul ignore next */
        (cov_sdv2kyoyg().b[66][0]++, errData.message) ||
        /* istanbul ignore next */
        (cov_sdv2kyoyg().b[66][1]++, "Failed to create product"));
