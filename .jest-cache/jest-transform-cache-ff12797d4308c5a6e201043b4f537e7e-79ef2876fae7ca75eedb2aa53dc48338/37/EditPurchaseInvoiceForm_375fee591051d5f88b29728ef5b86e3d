    /* istanbul ignore next */
    (cov_sdv2kyoyg().b[40][1]++,
    /* istanbul ignore next */
    (cov_sdv2kyoyg().b[41][0]++, parseInt(quantityStr)) ||
    /* istanbul ignore next */
    (cov_sdv2kyoyg().b[41][1]++, 0)));
    /* istanbul ignore next */
    cov_sdv2kyoyg().s[135]++;
    setItemDistributions(prevDists => {
      /* istanbul ignore next */
      cov_sdv2kyoyg().f[34]++;
      const newDists =
      /* istanbul ignore next */
      (cov_sdv2kyoyg().s[136]++, [...prevDists]);
      const currentItemDist =
      /* istanbul ignore next */
      (cov_sdv2kyoyg().s[137]++,
      /* istanbul ignore next */
      (cov_sdv2kyoyg().b[42][0]++, {
        ...newDists[selectedItemIndexForDistribution]
      }) ||
      /* istanbul ignore next */
      (cov_sdv2kyoyg().b[42][1]++, {}));
      /* istanbul ignore next */
      cov_sdv2kyoyg().s[138]++;
      if (quantity > 0) {
        /* istanbul ignore next */
        cov_sdv2kyoyg().b[43][0]++;
        cov_sdv2kyoyg().s[139]++;
        currentItemDist[shopId] = quantity;
      } else {
        /* istanbul ignore next */
        cov_sdv2kyoyg().b[43][1]++;
        cov_sdv2kyoyg().s[140]++;
        delete currentItemDist[shopId];
      }
      /* istanbul ignore next */
      cov_sdv2kyoyg().s[141]++;
      newDists[selectedItemIndexForDistribution] = currentItemDist;
      /* istanbul ignore next */
      cov_sdv2kyoyg().s[142]++;
      return newDists;
    });
  };
  /* istanbul ignore next */
  cov_sdv2kyoyg().s[143]++;
  const getTotalDistributedForItem = itemIndex => {
    /* istanbul ignore next */
    cov_sdv2kyoyg().f[35]++;
    cov_sdv2kyoyg().s[144]++;
    if (
    /* istanbul ignore next */
    (cov_sdv2kyoyg().b[45][0]++, itemIndex === null) ||
    /* istanbul ignore next */
    (cov_sdv2kyoyg().b[45][1]++, itemIndex < 0) ||
    /* istanbul ignore next */
    (cov_sdv2kyoyg().b[45][2]++, itemIndex >= itemDistributions.length)) {
      /* istanbul ignore next */
      cov_sdv2kyoyg().b[44][0]++;
      cov_sdv2kyoyg().s[145]++;
      return 0;
    } else
    /* istanbul ignore next */
    {
      cov_sdv2kyoyg().b[44][1]++;
    }
    cov_sdv2kyoyg().s[146]++;
    if (
    /* istanbul ignore next */
    (cov_sdv2kyoyg().b[47][0]++, !itemDistributions[itemIndex]) ||
    /* istanbul ignore next */
    (cov_sdv2kyoyg().b[47][1]++, typeof itemDistributions[itemIndex] !== "object")) {
      /* istanbul ignore next */
      cov_sdv2kyoyg().b[46][0]++;
      cov_sdv2kyoyg().s[147]++;
      return 0;
    } else
    /* istanbul ignore next */
    {
      cov_sdv2kyoyg().b[46][1]++;
    }
    cov_sdv2kyoyg().s[148]++;
    return Object.values(itemDistributions[itemIndex]).reduce((sum, qty) => {
      /* istanbul ignore next */
      cov_sdv2kyoyg().f[36]++;
      cov_sdv2kyoyg().s[149]++;
      return sum + (
      /* istanbul ignore next */
      (cov_sdv2kyoyg().b[48][0]++, Number(qty)) ||
      /* istanbul ignore next */
      (cov_sdv2kyoyg().b[48][1]++, 0));
    }, 0);
  };
  // Helper function to validate distributions
  /* istanbul ignore next */
  cov_sdv2kyoyg().s[150]++;
  const validateDistributions = () => {
    /* istanbul ignore next */
    cov_sdv2kyoyg().f[37]++;
    cov_sdv2kyoyg().s[151]++;
    if (
    /* istanbul ignore next */
    (cov_sdv2kyoyg().b[50][0]++, !formData.items) ||
    /* istanbul ignore next */
    (cov_sdv2kyoyg().b[50][1]++, formData.items.length === 0)) {
      /* istanbul ignore next */
      cov_sdv2kyoyg().b[49][0]++;
      cov_sdv2kyoyg().s[152]++;
      return {
        isValid: false,
        error: "No items to validate"
      };
    } else
    /* istanbul ignore next */
    {
      cov_sdv2kyoyg().b[49][1]++;
    }
    cov_sdv2kyoyg().s[153]++;
    if (shops.length === 0) {
      /* istanbul ignore next */
      cov_sdv2kyoyg().b[51][0]++;
      cov_sdv2kyoyg().s[154]++;
      return {
        isValid: false,
        error: "No shops available for distribution. Please configure at least one shop before editing purchase invoices."
      };
    } else
    /* istanbul ignore next */
    {
      cov_sdv2kyoyg().b[51][1]++;
    }
    cov_sdv2kyoyg().s[155]++;
    for (let i =
    /* istanbul ignore next */
    (cov_sdv2kyoyg().s[156]++, 0); i < formData.items.length; i++) {
      const item =
      /* istanbul ignore next */
      (cov_sdv2kyoyg().s[157]++, formData.items[i]);
      const distribution =
      /* istanbul ignore next */
      (cov_sdv2kyoyg().s[158]++,
      /* istanbul ignore next */
      (cov_sdv2kyoyg().b[52][0]++, itemDistributions[i]) ||
      /* istanbul ignore next */
      (cov_sdv2kyoyg().b[52][1]++, {}));
      // Calculate total distributed quantity for this item
      const totalDistributed =
      /* istanbul ignore next */
      (cov_sdv2kyoyg().s[159]++, Object.values(distribution).reduce((sum, qty) => {
        /* istanbul ignore next */
        cov_sdv2kyoyg().f[38]++;
        const num =
        /* istanbul ignore next */
        (cov_sdv2kyoyg().s[160]++,
        /* istanbul ignore next */
        (cov_sdv2kyoyg().b[53][0]++, Number(qty)) ||
        /* istanbul ignore next */
        (cov_sdv2kyoyg().b[53][1]++, 0));
        /* istanbul ignore next */
        cov_sdv2kyoyg().s[161]++;
        return sum + num;
      }, 0));
      const requiredQuantity =
      /* istanbul ignore next */
      (cov_sdv2kyoyg().s[162]++, Number(item.quantity));
      /* istanbul ignore next */
      cov_sdv2kyoyg().s[163]++;
      if (totalDistributed === 0) {
        /* istanbul ignore next */
        cov_sdv2kyoyg().b[54][0]++;
        cov_sdv2kyoyg().s[164]++;
        return {
          isValid: false,
          error: `Product "${
          /* istanbul ignore next */
          (cov_sdv2kyoyg().b[55][0]++, item.productName) ||
          /* istanbul ignore next */
          (cov_sdv2kyoyg().b[55][1]++, `Product ${item.productId}`)}" has no distribution set. Please distribute all quantities to shops.`
        };
      } else
      /* istanbul ignore next */
      {
        cov_sdv2kyoyg().b[54][1]++;
      }
      cov_sdv2kyoyg().s[165]++;
      if (totalDistributed !== requiredQuantity) {
        /* istanbul ignore next */
        cov_sdv2kyoyg().b[56][0]++;
        cov_sdv2kyoyg().s[166]++;
        return {
          isValid: false,
          error: `Product "${
          /* istanbul ignore next */
          (cov_sdv2kyoyg().b[57][0]++, item.productName) ||
          /* istanbul ignore next */
          (cov_sdv2kyoyg().b[57][1]++, `Product ${item.productId}`)}" distribution mismatch. Required: ${requiredQuantity}, Distributed: ${totalDistributed}`
        };
      } else
      /* istanbul ignore next */
      {
        cov_sdv2kyoyg().b[56][1]++;
      }
    }
    /* istanbul ignore next */
    cov_sdv2kyoyg().s[167]++;
    return {
      isValid: true,
      error: null
    };
  };
  /* istanbul ignore next */
  cov_sdv2kyoyg().s[168]++;
  const getItemDistributionStatus = itemIndex => {
    /* istanbul ignore next */
    cov_sdv2kyoyg().f[39]++;
    cov_sdv2kyoyg().s[169]++;
    if (
    /* istanbul ignore next */
    (cov_sdv2kyoyg().b[59][0]++, !formData.items) ||
    /* istanbul ignore next */
    (cov_sdv2kyoyg().b[59][1]++, !formData.items[itemIndex])) {
      /* istanbul ignore next */
      cov_sdv2kyoyg().b[58][0]++;
      cov_sdv2kyoyg().s[170]++;
      return {
        status: "none",
        message: "No item"
      };
    } else
    /* istanbul ignore next */
    {
      cov_sdv2kyoyg().b[58][1]++;
    }
    const item =
    /* istanbul ignore next */
    (cov_sdv2kyoyg().s[171]++, formData.items[itemIndex]);
    const requiredQty =
    /* istanbul ignore next */
    (cov_sdv2kyoyg().s[172]++, Number(item.quantity));
    const distributedQty =
    /* istanbul ignore next */
    (cov_sdv2kyoyg().s[173]++, getTotalDistributedForItem(itemIndex));
    /* istanbul ignore next */
    cov_sdv2kyoyg().s[174]++;
    if (distributedQty === 0) {
      /* istanbul ignore next */
      cov_sdv2kyoyg().b[60][0]++;
      cov_sdv2kyoyg().s[175]++;
      return {
        status: "none",
        message: "Not distributed"
      };
    } else {
      /* istanbul ignore next */
      cov_sdv2kyoyg().b[60][1]++;
      cov_sdv2kyoyg().s[176]++;
      if (distributedQty < requiredQty) {
        /* istanbul ignore next */
        cov_sdv2kyoyg().b[61][0]++;
        cov_sdv2kyoyg().s[177]++;
        return {
          status: "partial",
          message: `${distributedQty}/${requiredQty} distributed`
        };
      } else {
        /* istanbul ignore next */
        cov_sdv2kyoyg().b[61][1]++;
        cov_sdv2kyoyg().s[178]++;
        if (distributedQty === requiredQty) {
          /* istanbul ignore next */
          cov_sdv2kyoyg().b[62][0]++;
          cov_sdv2kyoyg().s[179]++;
          return {
            status: "complete",
            message: "Fully distributed"
          };
        } else {
          /* istanbul ignore next */
          cov_sdv2kyoyg().b[62][1]++;
          cov_sdv2kyoyg().s[180]++;
          return {
            status: "over",
            message: `Over-distributed: ${distributedQty}/${requiredQty}`
          };
        }
      }
    }
  };
  // New helper to get shop options for combobox, ensuring shop.id is string
  const shopOptions =
  /* istanbul ignore next */
  (cov_sdv2kyoyg().s[181]++, shops.map(shop => {
    /* istanbul ignore next */
    cov_sdv2kyoyg().f[40]++;
    cov_sdv2kyoyg().s[182]++;
    return {
      value: String(shop.id),
      label: shop.name
    };
  }));
  /* istanbul ignore next */
  cov_sdv2kyoyg().s[183]++;
  const handleCreateNewProduct = async () => {
    /* istanbul ignore next */
    cov_sdv2kyoyg().f[41]++;
    cov_sdv2kyoyg().s[184]++;
    if (
    /* istanbul ignore next */
    (cov_sdv2kyoyg().b[64][0]++, !newProductData.name) ||
    /* istanbul ignore next */
    (cov_sdv2kyoyg().b[64][1]++, !newProductData.categoryId) ||
    /* istanbul ignore next */
