              } else
              /* istanbul ignore next */
              {
                cov_sdv2kyoyg().b[18][1]++;
              }
            } else {
              /* istanbul ignore next */
              cov_sdv2kyoyg().b[15][1]++;
              cov_sdv2kyoyg().s[63]++;
              if (newQty === 0) {
                /* istanbul ignore next */
                cov_sdv2kyoyg().b[20][0]++;
                cov_sdv2kyoyg().s[64]++;
