                    /* istanbul ignore next */
                    (cov_inz4lgakk().b[96][1]++, "outline"),
                    size: "sm",
                    onClick: () => {
                      /* istanbul ignore next */
                      cov_inz4lgakk().f[57]++;
                      cov_inz4lgakk().s[314]++;
                      return handlePageChange(page);
