                /* istanbul ignore next */
                (cov_1018jlj0r2().b[70][0]++, "Creating...") :
                /* istanbul ignore next */
                (cov_1018jlj0r2().b[70][1]++, "Create Customer")
              })]
            })]
          })]
        })
      }))]
    })
  });
  // Initialize product search arrays when items change
  /* istanbul ignore next */
  cov_1018jlj0r2().s[243]++;
  (0, _react.useEffect)(() => {
    /* istanbul ignore next */
    cov_1018jlj0r2().f[53]++;
    const newProductSearches =
    /* istanbul ignore next */
    (cov_1018jlj0r2().s[244]++, items.map((item, index) => {
      /* istanbul ignore next */
      cov_1018jlj0r2().f[54]++;
      const existingSearch =
      /* istanbul ignore next */
      (cov_1018jlj0r2().s[245]++, productSearches[index]);
      /* istanbul ignore next */
      cov_1018jlj0r2().s[246]++;
      if (existingSearch !== undefined) {
        /* istanbul ignore next */
        cov_1018jlj0r2().b[71][0]++;
        cov_1018jlj0r2().s[247]++;
        return existingSearch;
      } else
      /* istanbul ignore next */
      {
        cov_1018jlj0r2().b[71][1]++;
      }
      const product =
      /* istanbul ignore next */
      (cov_1018jlj0r2().s[248]++, products.find(p => {
        /* istanbul ignore next */
        cov_1018jlj0r2().f[55]++;
        cov_1018jlj0r2().s[249]++;
        return p.id === item.productId;
      }));
      /* istanbul ignore next */
      cov_1018jlj0r2().s[250]++;
      return product ?
      /* istanbul ignore next */
      (cov_1018jlj0r2().b[72][0]++, product.name) :
      /* istanbul ignore next */
      (cov_1018jlj0r2().b[72][1]++, "");
    }));
    const newShowProductDropdowns =
    /* istanbul ignore next */
    (cov_1018jlj0r2().s[251]++, items.map((_, index) => {
      /* istanbul ignore next */
      cov_1018jlj0r2().f[56]++;
      cov_1018jlj0r2().s[252]++;
      return showProductDropdowns[index] !== undefined ?
      /* istanbul ignore next */
      (cov_1018jlj0r2().b[73][0]++, showProductDropdowns[index]) :
      /* istanbul ignore next */
      (cov_1018jlj0r2().b[73][1]++, false);
    }));
    const newFilteredProducts =
    /* istanbul ignore next */
    (cov_1018jlj0r2().s[253]++, items.map(() => {
      /* istanbul ignore next */
      cov_1018jlj0r2().f[57]++;
      cov_1018jlj0r2().s[254]++;
      return products;
    }));
    /* istanbul ignore next */
    cov_1018jlj0r2().s[255]++;
    setProductSearches(newProductSearches);
    /* istanbul ignore next */
    cov_1018jlj0r2().s[256]++;
    setShowProductDropdowns(newShowProductDropdowns);
    /* istanbul ignore next */
    cov_1018jlj0r2().s[257]++;
    setFilteredProducts(newFilteredProducts);
    // Ensure refs array matches items length
    /* istanbul ignore next */
    cov_1018jlj0r2().s[258]++;
    productDropdownRefs.current = productDropdownRefs.current.slice(0, items.length);
    /* istanbul ignore next */
    cov_1018jlj0r2().s[259]++;
    while (productDropdownRefs.current.length < items.length) {
      /* istanbul ignore next */
      cov_1018jlj0r2().s[260]++;
      productDropdownRefs.current.push(null);
    }
  }, [items.length, products]);
  // Handle clicks outside to close dropdowns and window events for positioning
  /* istanbul ignore next */
  cov_1018jlj0r2().s[261]++;
  (0, _react.useEffect)(() => {
    /* istanbul ignore next */
    cov_1018jlj0r2().f[58]++;
    cov_1018jlj0r2().s[262]++;
    const handleClickOutside = event => {
      /* istanbul ignore next */
      cov_1018jlj0r2().f[59]++;
      cov_1018jlj0r2().s[263]++;
      // Check customer dropdown
      if (
      /* istanbul ignore next */
      (cov_1018jlj0r2().b[75][0]++, customerDropdownRef.current) &&
      /* istanbul ignore next */
      (cov_1018jlj0r2().b[75][1]++, !customerDropdownRef.current.contains(event.target))) {
        /* istanbul ignore next */
        cov_1018jlj0r2().b[74][0]++;
        cov_1018jlj0r2().s[264]++;
        setShowCustomerDropdown(false);
      } else
      /* istanbul ignore next */
      {
        cov_1018jlj0r2().b[74][1]++;
      }
      // Check product dropdowns
      cov_1018jlj0r2().s[265]++;
      productDropdownRefs.current.forEach((ref, index) => {
        /* istanbul ignore next */
        cov_1018jlj0r2().f[60]++;
        cov_1018jlj0r2().s[266]++;
        if (
        /* istanbul ignore next */
        (cov_1018jlj0r2().b[77][0]++, ref) &&
        /* istanbul ignore next */
        (cov_1018jlj0r2().b[77][1]++, !ref.contains(event.target))) {
          /* istanbul ignore next */
          cov_1018jlj0r2().b[76][0]++;
          const newShowProductDropdowns =
          /* istanbul ignore next */
          (cov_1018jlj0r2().s[267]++, [...showProductDropdowns]);
          /* istanbul ignore next */
          cov_1018jlj0r2().s[268]++;
          newShowProductDropdowns[index] = false;
          /* istanbul ignore next */
          cov_1018jlj0r2().s[269]++;
          setShowProductDropdowns(newShowProductDropdowns);
        } else
        /* istanbul ignore next */
        {
          cov_1018jlj0r2().b[76][1]++;
        }
      });
    };
    /* istanbul ignore next */
    cov_1018jlj0r2().s[270]++;
    const handleScroll = () => {
      /* istanbul ignore next */
      cov_1018jlj0r2().f[61]++;
      cov_1018jlj0r2().s[271]++;
      // Force re-render to update dropdown positions
      setShowProductDropdowns(prev => {
        /* istanbul ignore next */
        cov_1018jlj0r2().f[62]++;
        cov_1018jlj0r2().s[272]++;
        return [...prev];
      });
    };
    /* istanbul ignore next */
    cov_1018jlj0r2().s[273]++;
    const handleResize = () => {
      /* istanbul ignore next */
      cov_1018jlj0r2().f[63]++;
      cov_1018jlj0r2().s[274]++;
      // Force re-render to update dropdown positions
      setShowProductDropdowns(prev => {
        /* istanbul ignore next */
        cov_1018jlj0r2().f[64]++;
        cov_1018jlj0r2().s[275]++;
        return [...prev];
      });
    };
    /* istanbul ignore next */
    cov_1018jlj0r2().s[276]++;
    document.addEventListener("mousedown", handleClickOutside);
    /* istanbul ignore next */
    cov_1018jlj0r2().s[277]++;
    window.addEventListener("scroll", handleScroll);
    /* istanbul ignore next */
    cov_1018jlj0r2().s[278]++;
    window.addEventListener("resize", handleResize);
    /* istanbul ignore next */
    cov_1018jlj0r2().s[279]++;
    return () => {
      /* istanbul ignore next */
      cov_1018jlj0r2().f[65]++;
      cov_1018jlj0r2().s[280]++;
      document.removeEventListener("mousedown", handleClickOutside);
      /* istanbul ignore next */
      cov_1018jlj0r2().s[281]++;
      window.removeEventListener("scroll", handleScroll);
      /* istanbul ignore next */
      cov_1018jlj0r2().s[282]++;
      window.removeEventListener("resize", handleResize);
    };
  }, [showProductDropdowns]);
}
