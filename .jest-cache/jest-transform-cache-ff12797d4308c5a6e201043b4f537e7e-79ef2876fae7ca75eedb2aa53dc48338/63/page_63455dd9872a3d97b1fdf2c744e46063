// Helper function to get badge class for invoice status (you might have this elsewhere)
/* istanbul ignore next */
cov_1tt3el6gx9().s[75]++;
const getInvoiceStatusClass = status => {
  /* istanbul ignore next */
  cov_1tt3el6gx9().f[15]++;
  cov_1tt3el6gx9().s[76]++;
  switch (status.toLowerCase()) {
    case "paid":
      /* istanbul ignore next */
      cov_1tt3el6gx9().b[61][0]++;
      cov_1tt3el6gx9().s[77]++;
      return "bg-green-100 text-green-800";
    case "pending":
      /* istanbul ignore next */
      cov_1tt3el6gx9().b[61][1]++;
      cov_1tt3el6gx9().s[78]++;
      return "bg-yellow-100 text-yellow-800";
    case "overdue":
      /* istanbul ignore next */
      cov_1tt3el6gx9().b[61][2]++;
      cov_1tt3el6gx9().s[79]++;
      return "bg-red-100 text-red-800";
    case "draft":
      /* istanbul ignore next */
      cov_1tt3el6gx9().b[61][3]++;
      cov_1tt3el6gx9().s[80]++;
      return "bg-gray-100 text-gray-800";
    default:
      /* istanbul ignore next */
      cov_1tt3el6gx9().b[61][4]++;
      cov_1tt3el6gx9().s[81]++;
      return "bg-gray-100 text-gray-800";
  }
};
