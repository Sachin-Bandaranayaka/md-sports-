              cov_1seq0uhjo2().f[21]++;
              cov_1seq0uhjo2().s[116]++;
              return setConfirmDelete(true);
            },
            children: [/*#__PURE__*/(0, _jsxruntime.jsx)(_lucidereact.Trash2, {
              className: "w-4 h-4 mr-2"
            }), "Delete"]
          }))), /*#__PURE__*/(0, _jsxruntime.jsxs)(_Button.Button, {
            variant: "primary",
            size: "sm",
            onClick: handleDownloadPDF,
            children: [/*#__PURE__*/(0, _jsxruntime.jsx)(_lucidereact.Download, {
              className: "w-4 h-4 mr-2"
            }), "Download PDF"]
          })]
        })]
      }),
      /* istanbul ignore next */
      (cov_1seq0uhjo2().b[32][0]++, smsStatus) &&
      /* istanbul ignore next */
      (cov_1seq0uhjo2().b[32][1]++, /*#__PURE__*/(0, _jsxruntime.jsx)("div", {
        className: `mb-4 p-3 rounded-md print:hidden ${smsStatus.success ?
        /* istanbul ignore next */
        (cov_1seq0uhjo2().b[33][0]++, "bg-green-50 border border-green-200 text-green-700") :
        /* istanbul ignore next */
        (cov_1seq0uhjo2().b[33][1]++, "bg-red-50 border border-red-200 text-red-700")}`,
        children: /*#__PURE__*/(0, _jsxruntime.jsxs)("p", {
          className: "text-sm flex items-center",
          children: [smsStatus.success ?
          /* istanbul ignore next */
          (/*#__PURE__*/cov_1seq0uhjo2().b[34][0]++, (0, _jsxruntime.jsx)(_lucidereact.CheckCircle, {
            className: "w-4 h-4 mr-2"
          })) :
          /* istanbul ignore next */
          (/*#__PURE__*/cov_1seq0uhjo2().b[34][1]++, (0, _jsxruntime.jsx)(_lucidereact.AlertTriangle, {
            className: "w-4 h-4 mr-2"
          })), smsStatus.message]
        })
      })), /*#__PURE__*/(0, _jsxruntime.jsx)("div", {
        ref: printRef,
        className: "bg-white",
        children: /*#__PURE__*/(0, _jsxruntime.jsx)(_InvoiceTemplate.default, {
          invoice: invoice
        })
      })]
    })
  });
}
