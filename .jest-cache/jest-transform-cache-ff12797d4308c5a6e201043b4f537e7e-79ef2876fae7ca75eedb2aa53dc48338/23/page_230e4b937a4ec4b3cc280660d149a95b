            })]
          }), /*#__PURE__*/(0, _jsxruntime.jsxs)("div", {
            className: "flex items-center justify-end mt-8 border-t border-gray-200 pt-5",
            children: [/*#__PURE__*/(0, _jsxruntime.jsx)(_Button.Button, {
              type: "button",
              variant: "outline",
              onClick: () => {
                /* istanbul ignore next */
                cov_1sf2c1j28t().f[38]++;
                cov_1sf2c1j28t().s[99]++;
                return router.push("/settings?tab=roles");
              },
              className: "mr-3",
              disabled: isLoading,
              children: "Cancel"
            }), /*#__PURE__*/(0, _jsxruntime.jsx)(_Button.Button, {
              type: "submit",
              variant: "primary",
              disabled: isLoading,
              children: isLoading ?
              /* istanbul ignore next */
              (/*#__PURE__*/cov_1sf2c1j28t().b[15][0]++, (0, _jsxruntime.jsxs)(_jsxruntime.Fragment, {
                children: [/*#__PURE__*/(0, _jsxruntime.jsx)(_lucidereact.Loader2, {
                  className: "h-4 w-4 mr-2 animate-spin"
                }), "Saving Role..."]
              })) :
              /* istanbul ignore next */
              (cov_1sf2c1j28t().b[15][1]++, "Save Role")
            })]
          })]
        })]
      })
    })
  });
}
