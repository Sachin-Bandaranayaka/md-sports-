// Enhanced caching configuration
const CACHE_CONFIG =
/* istanbul ignore next */
(cov_8ko9wrib9().s[18]++, {
  invoices: {
    ttl: 60,
    staleWhileRevalidate: 300 // 5 minutes
  },
  statistics: {
    ttl: 300,
    staleWhileRevalidate: 900 // 15 minutes
  },
  customers: {
    ttl: 600,
    staleWhileRevalidate: 1800 // 30 minutes
  }
});
// Cursor-based pagination helper
function createCursor(id, createdAt) {
  /* istanbul ignore next */
  cov_8ko9wrib9().f[6]++;
  cov_8ko9wrib9().s[19]++;
  return Buffer.from(`${id}:${createdAt.getTime()}`).toString("base64");
}
function parseCursor(cursor) {
  /* istanbul ignore next */
  cov_8ko9wrib9().f[7]++;
  cov_8ko9wrib9().s[20]++;
  try {
    const decoded =
    /* istanbul ignore next */
    (cov_8ko9wrib9().s[21]++, Buffer.from(cursor, "base64").toString());
    const [id, timestamp] =
    /* istanbul ignore next */
    (cov_8ko9wrib9().s[22]++, decoded.split(":"));
    /* istanbul ignore next */
    cov_8ko9wrib9().s[23]++;
    return {
      id,
      createdAt: new Date(parseInt(timestamp))
    };
  } catch {
    /* istanbul ignore next */
    cov_8ko9wrib9().s[24]++;
    return null;
  }
}
// Optimized query builder
function buildWhereClause(filters, shopId) {
  /* istanbul ignore next */
  cov_8ko9wrib9().f[8]++;
  const where =
  /* istanbul ignore next */
  (cov_8ko9wrib9().s[25]++, {});
  /* istanbul ignore next */
  cov_8ko9wrib9().s[26]++;
  if (shopId) {
    /* istanbul ignore next */
    cov_8ko9wrib9().b[0][0]++;
    cov_8ko9wrib9().s[27]++;
    where.shopId = shopId;
  } else
  /* istanbul ignore next */
  {
    cov_8ko9wrib9().b[0][1]++;
  }
  cov_8ko9wrib9().s[28]++;
  if (filters.status) {
    /* istanbul ignore next */
    cov_8ko9wrib9().b[1][0]++;
    cov_8ko9wrib9().s[29]++;
    where.status = filters.status;
  } else
  /* istanbul ignore next */
  {
    cov_8ko9wrib9().b[1][1]++;
  }
  cov_8ko9wrib9().s[30]++;
  if (filters.paymentMethod) {
    /* istanbul ignore next */
    cov_8ko9wrib9().b[2][0]++;
    cov_8ko9wrib9().s[31]++;
    where.paymentMethod = filters.paymentMethod;
  } else
  /* istanbul ignore next */
  {
    cov_8ko9wrib9().b[2][1]++;
  }
  cov_8ko9wrib9().s[32]++;
  if (filters.customerId) {
    /* istanbul ignore next */
    cov_8ko9wrib9().b[3][0]++;
    cov_8ko9wrib9().s[33]++;
    where.customerId = parseInt(filters.customerId);
  } else
  /* istanbul ignore next */
  {
    cov_8ko9wrib9().b[3][1]++;
  }
  cov_8ko9wrib9().s[34]++;
  if (filters.search) {
    /* istanbul ignore next */
    cov_8ko9wrib9().b[4][0]++;
    cov_8ko9wrib9().s[35]++;
    where.OR = [{
      invoiceNumber: {
        contains: filters.search,
        mode: "insensitive"
      }
    }, {
      customer: {
        name: {
          contains: filters.search,
          mode: "insensitive"
        }
      }
    }, {
      total: {
        equals:
        /* istanbul ignore next */
        (cov_8ko9wrib9().b[5][0]++, parseFloat(filters.search)) ||
        /* istanbul ignore next */
        (cov_8ko9wrib9().b[5][1]++, undefined)
      }
    }].filter(condition => {
      /* istanbul ignore next */
      cov_8ko9wrib9().f[9]++;
      cov_8ko9wrib9().s[36]++;
      // Remove invalid conditions
      if (condition.total?.equals === undefined) {
        /* istanbul ignore next */
        cov_8ko9wrib9().b[6][0]++;
        cov_8ko9wrib9().s[37]++;
        delete condition.total;
      } else
      /* istanbul ignore next */
      {
        cov_8ko9wrib9().b[6][1]++;
      }
      cov_8ko9wrib9().s[38]++;
      return Object.keys(condition).length > 0;
    });
  } else
  /* istanbul ignore next */
  {
    cov_8ko9wrib9().b[4][1]++;
  }
  // Time period filtering
  cov_8ko9wrib9().s[39]++;
  if (
  /* istanbul ignore next */
  (cov_8ko9wrib9().b[8][0]++, filters.timePeriod) &&
  /* istanbul ignore next */
  (cov_8ko9wrib9().b[8][1]++, filters.timePeriod !== "all")) {
    /* istanbul ignore next */
    cov_8ko9wrib9().b[7][0]++;
    const now =
    /* istanbul ignore next */
    (cov_8ko9wrib9().s[40]++, new Date());
    let startDate;
    /* istanbul ignore next */
    cov_8ko9wrib9().s[41]++;
    switch (filters.timePeriod) {
      case "today":
        /* istanbul ignore next */
        cov_8ko9wrib9().b[9][0]++;
        cov_8ko9wrib9().s[42]++;
        startDate = new Date(now.getFullYear(), now.getMonth(), now.getDate());
        /* istanbul ignore next */
        cov_8ko9wrib9().s[43]++;
        break;
      case "week":
        /* istanbul ignore next */
        cov_8ko9wrib9().b[9][1]++;
        cov_8ko9wrib9().s[44]++;
        startDate = new Date(now.getTime() - 7 * 24 * 60 * 60 * 1000);
        /* istanbul ignore next */
        cov_8ko9wrib9().s[45]++;
        break;
      case "month":
        /* istanbul ignore next */
        cov_8ko9wrib9().b[9][2]++;
        cov_8ko9wrib9().s[46]++;
        startDate = new Date(now.getFullYear(), now.getMonth(), 1);
        /* istanbul ignore next */
        cov_8ko9wrib9().s[47]++;
        break;
      case "quarter":
        /* istanbul ignore next */
        cov_8ko9wrib9().b[9][3]++;
        const quarterStart =
        /* istanbul ignore next */
        (cov_8ko9wrib9().s[48]++, Math.floor(now.getMonth() / 3) * 3);
        /* istanbul ignore next */
        cov_8ko9wrib9().s[49]++;
        startDate = new Date(now.getFullYear(), quarterStart, 1);
        /* istanbul ignore next */
        cov_8ko9wrib9().s[50]++;
        break;
      case "year":
        /* istanbul ignore next */
        cov_8ko9wrib9().b[9][4]++;
        cov_8ko9wrib9().s[51]++;
        startDate = new Date(now.getFullYear(), 0, 1);
        /* istanbul ignore next */
        cov_8ko9wrib9().s[52]++;
        break;
      default:
        /* istanbul ignore next */
        cov_8ko9wrib9().b[9][5]++;
        cov_8ko9wrib9().s[53]++;
        startDate = new Date(0);
    }
    /* istanbul ignore next */
    cov_8ko9wrib9().s[54]++;
    where.createdAt = {
      gte: startDate
    };
  } else
  /* istanbul ignore next */
  {
    cov_8ko9wrib9().b[7][1]++;
  }
  cov_8ko9wrib9().s[55]++;
  return where;
}
function buildOrderBy(sortBy) {
  /* istanbul ignore next */
  cov_8ko9wrib9().f[10]++;
  cov_8ko9wrib9().s[56]++;
  switch (sortBy) {
    case "oldest":
      /* istanbul ignore next */
      cov_8ko9wrib9().b[10][0]++;
      cov_8ko9wrib9().s[57]++;
      return {
        createdAt: "asc"
      };
    case "amount-high":
      /* istanbul ignore next */
      cov_8ko9wrib9().b[10][1]++;
      cov_8ko9wrib9().s[58]++;
      return {
        total: "desc"
      };
    case "amount-low":
      /* istanbul ignore next */
      cov_8ko9wrib9().b[10][2]++;
      cov_8ko9wrib9().s[59]++;
      return {
        total: "asc"
      };
    case "customer":
      /* istanbul ignore next */
      cov_8ko9wrib9().b[10][3]++;
      cov_8ko9wrib9().s[60]++;
      return {
        customer: {
          name: "asc"
        }
      };
    case "due-date":
      /* istanbul ignore next */
      cov_8ko9wrib9().b[10][4]++;
      cov_8ko9wrib9().s[61]++;
      return {
        dueDate: "asc"
      };
    case "due-date-desc":
      /* istanbul ignore next */
      cov_8ko9wrib9().b[10][5]++;
      cov_8ko9wrib9().s[62]++;
      return {
        dueDate: "desc"
      };
    case "newest":
      /* istanbul ignore next */
      cov_8ko9wrib9().b[10][6]++;
    default:
      /* istanbul ignore next */
      cov_8ko9wrib9().b[10][7]++;
      cov_8ko9wrib9().s[63]++;
      return {
        createdAt: "desc"
      };
  }
}
// Optimized statistics query with materialized view fallback
async function getStatistics(shopId) {
  /* istanbul ignore next */
  cov_8ko9wrib9().f[11]++;
  const cacheKey =
  /* istanbul ignore next */
  (cov_8ko9wrib9().s[64]++, `invoice-statistics:${
  /* istanbul ignore next */
  (cov_8ko9wrib9().b[11][0]++, shopId) ||
  /* istanbul ignore next */
  (cov_8ko9wrib9().b[11][1]++, "all")}`);
  /* istanbul ignore next */
  cov_8ko9wrib9().s[65]++;
  return _cache.cache.getOrSet(cacheKey, async () => {
    /* istanbul ignore next */
    cov_8ko9wrib9().f[12]++;
    const whereClause =
    /* istanbul ignore next */
    (cov_8ko9wrib9().s[66]++, shopId ?
    /* istanbul ignore next */
    (cov_8ko9wrib9().b[12][0]++, {
      shopId
    }) :
    /* istanbul ignore next */
    (cov_8ko9wrib9().b[12][1]++, {}));
    // Try to use materialized view first (if exists)
    /* istanbul ignore next */
    cov_8ko9wrib9().s[67]++;
    try {
      const result =
      /* istanbul ignore next */
      (cov_8ko9wrib9().s[68]++, await _prisma.prisma.$queryRaw`
                    SELECT 
                        COALESCE(SUM(CASE WHEN LOWER(status) != 'paid' THEN total ELSE 0 END), 0) as total_outstanding,
                        COALESCE(SUM(CASE WHEN LOWER(status) = 'paid' AND DATE_TRUNC('month', "createdAt") = DATE_TRUNC('month', CURRENT_DATE) THEN total ELSE 0 END), 0) as paid_this_month,
                        COUNT(CASE WHEN status = 'overdue' THEN 1 END) as overdue_count,
                        COALESCE(SUM(CASE WHEN "paymentMethod" = 'credit' THEN total ELSE 0 END), 0) as total_credit_sales,
                        COALESCE(SUM(CASE WHEN "paymentMethod" != 'credit' THEN total ELSE 0 END), 0) as total_non_credit_sales
                    FROM "Invoice"
                    ${shopId ?
      /* istanbul ignore next */
      (cov_8ko9wrib9().b[13][0]++, `WHERE "shopId" = '${shopId}'`) :
      /* istanbul ignore next */
      (cov_8ko9wrib9().b[13][1]++, "")}
                `);
      const stats =
      /* istanbul ignore next */
      (cov_8ko9wrib9().s[69]++, result[0]);
      /* istanbul ignore next */
      cov_8ko9wrib9().s[70]++;
      return {
        totalOutstanding:
        /* istanbul ignore next */
        (cov_8ko9wrib9().b[14][0]++, parseFloat(stats.total_outstanding)) ||
        /* istanbul ignore next */
        (cov_8ko9wrib9().b[14][1]++, 0),
        paidThisMonth:
        /* istanbul ignore next */
        (cov_8ko9wrib9().b[15][0]++, parseFloat(stats.paid_this_month)) ||
        /* istanbul ignore next */
        (cov_8ko9wrib9().b[15][1]++, 0),
        overdueCount:
        /* istanbul ignore next */
        (cov_8ko9wrib9().b[16][0]++, parseInt(stats.overdue_count)) ||
        /* istanbul ignore next */
        (cov_8ko9wrib9().b[16][1]++, 0),
        totalCreditSales:
        /* istanbul ignore next */
        (cov_8ko9wrib9().b[17][0]++, parseFloat(stats.total_credit_sales)) ||
        /* istanbul ignore next */
        (cov_8ko9wrib9().b[17][1]++, 0),
        totalNonCreditSales:
        /* istanbul ignore next */
        (cov_8ko9wrib9().b[18][0]++, parseFloat(stats.total_non_credit_sales)) ||
        /* istanbul ignore next */
        (cov_8ko9wrib9().b[18][1]++, 0)
      };
    } catch (error) {
      /* istanbul ignore next */
      cov_8ko9wrib9().s[71]++;
      console.warn("Materialized view not available, falling back to aggregation queries");
      // Fallback to individual queries
      const [totalOutstanding, paidThisMonth, overdueCount, creditSales, nonCreditSales] =
      /* istanbul ignore next */
      (cov_8ko9wrib9().s[72]++, await Promise.all([_prisma.prisma.invoice.aggregate({
        where: {
          ...whereClause,
          status: {
            not: "paid"
          }
        },
        _sum: {
          total: true
        }
      }), _prisma.prisma.invoice.aggregate({
        where: {
          ...whereClause,
          status: "paid",
          createdAt: {
            gte: new Date(new Date().getFullYear(), new Date().getMonth(), 1)
          }
        },
        _sum: {
          total: true
        }
      }), _prisma.prisma.invoice.count({
        where: {
          ...whereClause,
          status: "overdue"
        }
      }), _prisma.prisma.invoice.aggregate({
        where: {
          ...whereClause,
          paymentMethod: "credit"
        },
        _sum: {
          total: true
        }
      }), _prisma.prisma.invoice.aggregate({
        where: {
          ...whereClause,
          paymentMethod: {
            not: "credit"
          }
        },
        _sum: {
          total: true
        }
      })]));
      /* istanbul ignore next */
      cov_8ko9wrib9().s[73]++;
      return {
        totalOutstanding:
        /* istanbul ignore next */
        (cov_8ko9wrib9().b[19][0]++, totalOutstanding._sum.total) ||
        /* istanbul ignore next */
        (cov_8ko9wrib9().b[19][1]++, 0),
        paidThisMonth:
        /* istanbul ignore next */
        (cov_8ko9wrib9().b[20][0]++, paidThisMonth._sum.total) ||
        /* istanbul ignore next */
        (cov_8ko9wrib9().b[20][1]++, 0),
        overdueCount,
        totalCreditSales:
        /* istanbul ignore next */
        (cov_8ko9wrib9().b[21][0]++, creditSales._sum.total) ||
        /* istanbul ignore next */
        (cov_8ko9wrib9().b[21][1]++, 0),
        totalNonCreditSales:
        /* istanbul ignore next */
        (cov_8ko9wrib9().b[22][0]++, nonCreditSales._sum.total) ||
        /* istanbul ignore next */
        (cov_8ko9wrib9().b[22][1]++, 0)
      };
    }
  }, CACHE_CONFIG.statistics.ttl);
}
// Bulk operations helper
async function bulkUpdateInvoices(invoiceIds, updates) {
  /* istanbul ignore next */
  cov_8ko9wrib9().f[13]++;
  cov_8ko9wrib9().s[74]++;
  return _prisma.prisma.invoice.updateMany({
    where: {
      id: {
        in: invoiceIds
      }
    },
    data: updates
  });
}
async function GET(request) {
  /* istanbul ignore next */
  cov_8ko9wrib9().f[14]++;
  const startTime =
  /* istanbul ignore next */
  (cov_8ko9wrib9().s[75]++, _performance.performance.now());
  /* istanbul ignore next */
  cov_8ko9wrib9().s[76]++;
  try {
    const session =
    /* istanbul ignore next */
    (cov_8ko9wrib9().s[77]++, await (0, _nextauth.getServerSession)(_auth.authOptions));
    /* istanbul ignore next */
    cov_8ko9wrib9().s[78]++;
    if (!session?.user?.id) {
      /* istanbul ignore next */
      cov_8ko9wrib9().b[23][0]++;
      cov_8ko9wrib9().s[79]++;
      return _server.NextResponse.json({
        error: "Unauthorized"
      }, {
        status: 401
      });
    } else
    /* istanbul ignore next */
    {
      cov_8ko9wrib9().b[23][1]++;
    }
    const {
      searchParams
    } =
    /* istanbul ignore next */
    (cov_8ko9wrib9().s[80]++, new URL(request.url));
    const filters =
    /* istanbul ignore next */
    (cov_8ko9wrib9().s[81]++, {
      cursor: searchParams.get("cursor"),
      limit: Math.min(parseInt(
      /* istanbul ignore next */
      (cov_8ko9wrib9().b[24][0]++, searchParams.get("limit")) ||
      /* istanbul ignore next */
      (cov_8ko9wrib9().b[24][1]++, "20")), 100),
      status: searchParams.get("status"),
      paymentMethod: searchParams.get("paymentMethod"),
      customerId: searchParams.get("customerId"),
      search: searchParams.get("search"),
      timePeriod:
      /* istanbul ignore next */
      (cov_8ko9wrib9().b[25][0]++, searchParams.get("timePeriod")) ||
      /* istanbul ignore next */
      (cov_8ko9wrib9().b[25][1]++, "all"),
      sortBy:
      /* istanbul ignore next */
      (cov_8ko9wrib9().b[26][0]++, searchParams.get("sortBy")) ||
      /* istanbul ignore next */
      (cov_8ko9wrib9().b[26][1]++, "newest"),
      includeStatistics: searchParams.get("includeStatistics") === "true"
    });
    // Get user's shop access
    const userShops =
    /* istanbul ignore next */
    (cov_8ko9wrib9().s[82]++, await _prisma.prisma.userShop.findMany({
      where: {
        userId: session.user.id
      },
      select: {
        shopId: true
      }
    }));
    const shopIds =
    /* istanbul ignore next */
    (cov_8ko9wrib9().s[83]++, userShops.map(us => {
      /* istanbul ignore next */
      cov_8ko9wrib9().f[15]++;
      cov_8ko9wrib9().s[84]++;
      return us.shopId;
    }));
    const shopId =
    /* istanbul ignore next */
    (cov_8ko9wrib9().s[85]++, searchParams.get("shopId"));
    /* istanbul ignore next */
    cov_8ko9wrib9().s[86]++;
    if (
    /* istanbul ignore next */
    (cov_8ko9wrib9().b[28][0]++, shopId) &&
    /* istanbul ignore next */
    (cov_8ko9wrib9().b[28][1]++, !shopIds.includes(shopId))) {
      /* istanbul ignore next */
      cov_8ko9wrib9().b[27][0]++;
      cov_8ko9wrib9().s[87]++;
      return _server.NextResponse.json({
        error: "Access denied to this shop"
      }, {
        status: 403
      });
    } else
    /* istanbul ignore next */
    {
      cov_8ko9wrib9().b[27][1]++;
    }
    // Build cache key
    const cacheKey =
    /* istanbul ignore next */
    (cov_8ko9wrib9().s[88]++, `invoices:${JSON.stringify({
      ...filters,
      shopIds: shopId ?
      /* istanbul ignore next */
      (cov_8ko9wrib9().b[29][0]++, [shopId]) :
      /* istanbul ignore next */
      (cov_8ko9wrib9().b[29][1]++, shopIds)
    })}`);
    // Try cache first
    const cachedResult =
    /* istanbul ignore next */
    (cov_8ko9wrib9().s[89]++, await _cache.cache.get(cacheKey));
    /* istanbul ignore next */
    cov_8ko9wrib9().s[90]++;
    if (cachedResult) {
      /* istanbul ignore next */
      cov_8ko9wrib9().b[30][0]++;
      cov_8ko9wrib9().s[91]++;
      return _server.NextResponse.json({
        ...cachedResult,
        cached: true,
        responseTime: _performance.performance.now() - startTime
      });
    } else
    /* istanbul ignore next */
    {
      cov_8ko9wrib9().b[30][1]++;
    }
    // Build query conditions
    const where =
    /* istanbul ignore next */
    (cov_8ko9wrib9().s[92]++, buildWhereClause(filters, shopId));
    /* istanbul ignore next */
    cov_8ko9wrib9().s[93]++;
    if (!shopId) {
      /* istanbul ignore next */
      cov_8ko9wrib9().b[31][0]++;
      cov_8ko9wrib9().s[94]++;
      where.shopId = {
        in: shopIds
      };
    } else
    /* istanbul ignore next */
    {
      cov_8ko9wrib9().b[31][1]++;
    }
    // Handle cursor-based pagination
    cov_8ko9wrib9().s[95]++;
    if (filters.cursor) {
      /* istanbul ignore next */
      cov_8ko9wrib9().b[32][0]++;
      const cursorData =
      /* istanbul ignore next */
      (cov_8ko9wrib9().s[96]++, parseCursor(filters.cursor));
      /* istanbul ignore next */
      cov_8ko9wrib9().s[97]++;
      if (cursorData) {
        /* istanbul ignore next */
        cov_8ko9wrib9().b[33][0]++;
        cov_8ko9wrib9().s[98]++;
        where.OR = [{
          createdAt: {
            lt: cursorData.createdAt
          }
        }, {
          createdAt: cursorData.createdAt,
          id: {
            lt: cursorData.id
          }
        }];
      } else
      /* istanbul ignore next */
      {
        cov_8ko9wrib9().b[33][1]++;
      }
    } else
    /* istanbul ignore next */
    {
      cov_8ko9wrib9().b[32][1]++;
    }
    const orderBy =
    /* istanbul ignore next */
    (cov_8ko9wrib9().s[99]++, buildOrderBy(filters.sortBy));
    // Optimized query with selective field loading
    const [invoices, totalCount, statistics] =
    /* istanbul ignore next */
    (cov_8ko9wrib9().s[100]++, await Promise.all([_prisma.prisma.invoice.findMany({
      where,
      orderBy,
      take: filters.limit + 1,
      select: {
        id: true,
        invoiceNumber: true,
        customerId: true,
        total: true,
        totalProfit: true,
        profitMargin: true,
        status: true,
        paymentMethod: true,
        createdAt: true,
        updatedAt: true,
        dueDate: true,
        customer: {
          select: {
            name: true
          }
        },
        shop: {
          select: {
            name: true
          }
        },
        payments: {
          where: {
            receipt: {
              isNot: null
            }
          },
          select: {
            amount: true
          }
        },
        _count: {
          select: {
            items: true
          }
        }
      }
    }),
    // Only count if we need pagination info
    filters.cursor ?
    /* istanbul ignore next */
    (cov_8ko9wrib9().b[34][0]++, Promise.resolve(0)) :
    /* istanbul ignore next */
    (cov_8ko9wrib9().b[34][1]++, _prisma.prisma.invoice.count({
      where
    })),
    // Only fetch statistics if requested
    filters.includeStatistics ?
    /* istanbul ignore next */
    (cov_8ko9wrib9().b[35][0]++, getStatistics(shopId)) :
    /* istanbul ignore next */
    (cov_8ko9wrib9().b[35][1]++, Promise.resolve(null))]));
    // Check if there are more results
    const hasMore =
    /* istanbul ignore next */
    (cov_8ko9wrib9().s[101]++, invoices.length > filters.limit);
    const resultInvoices =
    /* istanbul ignore next */
    (cov_8ko9wrib9().s[102]++, hasMore ?
    /* istanbul ignore next */
    (cov_8ko9wrib9().b[36][0]++, invoices.slice(0, -1)) :
    /* istanbul ignore next */
    (cov_8ko9wrib9().b[36][1]++, invoices));
    // Generate next cursor
    const nextCursor =
    /* istanbul ignore next */
    (cov_8ko9wrib9().s[103]++,
    /* istanbul ignore next */
    (cov_8ko9wrib9().b[38][0]++, hasMore) &&
    /* istanbul ignore next */
    (cov_8ko9wrib9().b[38][1]++, resultInvoices.length > 0) ?
    /* istanbul ignore next */
    (cov_8ko9wrib9().b[37][0]++, createCursor(resultInvoices[resultInvoices.length - 1].id, resultInvoices[resultInvoices.length - 1].createdAt)) :
    /* istanbul ignore next */
    (cov_8ko9wrib9().b[37][1]++, null));
    // Format response
    const formattedInvoices =
    /* istanbul ignore next */
    (cov_8ko9wrib9().s[104]++, resultInvoices.map(invoice => {
      /* istanbul ignore next */
      cov_8ko9wrib9().f[16]++;
      const totalPaid =
      /* istanbul ignore next */
      (cov_8ko9wrib9().s[105]++, invoice.payments.reduce((sum, payment) => {
        /* istanbul ignore next */
        cov_8ko9wrib9().f[17]++;
        cov_8ko9wrib9().s[106]++;
        return sum + payment.amount;
      }, 0));
      const dueAmount =
      /* istanbul ignore next */
      (cov_8ko9wrib9().s[107]++, Math.max(0, invoice.total - totalPaid));
      /* istanbul ignore next */
      cov_8ko9wrib9().s[108]++;
      return {
        ...invoice,
        customerName: invoice.customer.name,
        shopName: invoice.shop?.name,
        itemCount: invoice._count.items,
        date: invoice.createdAt.toISOString().split("T")[0],
        dueDate: invoice.dueDate?.toISOString().split("T")[0],
        totalPaid,
        dueAmount
      };
    }));
    const result =
    /* istanbul ignore next */
    (cov_8ko9wrib9().s[109]++, {
      invoices: formattedInvoices,
      hasMore,
      nextCursor,
      totalCount: filters.cursor ?
      /* istanbul ignore next */
      (cov_8ko9wrib9().b[39][0]++, undefined) :
      /* istanbul ignore next */
      (cov_8ko9wrib9().b[39][1]++, totalCount),
      statistics,
      responseTime: _performance.performance.now() - startTime
    });
    // Cache the result
    /* istanbul ignore next */
    cov_8ko9wrib9().s[110]++;
    await _cache.cache.set(cacheKey, result, CACHE_CONFIG.invoices.ttl);
    /* istanbul ignore next */
    cov_8ko9wrib9().s[111]++;
    return _server.NextResponse.json(result);
  } catch (error) {
    /* istanbul ignore next */
    cov_8ko9wrib9().s[112]++;
    console.error("Error fetching invoices:", error);
    /* istanbul ignore next */
    cov_8ko9wrib9().s[113]++;
    return _server.NextResponse.json({
      error: "Failed to fetch invoices",
      details: process.env.NODE_ENV === "development" ?
      /* istanbul ignore next */
      (cov_8ko9wrib9().b[40][0]++, error.message) :
      /* istanbul ignore next */
      (cov_8ko9wrib9().b[40][1]++, undefined),
      responseTime: _performance.performance.now() - startTime
    }, {
      status: 500
    });
  }
}
async function PATCH(request) {
  /* istanbul ignore next */
  cov_8ko9wrib9().f[18]++;
  cov_8ko9wrib9().s[114]++;
  try {
    const session =
    /* istanbul ignore next */
    (cov_8ko9wrib9().s[115]++, await (0, _nextauth.getServerSession)(_auth.authOptions));
    /* istanbul ignore next */
    cov_8ko9wrib9().s[116]++;
    if (!session?.user?.id) {
      /* istanbul ignore next */
      cov_8ko9wrib9().b[41][0]++;
      cov_8ko9wrib9().s[117]++;
      return _server.NextResponse.json({
        error: "Unauthorized"
      }, {
        status: 401
      });
    } else
    /* istanbul ignore next */
    {
      cov_8ko9wrib9().b[41][1]++;
    }
    const body =
    /* istanbul ignore next */
    (cov_8ko9wrib9().s[118]++, await request.json());
    const {
      operation,
      invoiceIds,
      data
    } =
    /* istanbul ignore next */
    (cov_8ko9wrib9().s[119]++, body);
    /* istanbul ignore next */
    cov_8ko9wrib9().s[120]++;
    if (
    /* istanbul ignore next */
    (cov_8ko9wrib9().b[43][0]++, !operation) ||
    /* istanbul ignore next */
    (cov_8ko9wrib9().b[43][1]++, !invoiceIds) ||
    /* istanbul ignore next */
    (cov_8ko9wrib9().b[43][2]++, !Array.isArray(invoiceIds))) {
      /* istanbul ignore next */
      cov_8ko9wrib9().b[42][0]++;
      cov_8ko9wrib9().s[121]++;
      return _server.NextResponse.json({
        error: "Invalid request body"
      }, {
        status: 400
      });
    } else
    /* istanbul ignore next */
    {
      cov_8ko9wrib9().b[42][1]++;
    }
    // Verify user has access to these invoices
    const userShops =
    /* istanbul ignore next */
    (cov_8ko9wrib9().s[122]++, await _prisma.prisma.userShop.findMany({
      where: {
        userId: session.user.id
      },
      select: {
        shopId: true
      }
    }));
    const shopIds =
    /* istanbul ignore next */
    (cov_8ko9wrib9().s[123]++, userShops.map(us => {
      /* istanbul ignore next */
      cov_8ko9wrib9().f[19]++;
      cov_8ko9wrib9().s[124]++;
      return us.shopId;
    }));
    const invoices =
    /* istanbul ignore next */
    (cov_8ko9wrib9().s[125]++, await _prisma.prisma.invoice.findMany({
      where: {
        id: {
          in: invoiceIds
        },
        shopId: {
          in: shopIds
        }
      },
      select: {
        id: true
      }
    }));
    /* istanbul ignore next */
    cov_8ko9wrib9().s[126]++;
    if (invoices.length !== invoiceIds.length) {
      /* istanbul ignore next */
      cov_8ko9wrib9().b[44][0]++;
      cov_8ko9wrib9().s[127]++;
      return _server.NextResponse.json({
        error: "Some invoices not found or access denied"
      }, {
        status: 403
      });
    } else
    /* istanbul ignore next */
    {
      cov_8ko9wrib9().b[44][1]++;
    }
    let result;
    /* istanbul ignore next */
    cov_8ko9wrib9().s[128]++;
    switch (operation) {
      case "bulkUpdateStatus":
        /* istanbul ignore next */
        cov_8ko9wrib9().b[45][0]++;
        cov_8ko9wrib9().s[129]++;
        result = await bulkUpdateInvoices(invoiceIds, {
          status: data.status
        });
        /* istanbul ignore next */
        cov_8ko9wrib9().s[130]++;
        break;
      case "bulkDelete":
        /* istanbul ignore next */
        cov_8ko9wrib9().b[45][1]++;
        cov_8ko9wrib9().s[131]++;
        result = await _prisma.prisma.invoice.deleteMany({
          where: {
            id: {
              in: invoiceIds
            }
          }
        });
        /* istanbul ignore next */
        cov_8ko9wrib9().s[132]++;
        break;
      default:
        /* istanbul ignore next */
        cov_8ko9wrib9().b[45][2]++;
        cov_8ko9wrib9().s[133]++;
        return _server.NextResponse.json({
          error: "Invalid operation"
        }, {
          status: 400
        });
    }
    // Invalidate related caches
    /* istanbul ignore next */
    cov_8ko9wrib9().s[134]++;
    await _cache.cache.invalidatePattern("invoices:*");
    /* istanbul ignore next */
    cov_8ko9wrib9().s[135]++;
    await _cache.cache.invalidatePattern("invoice-statistics:*");
    /* istanbul ignore next */
    cov_8ko9wrib9().s[136]++;
    return _server.NextResponse.json({
      success: true,
      affected: result.count,
      operation
    });
  } catch (error) {
    /* istanbul ignore next */
    cov_8ko9wrib9().s[137]++;
    console.error("Error in bulk operation:", error);
    /* istanbul ignore next */
    cov_8ko9wrib9().s[138]++;
    return _server.NextResponse.json({
      error: "Failed to perform bulk operation"
    }, {
      status: 500
    });
  }
}
