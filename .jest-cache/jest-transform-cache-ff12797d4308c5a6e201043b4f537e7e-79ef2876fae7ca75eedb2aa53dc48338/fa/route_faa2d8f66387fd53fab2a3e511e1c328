        if (!parent) {
          /* istanbul ignore next */
          cov_os53vclbf().b[21][0]++;
          cov_os53vclbf().s[58]++;
          break;
        } else
        /* istanbul ignore next */
        {
          cov_os53vclbf().b[21][1]++;
        }
        cov_os53vclbf().s[59]++;
        if (parent.parentId === categoryId) {
          /* istanbul ignore next */
          cov_os53vclbf().b[22][0]++;
          cov_os53vclbf().s[60]++;
          return _server.NextResponse.json({
            success: false,
            message: "Circular parent relationship detected"
          }, {
            status: 400
          });
        } else
        /* istanbul ignore next */
        {
          cov_os53vclbf().b[22][1]++;
        }
        cov_os53vclbf().s[61]++;
        currentParentId = parent.parentId;
      }
    } else
    /* istanbul ignore next */
    {
      cov_os53vclbf().b[19][1]++;
    }
    // Check if the new name already exists (excluding the current category)
    const existingCategory =
    /* istanbul ignore next */
    (cov_os53vclbf().s[62]++, await _prisma.prisma.category.findFirst({
      where: {
        name: {
          equals: name,
          mode: "insensitive"
        },
        id: {
          not: categoryId
        }
      }
    }));
    /* istanbul ignore next */
    cov_os53vclbf().s[63]++;
    if (existingCategory) {
      /* istanbul ignore next */
      cov_os53vclbf().b[23][0]++;
      cov_os53vclbf().s[64]++;
      return _server.NextResponse.json({
        success: false,
        message: "A category with this name already exists"
      }, {
        status: 400
      });
    } else
    /* istanbul ignore next */
    {
      cov_os53vclbf().b[23][1]++;
    }
    // Update the category
    const updatedCategory =
    /* istanbul ignore next */
    (cov_os53vclbf().s[65]++, await _prisma.prisma.category.update({
      where: {
        id: categoryId
      },
      data: {
        name,
        description:
        /* istanbul ignore next */
        (cov_os53vclbf().b[24][0]++, description) ||
        /* istanbul ignore next */
        (cov_os53vclbf().b[24][1]++, null),
        parentId:
        /* istanbul ignore next */
        (cov_os53vclbf().b[25][0]++, parentId) ||
        /* istanbul ignore next */
        (cov_os53vclbf().b[25][1]++, null)
      }
    }));
    // Invalidate reference data cache
    const {
      cacheService
    } =
    /* istanbul ignore next */
    (cov_os53vclbf().s[66]++, await Promise.resolve().then(() => {
      /* istanbul ignore next */
      cov_os53vclbf().f[9]++;
      cov_os53vclbf().s[67]++;
      return /*#__PURE__*/_interop_require_wildcard(require("../../../../../lib/cache"));
    }));
    /* istanbul ignore next */
    cov_os53vclbf().s[68]++;
    await cacheService.invalidateReferenceData();
    /* istanbul ignore next */
    cov_os53vclbf().s[69]++;
    return _server.NextResponse.json({
      success: true,
      message: "Category updated successfully",
      data: updatedCategory
    });
  } catch (error) {
    /* istanbul ignore next */
    cov_os53vclbf().s[70]++;
    console.error("Error updating category:", error);
    /* istanbul ignore next */
    cov_os53vclbf().s[71]++;
    return _server.NextResponse.json({
      success: false,
      message: "Failed to update category"
    }, {
      status: 500
    });
  }
}
async function DELETE(request, {
  params
}) {
  /* istanbul ignore next */
  cov_os53vclbf().f[10]++;
  cov_os53vclbf().s[72]++;
  try {
    const categoryId =
    /* istanbul ignore next */
    (cov_os53vclbf().s[73]++, parseInt(params.id));
    /* istanbul ignore next */
    cov_os53vclbf().s[74]++;
    if (isNaN(categoryId)) {
      /* istanbul ignore next */
      cov_os53vclbf().b[26][0]++;
      cov_os53vclbf().s[75]++;
      return _server.NextResponse.json({
        success: false,
        message: "Invalid category ID"
      }, {
        status: 400
      });
    } else
    /* istanbul ignore next */
    {
      cov_os53vclbf().b[26][1]++;
    }
    // Check auth header for development token
    const authHeader =
    /* istanbul ignore next */
    (cov_os53vclbf().s[76]++, request.headers.get("authorization"));
    const token =
    /* istanbul ignore next */
    (cov_os53vclbf().s[77]++, authHeader?.startsWith("Bearer ") ?
    /* istanbul ignore next */
    (cov_os53vclbf().b[27][0]++, authHeader.slice(7)) :
    /* istanbul ignore next */
    (cov_os53vclbf().b[27][1]++, null));
    /* istanbul ignore next */
    cov_os53vclbf().s[78]++;
    console.log("Delete category request:", {
      categoryId,
      hasAuthHeader: !!authHeader,
      token: token ?
      /* istanbul ignore next */
      (cov_os53vclbf().b[28][0]++, `${token.substring(0, 10)}...`) :
      /* istanbul ignore next */
      (cov_os53vclbf().b[28][1]++, null)
    });
    // Get user ID for audit logging
    let userId =
    /* istanbul ignore next */
    (cov_os53vclbf().s[79]++, null);
    /* istanbul ignore next */
    cov_os53vclbf().s[80]++;
    if (token !== "dev-token") {
      /* istanbul ignore next */
      cov_os53vclbf().b[29][0]++;
      // Verify permission
      const hasPermission =
      /* istanbul ignore next */
      (cov_os53vclbf().s[81]++, await (0, _auth.validateTokenPermission)(request, "category:delete"));
      /* istanbul ignore next */
      cov_os53vclbf().s[82]++;
      if (!hasPermission) {
        /* istanbul ignore next */
        cov_os53vclbf().b[30][0]++;
        cov_os53vclbf().s[83]++;
        return _server.NextResponse.json({
          success: false,
          message: "Unauthorized: Insufficient permissions"
        }, {
          status: 403
        });
      } else
      /* istanbul ignore next */
      {
        cov_os53vclbf().b[30][1]++;
      }
      // Get user ID from token
      const decoded =
      /* istanbul ignore next */
      (cov_os53vclbf().s[84]++, await (0, _auth.verifyToken)(token));
      /* istanbul ignore next */
      cov_os53vclbf().s[85]++;
      if (decoded) {
        /* istanbul ignore next */
        cov_os53vclbf().b[31][0]++;
        cov_os53vclbf().s[86]++;
        userId = decoded.userId;
      } else
      /* istanbul ignore next */
      {
        cov_os53vclbf().b[31][1]++;
      }
    } else {
      /* istanbul ignore next */
      cov_os53vclbf().b[29][1]++;
      cov_os53vclbf().s[87]++;
      console.log("Using development token - bypassing permission check");
      /* istanbul ignore next */
      cov_os53vclbf().s[88]++;
      userId = 1; // Default user for dev token
    }
    // Check if category exists
    const category =
    /* istanbul ignore next */
    (cov_os53vclbf().s[89]++, await _prisma.prisma.category.findUnique({
      where: {
        id: categoryId
      },
      include: {
        children: true,
        products: true
      }
    }));
    /* istanbul ignore next */
    cov_os53vclbf().s[90]++;
    if (!category) {
      /* istanbul ignore next */
      cov_os53vclbf().b[32][0]++;
      cov_os53vclbf().s[91]++;
      return _server.NextResponse.json({
        success: false,
        message: "Category not found"
      }, {
        status: 404
      });
    } else
    /* istanbul ignore next */
    {
      cov_os53vclbf().b[32][1]++;
    }
    // Check if category has child categories
    cov_os53vclbf().s[92]++;
    if (
    /* istanbul ignore next */
    (cov_os53vclbf().b[34][0]++, category.children) &&
    /* istanbul ignore next */
    (cov_os53vclbf().b[34][1]++, category.children.length > 0)) {
      /* istanbul ignore next */
      cov_os53vclbf().b[33][0]++;
      cov_os53vclbf().s[93]++;
      return _server.NextResponse.json({
        success: false,
        message: "Cannot delete category with subcategories. Please delete subcategories first."
      }, {
        status: 400
      });
    } else
    /* istanbul ignore next */
    {
      cov_os53vclbf().b[33][1]++;
    }
    // Check if category is used by any products
    cov_os53vclbf().s[94]++;
    if (
    /* istanbul ignore next */
    (cov_os53vclbf().b[36][0]++, category.products) &&
    /* istanbul ignore next */
    (cov_os53vclbf().b[36][1]++, category.products.length > 0)) {
      /* istanbul ignore next */
      cov_os53vclbf().b[35][0]++;
      cov_os53vclbf().s[95]++;
      return _server.NextResponse.json({
        success: false,
        message: "Cannot delete category that is assigned to products. Please reassign products first."
      }, {
        status: 400
      });
    } else
    /* istanbul ignore next */
    {
      cov_os53vclbf().b[35][1]++;
    }
    // Use audit service for soft delete
    cov_os53vclbf().s[96]++;
    if (userId) {
      /* istanbul ignore next */
      cov_os53vclbf().b[37][0]++;
      const auditService =
      /* istanbul ignore next */
      (cov_os53vclbf().s[97]++, new _auditService.AuditService());
      /* istanbul ignore next */
      cov_os53vclbf().s[98]++;
      await auditService.softDelete("Category", categoryId, userId, category, true // canRecover
      );
    } else {
      /* istanbul ignore next */
      cov_os53vclbf().b[37][1]++;
      cov_os53vclbf().s[99]++;
      // Fallback to hard delete if no user ID available
      await _prisma.prisma.category.delete({
        where: {
          id: categoryId
        }
      });
    }
    // Invalidate reference data cache
    const {
      cacheService
    } =
    /* istanbul ignore next */
    (cov_os53vclbf().s[100]++, await Promise.resolve().then(() => {
      /* istanbul ignore next */
      cov_os53vclbf().f[11]++;
      cov_os53vclbf().s[101]++;
      return /*#__PURE__*/_interop_require_wildcard(require("../../../../../lib/cache"));
    }));
    /* istanbul ignore next */
    cov_os53vclbf().s[102]++;
    await cacheService.invalidateReferenceData();
    /* istanbul ignore next */
    cov_os53vclbf().s[103]++;
    return _server.NextResponse.json({
      success: true,
      message: userId ?
      /* istanbul ignore next */
      (cov_os53vclbf().b[38][0]++, "Category moved to recycle bin successfully") :
      /* istanbul ignore next */
      (cov_os53vclbf().b[38][1]++, "Category deleted successfully")
    });
  } catch (error) {
    /* istanbul ignore next */
    cov_os53vclbf().s[104]++;
    console.error("Error deleting category:", error);
    /* istanbul ignore next */
    cov_os53vclbf().s[105]++;
    return _server.NextResponse.json({
      success: false,
      message: "Failed to delete category"
    }, {
      status: 500
    });
  }
}
async function GET(request, {
  params
}) {
  /* istanbul ignore next */
  cov_os53vclbf().f[12]++;
  cov_os53vclbf().s[106]++;
  try {
    const categoryId =
    /* istanbul ignore next */
    (cov_os53vclbf().s[107]++, parseInt(params.id));
    /* istanbul ignore next */
    cov_os53vclbf().s[108]++;
    if (isNaN(categoryId)) {
      /* istanbul ignore next */
      cov_os53vclbf().b[39][0]++;
      cov_os53vclbf().s[109]++;
      return _server.NextResponse.json({
        success: false,
        message: "Invalid category ID"
      }, {
        status: 400
      });
    } else
    /* istanbul ignore next */
    {
      cov_os53vclbf().b[39][1]++;
    }
    // Get the category
    const category =
    /* istanbul ignore next */
    (cov_os53vclbf().s[110]++, await _prisma.prisma.category.findUnique({
      where: {
        id: categoryId
      }
    }));
    /* istanbul ignore next */
    cov_os53vclbf().s[111]++;
    if (!category) {
      /* istanbul ignore next */
      cov_os53vclbf().b[40][0]++;
      cov_os53vclbf().s[112]++;
      return _server.NextResponse.json({
        success: false,
        message: "Category not found"
      }, {
        status: 404
      });
    } else
    /* istanbul ignore next */
    {
      cov_os53vclbf().b[40][1]++;
    }
    // If there's a parent ID, get the parent name
    let parentName =
    /* istanbul ignore next */
    (cov_os53vclbf().s[113]++, null);
    /* istanbul ignore next */
    cov_os53vclbf().s[114]++;
    if (category.parentId) {
      /* istanbul ignore next */
      cov_os53vclbf().b[41][0]++;
      const parentCategory =
      /* istanbul ignore next */
      (cov_os53vclbf().s[115]++, await _prisma.prisma.category.findUnique({
        where: {
          id: category.parentId
        }
      }));
      /* istanbul ignore next */
      cov_os53vclbf().s[116]++;
      parentName = parentCategory?.name;
    } else
    /* istanbul ignore next */
    {
      cov_os53vclbf().b[41][1]++;
    }
    cov_os53vclbf().s[117]++;
    return _server.NextResponse.json({
      success: true,
      data: {
        id: category.id,
        name: category.name,
        description: category.description,
        parent_id: category.parentId,
        parent_name: parentName
      }
    });
  } catch (error) {
    /* istanbul ignore next */
    cov_os53vclbf().s[118]++;
    console.error("Error fetching category:", error);
    /* istanbul ignore next */
    cov_os53vclbf().s[119]++;
    return _server.NextResponse.json({
      success: false,
      message: "Failed to fetch category"
    }, {
      status: 500
    });
  }
}
async function PATCH(request, {
  params
}) {
  /* istanbul ignore next */
  cov_os53vclbf().f[13]++;
  cov_os53vclbf().s[120]++;
  try {
    const categoryId =
    /* istanbul ignore next */
    (cov_os53vclbf().s[121]++, parseInt(params.id));
    /* istanbul ignore next */
    cov_os53vclbf().s[122]++;
    if (isNaN(categoryId)) {
      /* istanbul ignore next */
      cov_os53vclbf().b[42][0]++;
      cov_os53vclbf().s[123]++;
      return _server.NextResponse.json({
        success: false,
        message: "Invalid category ID"
      }, {
        status: 400
      });
    } else
    /* istanbul ignore next */
    {
      cov_os53vclbf().b[42][1]++;
    }
    // Verify permission
    const hasPermission =
    /* istanbul ignore next */
    (cov_os53vclbf().s[124]++, await (0, _auth.validateTokenPermission)(request, "category:update"));
    /* istanbul ignore next */
    cov_os53vclbf().s[125]++;
    if (!hasPermission) {
      /* istanbul ignore next */
      cov_os53vclbf().b[43][0]++;
      cov_os53vclbf().s[126]++;
      return _server.NextResponse.json({
        success: false,
        message: "Unauthorized: Insufficient permissions"
      }, {
        status: 403
      });
    } else
    /* istanbul ignore next */
    {
      cov_os53vclbf().b[43][1]++;
    }
    // Parse request body
    const data =
    /* istanbul ignore next */
    (cov_os53vclbf().s[127]++, await request.json());
    const {
      name,
      description,
      parentId
    } =
    /* istanbul ignore next */
    (cov_os53vclbf().s[128]++, data);
    // Check if category exists
    const category =
    /* istanbul ignore next */
    (cov_os53vclbf().s[129]++, await _prisma.prisma.category.findUnique({
      where: {
        id: categoryId
      },
      include: {
        children: true,
        products: true
      }
    }));
    /* istanbul ignore next */
    cov_os53vclbf().s[130]++;
    if (!category) {
      /* istanbul ignore next */
      cov_os53vclbf().b[44][0]++;
      cov_os53vclbf().s[131]++;
      return _server.NextResponse.json({
        success: false,
        message: "Category not found"
      }, {
        status: 404
      });
    } else
    /* istanbul ignore next */
    {
      cov_os53vclbf().b[44][1]++;
    }
    // If name is being changed, check for duplicates
    cov_os53vclbf().s[132]++;
    if (
    /* istanbul ignore next */
    (cov_os53vclbf().b[46][0]++, name) &&
    /* istanbul ignore next */
    (cov_os53vclbf().b[46][1]++, name !== category.name)) {
      /* istanbul ignore next */
      cov_os53vclbf().b[45][0]++;
      const existingCategory =
      /* istanbul ignore next */
      (cov_os53vclbf().s[133]++, await _prisma.prisma.category.findFirst({
        where: {
          name: {
            equals: name,
            mode: "insensitive"
          },
          id: {
            not: categoryId
          }
        }
      }));
      /* istanbul ignore next */
      cov_os53vclbf().s[134]++;
      if (existingCategory) {
        /* istanbul ignore next */
        cov_os53vclbf().b[47][0]++;
        cov_os53vclbf().s[135]++;
        return _server.NextResponse.json({
          success: false,
          message: "A category with this name already exists"
        }, {
          status: 400
        });
      } else
      /* istanbul ignore next */
      {
        cov_os53vclbf().b[47][1]++;
      }
    } else
    /* istanbul ignore next */
    {
      cov_os53vclbf().b[45][1]++;
    }
    // Check for circular reference if changing parent
    cov_os53vclbf().s[136]++;
    if (
    /* istanbul ignore next */
    (cov_os53vclbf().b[49][0]++, parentId) &&
    /* istanbul ignore next */
    (cov_os53vclbf().b[49][1]++, parentId !== category.parentId)) {
      /* istanbul ignore next */
      cov_os53vclbf().b[48][0]++;
      cov_os53vclbf().s[137]++;
      // Can't make itself its own parent
      if (parentId === categoryId) {
        /* istanbul ignore next */
        cov_os53vclbf().b[50][0]++;
        cov_os53vclbf().s[138]++;
        return _server.NextResponse.json({
          success: false,
          message: "Category cannot be its own parent"
        }, {
          status: 400
        });
      } else
      /* istanbul ignore next */
      {
        cov_os53vclbf().b[50][1]++;
      }
      // Check if the new parent is a descendant of this category (would create a loop)
      let checkId =
      /* istanbul ignore next */
      (cov_os53vclbf().s[139]++, parentId);
      /* istanbul ignore next */
      cov_os53vclbf().s[140]++;
      while (checkId) {
        /* istanbul ignore next */
        cov_os53vclbf().s[141]++;
        if (checkId === categoryId) {
          /* istanbul ignore next */
          cov_os53vclbf().b[51][0]++;
          cov_os53vclbf().s[142]++;
          return _server.NextResponse.json({
            success: false,
            message: "Cannot create circular hierarchy"
          }, {
            status: 400
          });
        } else
        /* istanbul ignore next */
        {
          cov_os53vclbf().b[51][1]++;
        }
        const parentCategory =
        /* istanbul ignore next */
        (cov_os53vclbf().s[143]++, await _prisma.prisma.category.findUnique({
          where: {
            id: checkId
          }
        }));
        /* istanbul ignore next */
        cov_os53vclbf().s[144]++;
        checkId =
        /* istanbul ignore next */
        (cov_os53vclbf().b[52][0]++, parentCategory?.parentId) ||
        /* istanbul ignore next */
        (cov_os53vclbf().b[52][1]++, null);
      }
    } else
    /* istanbul ignore next */
    {
      cov_os53vclbf().b[48][1]++;
    }
    // Update the category
    const updatedCategory =
    /* istanbul ignore next */
    (cov_os53vclbf().s[145]++, await _prisma.prisma.category.update({
      where: {
        id: categoryId
      },
      data: {
        name:
        /* istanbul ignore next */
        (cov_os53vclbf().b[53][0]++, name) ||
        /* istanbul ignore next */
        (cov_os53vclbf().b[53][1]++, undefined),
        description: description !== undefined ?
        /* istanbul ignore next */
        (cov_os53vclbf().b[54][0]++, description) :
        /* istanbul ignore next */
        (cov_os53vclbf().b[54][1]++, undefined),
        parentId: parentId !== undefined ?
        /* istanbul ignore next */
        (cov_os53vclbf().b[55][0]++,
        /* istanbul ignore next */
        (cov_os53vclbf().b[56][0]++, parentId) ||
        /* istanbul ignore next */
        (cov_os53vclbf().b[56][1]++, null)) :
        /* istanbul ignore next */
        (cov_os53vclbf().b[55][1]++, undefined)
      }
    }));
    /* istanbul ignore next */
    cov_os53vclbf().s[146]++;
    return _server.NextResponse.json({
      success: true,
      message: "Category updated successfully",
      data: updatedCategory
    });
  } catch (error) {
    /* istanbul ignore next */
    cov_os53vclbf().s[147]++;
    console.error("Error updating category:", error);
    /* istanbul ignore next */
    cov_os53vclbf().s[148]++;
    return _server.NextResponse.json({
      success: false,
      message: "Failed to update category"
    }, {
      status: 500
    });
  }
}
