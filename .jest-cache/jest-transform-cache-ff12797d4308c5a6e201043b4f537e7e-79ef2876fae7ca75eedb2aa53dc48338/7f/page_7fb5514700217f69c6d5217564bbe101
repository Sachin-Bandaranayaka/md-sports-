        (cov_qig5kn2rc().s[61]++, await fetch(`/api/invoices/${params.id}`));
        /* istanbul ignore next */
        cov_qig5kn2rc().s[62]++;
        if (!response.ok) {
          /* istanbul ignore next */
          cov_qig5kn2rc().b[12][0]++;
          cov_qig5kn2rc().s[63]++;
          throw new Error("Failed to fetch invoice");
        } else
        /* istanbul ignore next */
        {
          cov_qig5kn2rc().b[12][1]++;
        }
        const invoiceData =
        /* istanbul ignore next */
        (cov_qig5kn2rc().s[64]++, await response.json());
        // Set invoice number
        /* istanbul ignore next */
        cov_qig5kn2rc().s[65]++;
        setInvoiceNumber(invoiceData.invoiceNumber);
        // Transform API data to form data
        const items =
        /* istanbul ignore next */
        (cov_qig5kn2rc().s[66]++, invoiceData.items.map(item => {
          /* istanbul ignore next */
          cov_qig5kn2rc().f[13]++;
          cov_qig5kn2rc().s[67]++;
          return {
            id: item.id,
            productId: item.productId,
            productName: item.product.name,
            quantity: item.quantity,
            price: item.price,
            costPrice:
            /* istanbul ignore next */
            (cov_qig5kn2rc().b[13][0]++, item.costPrice) ||
            /* istanbul ignore next */
            (cov_qig5kn2rc().b[13][1]++, 0),
            total: item.total,
            profit:
            /* istanbul ignore next */
            (cov_qig5kn2rc().b[14][0]++, item.profit) ||
            /* istanbul ignore next */
            (cov_qig5kn2rc().b[14][1]++, 0) // Populate from API
          };
        }));
        // Calculate due date (30 days from creation date or based on customer credit period)
        const createdDate =
        /* istanbul ignore next */
        (cov_qig5kn2rc().s[68]++, new Date(invoiceData.createdAt));
        const dueDate =
        /* istanbul ignore next */
        (cov_qig5kn2rc().s[69]++, new Date(createdDate));
        /* istanbul ignore next */
        cov_qig5kn2rc().s[70]++;
        dueDate.setDate(dueDate.getDate() + 30); // Default to 30 days
        /* istanbul ignore next */
        cov_qig5kn2rc().s[71]++;
        setFormData({
          customerId: invoiceData.customerId,
          customerName: invoiceData.customer.name,
          invoiceDate: createdDate.toISOString().split("T")[0],
          dueDate: dueDate.toISOString().split("T")[0],
          notes: "",
          status: invoiceData.status,
          paymentMethod: invoiceData.paymentMethod,
          shopId:
          /* istanbul ignore next */
          (cov_qig5kn2rc().b[15][0]++, invoiceData.shopId) ||
          /* istanbul ignore next */
          (cov_qig5kn2rc().b[15][1]++, null),
          items,
          discountType:
          /* istanbul ignore next */
          (cov_qig5kn2rc().b[16][0]++, invoiceData.discountType) ||
          /* istanbul ignore next */
          (cov_qig5kn2rc().b[16][1]++, "amount"),
          discountValue:
          /* istanbul ignore next */
          (cov_qig5kn2rc().b[17][0]++, invoiceData.discountValue) ||
          /* istanbul ignore next */
          (cov_qig5kn2rc().b[17][1]++, 0)
        });
        // Ensure we have customer type and credit limit
        const customerWithDetails =
        /* istanbul ignore next */
        (cov_qig5kn2rc().s[72]++, invoiceData.customer);
        /* istanbul ignore next */
        cov_qig5kn2rc().s[73]++;
        if (
        /* istanbul ignore next */
        (cov_qig5kn2rc().b[19][0]++, !customerWithDetails.customerType) ||
        /* istanbul ignore next */
        (cov_qig5kn2rc().b[19][1]++, customerWithDetails.creditLimit === undefined)) {
          /* istanbul ignore next */
          cov_qig5kn2rc().b[18][0]++;
          cov_qig5kn2rc().s[74]++;
          // Fetch full customer details
          try {
            const customerResponse =
            /* istanbul ignore next */
            (cov_qig5kn2rc().s[75]++, await fetch(`/api/customers/${invoiceData.customerId}`));
            /* istanbul ignore next */
            cov_qig5kn2rc().s[76]++;
            if (customerResponse.ok) {
              /* istanbul ignore next */
              cov_qig5kn2rc().b[20][0]++;
              const fullCustomerData =
              /* istanbul ignore next */
              (cov_qig5kn2rc().s[77]++, await customerResponse.json());
              /* istanbul ignore next */
              cov_qig5kn2rc().s[78]++;
              customerWithDetails.customerType = fullCustomerData.customerType;
              /* istanbul ignore next */
              cov_qig5kn2rc().s[79]++;
              customerWithDetails.creditLimit = fullCustomerData.creditLimit;
            } else
            /* istanbul ignore next */
            {
              cov_qig5kn2rc().b[20][1]++;
            }
          } catch (error) {
            /* istanbul ignore next */
            cov_qig5kn2rc().s[80]++;
            console.error("Error fetching full customer details:", error);
          }
        } else
        /* istanbul ignore next */
        {
          cov_qig5kn2rc().b[18][1]++;
        }
        cov_qig5kn2rc().s[81]++;
        setSelectedCustomer(customerWithDetails);
      } catch (error) {
        /* istanbul ignore next */
        cov_qig5kn2rc().s[82]++;
        console.error("Error fetching invoice:", error);
        /* istanbul ignore next */
        cov_qig5kn2rc().s[83]++;
        alert("Failed to load invoice data. Please try again.");
        /* istanbul ignore next */
        cov_qig5kn2rc().s[84]++;
        router.push("/invoices");
      } finally {
        /* istanbul ignore next */
        cov_qig5kn2rc().s[85]++;
        setIsLoading(false);
      }
    };
    // Fetch customers and products
    /* istanbul ignore next */
    cov_qig5kn2rc().s[86]++;
    const fetchMasterData = async () => {
      /* istanbul ignore next */
      cov_qig5kn2rc().f[14]++;
      cov_qig5kn2rc().s[87]++;
      try {
        // Fetch customers
        const customerResponse =
        /* istanbul ignore next */
        (cov_qig5kn2rc().s[88]++, await fetch("/api/customers"));
        /* istanbul ignore next */
        cov_qig5kn2rc().s[89]++;
        if (customerResponse.ok) {
          /* istanbul ignore next */
          cov_qig5kn2rc().b[21][0]++;
          const customerData =
          /* istanbul ignore next */
          (cov_qig5kn2rc().s[90]++, await customerResponse.json());
          // Handle both direct array and wrapped object formats
          /* istanbul ignore next */
          cov_qig5kn2rc().s[91]++;
          setCustomers(Array.isArray(customerData) ?
          /* istanbul ignore next */
          (cov_qig5kn2rc().b[22][0]++, customerData) :
          /* istanbul ignore next */
          (cov_qig5kn2rc().b[22][1]++,
          /* istanbul ignore next */
          (cov_qig5kn2rc().b[24][0]++, customerData.data) &&
          /* istanbul ignore next */
          (cov_qig5kn2rc().b[24][1]++, Array.isArray(customerData.data)) ?
          /* istanbul ignore next */
          (cov_qig5kn2rc().b[23][0]++, customerData.data) :
          /* istanbul ignore next */
          (cov_qig5kn2rc().b[23][1]++, [])));
        } else
        /* istanbul ignore next */
        {
          cov_qig5kn2rc().b[21][1]++;
        }
        // Fetch products
        const productResponse =
        /* istanbul ignore next */
        (cov_qig5kn2rc().s[92]++, await fetch("/api/products?limit=10000"));
        /* istanbul ignore next */
        cov_qig5kn2rc().s[93]++;
        if (productResponse.ok) {
          /* istanbul ignore next */
          cov_qig5kn2rc().b[25][0]++;
          const productData =
          /* istanbul ignore next */
          (cov_qig5kn2rc().s[94]++, await productResponse.json());
          /* istanbul ignore next */
          cov_qig5kn2rc().s[95]++;
          setProducts(Array.isArray(productData.data) ?
          /* istanbul ignore next */
          (cov_qig5kn2rc().b[26][0]++, productData.data) :
          /* istanbul ignore next */
          (cov_qig5kn2rc().b[26][1]++, []));
        } else {
          /* istanbul ignore next */
          cov_qig5kn2rc().b[25][1]++;
          cov_qig5kn2rc().s[96]++;
          // If API not available, set some sample products
          setProducts([{
            id: 1,
            name: "Cricket Bat",
            price: 12500,
            description: "Professional cricket bat",
            sku: "CB-001"
          }, {
            id: 2,
            name: "Cricket Ball",
            price: 1800,
            description: "Match quality cricket ball",
            sku: "CBL-002"
          }, {
            id: 3,
            name: "Basketball",
            price: 4500,
            description: "Official size basketball",
            sku: "BB-003"
          }, {
            id: 4,
            name: "Football",
            price: 3200,
            description: "Professional football",
            sku: "FB-004"
          }, {
            id: 5,
            name: "Tennis Racket",
            price: 8500,
            description: "Professional tennis racket",
            sku: "TR-005"
          }]);
        }
        // Fetch shops
        const shopResponse =
        /* istanbul ignore next */
        (cov_qig5kn2rc().s[97]++, await fetch("/api/shops?simple=true"));
        /* istanbul ignore next */
        cov_qig5kn2rc().s[98]++;
        if (shopResponse.ok) {
          /* istanbul ignore next */
          cov_qig5kn2rc().b[27][0]++;
          const shopData =
          /* istanbul ignore next */
          (cov_qig5kn2rc().s[99]++, await shopResponse.json());
          /* istanbul ignore next */
          cov_qig5kn2rc().s[100]++;
          setShops(Array.isArray(shopData.data) ?
          /* istanbul ignore next */
          (cov_qig5kn2rc().b[28][0]++, shopData.data) :
          /* istanbul ignore next */
          (cov_qig5kn2rc().b[28][1]++, []));
        } else
        /* istanbul ignore next */
        {
          cov_qig5kn2rc().b[27][1]++;
        }
      } catch (error) {
        /* istanbul ignore next */
        cov_qig5kn2rc().s[101]++;
        console.error("Error fetching master data:", error);
        // Set sample data if API fails
        /* istanbul ignore next */
        cov_qig5kn2rc().s[102]++;
        setCustomers([{
          id: 1,
          name: "Colombo Cricket Club",
          email: "info@colombocricket.lk",
          phone: "+94 112 345 123",
          paymentType: "Credit",
          creditPeriod: 30
        }, {
          id: 2,
          name: "Kandy Sports Association",
          email: "info@kandysports.lk",
          phone: "+94 812 345 456",
          paymentType: "Cash",
          creditPeriod: null
        }, {
          id: 3,
          name: "Galle School District",
          email: "sports@galleschools.lk",
          phone: "+94 912 345 789",
          paymentType: "Credit",
          creditPeriod: 15
        }]);
      }
    };
    /* istanbul ignore next */
    cov_qig5kn2rc().s[103]++;
    fetchInvoiceData();
    /* istanbul ignore next */
    cov_qig5kn2rc().s[104]++;
    fetchMasterData();
  }, [params.id, router]);
  // Filter customers based on search term
  const filteredCustomers =
  /* istanbul ignore next */
  (cov_qig5kn2rc().s[105]++, (0, _react.useMemo)(() => {
    /* istanbul ignore next */
    cov_qig5kn2rc().f[15]++;
    cov_qig5kn2rc().s[106]++;
    if (!customerSearch) {
      /* istanbul ignore next */
      cov_qig5kn2rc().b[29][0]++;
      cov_qig5kn2rc().s[107]++;
      return customers;
    } else
    /* istanbul ignore next */
    {
      cov_qig5kn2rc().b[29][1]++;
    }
    cov_qig5kn2rc().s[108]++;
    if (!Array.isArray(customers)) {
      /* istanbul ignore next */
      cov_qig5kn2rc().b[30][0]++;
      cov_qig5kn2rc().s[109]++;
      return [];
    } else
    /* istanbul ignore next */
    {
      cov_qig5kn2rc().b[30][1]++;
    }
    cov_qig5kn2rc().s[110]++;
    return customers.filter(customer => {
      /* istanbul ignore next */
      cov_qig5kn2rc().f[16]++;
      cov_qig5kn2rc().s[111]++;
      return /* istanbul ignore next */(cov_qig5kn2rc().b[31][0]++, customer.name.toLowerCase().includes(customerSearch.toLowerCase())) ||
      /* istanbul ignore next */
      (cov_qig5kn2rc().b[31][1]++, customer.email) &&
      /* istanbul ignore next */
      (cov_qig5kn2rc().b[31][2]++, customer.email.toLowerCase().includes(customerSearch.toLowerCase())) ||
      /* istanbul ignore next */
      (cov_qig5kn2rc().b[31][3]++, customer.phone) &&
      /* istanbul ignore next */
      (cov_qig5kn2rc().b[31][4]++, customer.phone.includes(customerSearch));
    });
  }, [customers, customerSearch]));
  // Filter products based on search term - enhanced search with multiple words support
  const filteredProducts =
  /* istanbul ignore next */
  (cov_qig5kn2rc().s[112]++, (0, _react.useMemo)(() => {
    /* istanbul ignore next */
    cov_qig5kn2rc().f[17]++;
    cov_qig5kn2rc().s[113]++;
    if (!productSearch) {
      /* istanbul ignore next */
      cov_qig5kn2rc().b[32][0]++;
      cov_qig5kn2rc().s[114]++;
      return products;
    } else
    /* istanbul ignore next */
    {
      cov_qig5kn2rc().b[32][1]++;
    }
    cov_qig5kn2rc().s[115]++;
    if (!Array.isArray(products)) {
      /* istanbul ignore next */
      cov_qig5kn2rc().b[33][0]++;
      cov_qig5kn2rc().s[116]++;
      return [];
    } else
    /* istanbul ignore next */
    {
      cov_qig5kn2rc().b[33][1]++;
    }
    // Enhanced search - supports multiple words in any order
    const searchWords =
    /* istanbul ignore next */
    (cov_qig5kn2rc().s[117]++, productSearch.toLowerCase().trim().split(/\s+/));
    /* istanbul ignore next */
    cov_qig5kn2rc().s[118]++;
    return products.filter(product => {
      /* istanbul ignore next */
      cov_qig5kn2rc().f[18]++;
      const productName =
      /* istanbul ignore next */
      (cov_qig5kn2rc().s[119]++, product.name.toLowerCase());
      const productSku =
      /* istanbul ignore next */
      (cov_qig5kn2rc().s[120]++, (
      /* istanbul ignore next */
      (cov_qig5kn2rc().b[34][0]++, product.sku) ||
      /* istanbul ignore next */
      (cov_qig5kn2rc().b[34][1]++, "")).toLowerCase());
      // Each word must appear somewhere in the product name or SKU
      /* istanbul ignore next */
      cov_qig5kn2rc().s[121]++;
      return searchWords.every(word => {
        /* istanbul ignore next */
        cov_qig5kn2rc().f[19]++;
        cov_qig5kn2rc().s[122]++;
        return /* istanbul ignore next */(cov_qig5kn2rc().b[35][0]++, productName.includes(word)) ||
        /* istanbul ignore next */
        (cov_qig5kn2rc().b[35][1]++, productSku.includes(word));
      });
    });
  }, [products, productSearch]));
  // Handle form input changes
  /* istanbul ignore next */
  cov_qig5kn2rc().s[123]++;
  const handleInputChange = e => {
    /* istanbul ignore next */
    cov_qig5kn2rc().f[20]++;
    const {
      name,
      value
    } =
    /* istanbul ignore next */
    (cov_qig5kn2rc().s[124]++, e.target);
    /* istanbul ignore next */
    cov_qig5kn2rc().s[125]++;
    setFormData({
      ...formData,
      [name]: name === "discountValue" ?
      /* istanbul ignore next */
      (cov_qig5kn2rc().b[36][0]++,
      /* istanbul ignore next */
      (cov_qig5kn2rc().b[37][0]++, parseFloat(value)) ||
      /* istanbul ignore next */
      (cov_qig5kn2rc().b[37][1]++, 0)) :
      /* istanbul ignore next */
      (cov_qig5kn2rc().b[36][1]++, value)
    });
  };
  // Handle customer selection
  /* istanbul ignore next */
  cov_qig5kn2rc().s[126]++;
  const handleSelectCustomer = async customer => {
    /* istanbul ignore next */
    cov_qig5kn2rc().f[21]++;
    cov_qig5kn2rc().s[127]++;
    setSelectedCustomer(customer);
    // Get today's date
    const today =
    /* istanbul ignore next */
    (cov_qig5kn2rc().s[128]++, new Date());
    const invoiceDate =
    /* istanbul ignore next */
    (cov_qig5kn2rc().s[129]++, today.toISOString().split("T")[0]);
    // Calculate due date based on customer's credit period
    let dueDate =
    /* istanbul ignore next */
    (cov_qig5kn2rc().s[130]++, invoiceDate);
    let status =
    /* istanbul ignore next */
    (cov_qig5kn2rc().s[131]++, "paid"); // Default for cash customers
    // If customer has a credit period, calculate due date and set status to pending
    /* istanbul ignore next */
    cov_qig5kn2rc().s[132]++;
    if (
    /* istanbul ignore next */
    (cov_qig5kn2rc().b[39][0]++, customer.paymentType === "Credit") &&
    /* istanbul ignore next */
    (cov_qig5kn2rc().b[39][1]++, customer.creditPeriod)) {
      /* istanbul ignore next */
      cov_qig5kn2rc().b[38][0]++;
      const dueDateObj =
      /* istanbul ignore next */
      (cov_qig5kn2rc().s[133]++, new Date(today));
      /* istanbul ignore next */
      cov_qig5kn2rc().s[134]++;
      dueDateObj.setDate(today.getDate() + customer.creditPeriod);
      /* istanbul ignore next */
      cov_qig5kn2rc().s[135]++;
      dueDate = dueDateObj.toISOString().split("T")[0];
      /* istanbul ignore next */
      cov_qig5kn2rc().s[136]++;
      status = "pending";
    } else
    /* istanbul ignore next */
    {
      cov_qig5kn2rc().b[38][1]++;
    }
    // Fetch detailed customer info if needed
    cov_qig5kn2rc().s[137]++;
    try {
      const customerResponse =
      /* istanbul ignore next */
      (cov_qig5kn2rc().s[138]++, await fetch(`/api/customers/${customer.id}`));
      /* istanbul ignore next */
      cov_qig5kn2rc().s[139]++;
      if (customerResponse.ok) {
        /* istanbul ignore next */
        cov_qig5kn2rc().b[40][0]++;
        const customerData =
        /* istanbul ignore next */
        (cov_qig5kn2rc().s[140]++, await customerResponse.json());
        // Update customer with additional details if available
        /* istanbul ignore next */
        cov_qig5kn2rc().s[141]++;
        if (
        /* istanbul ignore next */
        (cov_qig5kn2rc().b[42][0]++, customerData.paymentType) ||
        /* istanbul ignore next */
        (cov_qig5kn2rc().b[42][1]++, customerData.creditPeriod)) {
          /* istanbul ignore next */
          cov_qig5kn2rc().b[41][0]++;
          cov_qig5kn2rc().s[142]++;
          customer = {
            ...customer,
            paymentType:
            /* istanbul ignore next */
            (cov_qig5kn2rc().b[43][0]++, customerData.paymentType) ||
            /* istanbul ignore next */
            (cov_qig5kn2rc().b[43][1]++, customer.paymentType),
            creditPeriod:
            /* istanbul ignore next */
            (cov_qig5kn2rc().b[44][0]++, customerData.creditPeriod) ||
            /* istanbul ignore next */
            (cov_qig5kn2rc().b[44][1]++, customer.creditPeriod)
          };
          // Recalculate due date with updated information
          /* istanbul ignore next */
          cov_qig5kn2rc().s[143]++;
          if (
          /* istanbul ignore next */
          (cov_qig5kn2rc().b[46][0]++, customer.paymentType === "Credit") &&
          /* istanbul ignore next */
          (cov_qig5kn2rc().b[46][1]++, customer.creditPeriod)) {
            /* istanbul ignore next */
            cov_qig5kn2rc().b[45][0]++;
            const dueDateObj =
            /* istanbul ignore next */
            (cov_qig5kn2rc().s[144]++, new Date(today));
            /* istanbul ignore next */
            cov_qig5kn2rc().s[145]++;
            dueDateObj.setDate(today.getDate() + customer.creditPeriod);
            /* istanbul ignore next */
            cov_qig5kn2rc().s[146]++;
            dueDate = dueDateObj.toISOString().split("T")[0];
            /* istanbul ignore next */
            cov_qig5kn2rc().s[147]++;
            status = "pending";
          } else
          /* istanbul ignore next */
          {
            cov_qig5kn2rc().b[45][1]++;
          }
        } else
        /* istanbul ignore next */
        {
          cov_qig5kn2rc().b[41][1]++;
        }
      } else
      /* istanbul ignore next */
      {
        cov_qig5kn2rc().b[40][1]++;
      }
    } catch (error) {
      /* istanbul ignore next */
      cov_qig5kn2rc().s[148]++;
      console.error("Error fetching customer details:", error);
    }
    // Update form data with customer info and calculated dates/status
    /* istanbul ignore next */
    cov_qig5kn2rc().s[149]++;
    setFormData({
      ...formData,
      customerId: customer.id,
      customerName: customer.name,
      invoiceDate: invoiceDate,
      dueDate: dueDate,
      status: status
    });
    /* istanbul ignore next */
    cov_qig5kn2rc().s[150]++;
    setCustomerSearch("");
    /* istanbul ignore next */
    cov_qig5kn2rc().s[151]++;
    setShowCustomerDropdown(false);
  };
  // Handle product selection for adding to line items
  /* istanbul ignore next */
  cov_qig5kn2rc().s[152]++;
  const handleSelectProduct = async product => {
    /* istanbul ignore next */
    cov_qig5kn2rc().f[22]++;
    cov_qig5kn2rc().s[153]++;
    // Fetch full product details to get weightedAverageCost for newly added items
    if (!formData.shopId) {
      /* istanbul ignore next */
      cov_qig5kn2rc().b[47][0]++;
      cov_qig5kn2rc().s[154]++;
      alert("Please select a shop first before adding products.");
      /* istanbul ignore next */
      cov_qig5kn2rc().s[155]++;
      return;
    } else
    /* istanbul ignore next */
    {
      cov_qig5kn2rc().b[47][1]++;
    }
    cov_qig5kn2rc().s[156]++;
    try {
      const response =
      /* istanbul ignore next */
      (cov_qig5kn2rc().s[157]++, await fetch(`/api/products/${product.id}`));
      /* istanbul ignore next */
      cov_qig5kn2rc().s[158]++;
      if (response.ok) {
        /* istanbul ignore next */
        cov_qig5kn2rc().b[48][0]++;
        const detailedProductData =
        /* istanbul ignore next */
        (cov_qig5kn2rc().s[159]++, await response.json());
        /* istanbul ignore next */
        cov_qig5kn2rc().s[160]++;
        if (
        /* istanbul ignore next */
        (cov_qig5kn2rc().b[50][0]++, detailedProductData.success) &&
        /* istanbul ignore next */
        (cov_qig5kn2rc().b[50][1]++, detailedProductData.data)) {
          /* istanbul ignore next */
          cov_qig5kn2rc().b[49][0]++;
          cov_qig5kn2rc().s[161]++;
          setSelectedProduct(detailedProductData.data);
          /* istanbul ignore next */
          cov_qig5kn2rc().s[162]++;
          setCurrentProductCost(
          /* istanbul ignore next */
          (cov_qig5kn2rc().b[51][0]++, detailedProductData.data.weightedAverageCost) ||
          /* istanbul ignore next */
          (cov_qig5kn2rc().b[51][1]++, 0));
          const shopStock =
          /* istanbul ignore next */
          (cov_qig5kn2rc().s[163]++,
          /* istanbul ignore next */
          (cov_qig5kn2rc().b[52][0]++, detailedProductData.data.inventory?.find(invItem => {
            /* istanbul ignore next */
            cov_qig5kn2rc().f[23]++;
            cov_qig5kn2rc().s[164]++;
            return invItem.shop_id === formData.shopId;
          } // Assuming shopId on inventoryItem is number
          )?.quantity) ||
          /* istanbul ignore next */
          (cov_qig5kn2rc().b[52][1]++, 0));
          /* istanbul ignore next */
          cov_qig5kn2rc().s[165]++;
          setProductStock(shopStock);
          /* istanbul ignore next */
          cov_qig5kn2rc().s[166]++;
          setCustomPrice(0); // Reset custom price when product is selected
        } else {
          /* istanbul ignore next */
          cov_qig5kn2rc().b[49][1]++;
          cov_qig5kn2rc().s[167]++;
          setSelectedProduct(product);
          /* istanbul ignore next */
          cov_qig5kn2rc().s[168]++;
          setCurrentProductCost(
          /* istanbul ignore next */
          (cov_qig5kn2rc().b[53][0]++, product.weightedAverageCost) ||
          /* istanbul ignore next */
          (cov_qig5kn2rc().b[53][1]++, 0));
          /* istanbul ignore next */
          cov_qig5kn2rc().s[169]++;
          setProductStock(null);
          /* istanbul ignore next */
          cov_qig5kn2rc().s[170]++;
          setCustomPrice(0); // Reset custom price when product is selected
        }
      } else {
        /* istanbul ignore next */
        cov_qig5kn2rc().b[48][1]++;
        cov_qig5kn2rc().s[171]++;
        setSelectedProduct(product);
        /* istanbul ignore next */
        cov_qig5kn2rc().s[172]++;
        setCurrentProductCost(
        /* istanbul ignore next */
        (cov_qig5kn2rc().b[54][0]++, product.weightedAverageCost) ||
        /* istanbul ignore next */
        (cov_qig5kn2rc().b[54][1]++, 0));
        /* istanbul ignore next */
        cov_qig5kn2rc().s[173]++;
        setProductStock(null);
        /* istanbul ignore next */
        cov_qig5kn2rc().s[174]++;
        setCustomPrice(0); // Reset custom price when product is selected
      }
    } catch (error) {
      /* istanbul ignore next */
      cov_qig5kn2rc().s[175]++;
      setSelectedProduct(product);
      /* istanbul ignore next */
      cov_qig5kn2rc().s[176]++;
      setCurrentProductCost(
      /* istanbul ignore next */
      (cov_qig5kn2rc().b[55][0]++, product.weightedAverageCost) ||
      /* istanbul ignore next */
      (cov_qig5kn2rc().b[55][1]++, 0));
      /* istanbul ignore next */
      cov_qig5kn2rc().s[177]++;
      setProductStock(null);
      /* istanbul ignore next */
      cov_qig5kn2rc().s[178]++;
      setCustomPrice(0); // Reset custom price when product is selected
      /* istanbul ignore next */
      cov_qig5kn2rc().s[179]++;
      console.error("Error fetching product stock and cost for new item:", error);
    }
    /* istanbul ignore next */
    cov_qig5kn2rc().s[180]++;
    setProductSearch("");
    /* istanbul ignore next */
    cov_qig5kn2rc().s[181]++;
    setShowProductDropdown(false);
  };
  // Add line item to invoice
  /* istanbul ignore next */
  cov_qig5kn2rc().s[182]++;
  const handleAddLineItem = () => {
    /* istanbul ignore next */
    cov_qig5kn2rc().f[24]++;
    cov_qig5kn2rc().s[183]++;
    if (!selectedProduct) {
      /* istanbul ignore next */
      cov_qig5kn2rc().b[56][0]++;
      cov_qig5kn2rc().s[184]++;
      return;
    } else
    /* istanbul ignore next */
    {
      cov_qig5kn2rc().b[56][1]++;
    }
    cov_qig5kn2rc().s[185]++;
    if (!formData.shopId) {
      /* istanbul ignore next */
      cov_qig5kn2rc().b[57][0]++;
      cov_qig5kn2rc().s[186]++;
      alert("Please select a shop first");
      /* istanbul ignore next */
      cov_qig5kn2rc().s[187]++;
      return;
    } else
    /* istanbul ignore next */
    {
      cov_qig5kn2rc().b[57][1]++;
    }
    cov_qig5kn2rc().s[188]++;
    if (quantity <= 0) {
      /* istanbul ignore next */
      cov_qig5kn2rc().b[58][0]++;
      cov_qig5kn2rc().s[189]++;
      setQuantity(1);
      /* istanbul ignore next */
      cov_qig5kn2rc().s[190]++;
      return;
    } else
    /* istanbul ignore next */
    {
      cov_qig5kn2rc().b[58][1]++;
    }
    cov_qig5kn2rc().s[191]++;
    if (
    /* istanbul ignore next */
    (cov_qig5kn2rc().b[60][0]++, productStock !== null) &&
    /* istanbul ignore next */
    (cov_qig5kn2rc().b[60][1]++, quantity > productStock)) {
      /* istanbul ignore next */
      cov_qig5kn2rc().b[59][0]++;
      cov_qig5kn2rc().s[192]++;
      alert(`Not enough stock in selected shop. Available: ${productStock}`);
      /* istanbul ignore next */
      cov_qig5kn2rc().s[193]++;
      return;
    } else
    /* istanbul ignore next */
    {
      cov_qig5kn2rc().b[59][1]++;
    }
    const sellingPrice =
    /* istanbul ignore next */
    (cov_qig5kn2rc().s[194]++, customPrice > 0 ?
    /* istanbul ignore next */
    (cov_qig5kn2rc().b[61][0]++, customPrice) :
    /* istanbul ignore next */
    (cov_qig5kn2rc().b[61][1]++, selectedProduct.price)); // Use custom price if set, otherwise default price
    const costPrice =
    /* istanbul ignore next */
    (cov_qig5kn2rc().s[195]++, currentProductCost); // Cost for the new item
    const itemTotal =
    /* istanbul ignore next */
    (cov_qig5kn2rc().s[196]++, sellingPrice * quantity);
    // Check credit limit for wholesale customers
    /* istanbul ignore next */
    cov_qig5kn2rc().s[197]++;
    if (
    /* istanbul ignore next */
    (cov_qig5kn2rc().b[63][0]++, selectedCustomer) &&
    /* istanbul ignore next */
    (cov_qig5kn2rc().b[63][1]++, selectedCustomer.customerType === "wholesale") &&
    /* istanbul ignore next */
    (cov_qig5kn2rc().b[63][2]++, selectedCustomer.creditLimit)) {
      /* istanbul ignore next */
      cov_qig5kn2rc().b[62][0]++;
      const currentTotal =
      /* istanbul ignore next */
      (cov_qig5kn2rc().s[198]++, formData.items.reduce((sum, item) => {
        /* istanbul ignore next */
        cov_qig5kn2rc().f[25]++;
        cov_qig5kn2rc().s[199]++;
        return sum + item.total;
      }, 0));
      /* istanbul ignore next */
      cov_qig5kn2rc().s[200]++;
      if (currentTotal + itemTotal > selectedCustomer.creditLimit) {
        /* istanbul ignore next */
        cov_qig5kn2rc().b[64][0]++;
        cov_qig5kn2rc().s[201]++;
        alert(`Adding this item would exceed the customer's credit limit of Rs. ${selectedCustomer.creditLimit.toLocaleString()}. Current total: Rs. ${currentTotal.toLocaleString()}`);
        /* istanbul ignore next */
        cov_qig5kn2rc().s[202]++;
        return;
      } else
      /* istanbul ignore next */
      {
        cov_qig5kn2rc().b[64][1]++;
      }
    } else
    /* istanbul ignore next */
    {
      cov_qig5kn2rc().b[62][1]++;
    }
    const newItem =
    /* istanbul ignore next */
    (cov_qig5kn2rc().s[203]++, {
      id: Date.now().toString(),
      productId: selectedProduct.id,
      productName: selectedProduct.name,
      quantity: quantity,
      price: sellingPrice,
      costPrice: costPrice,
      total: itemTotal
    });
    /* istanbul ignore next */
    cov_qig5kn2rc().s[204]++;
    setFormData({
      ...formData,
      items: [...formData.items, newItem]
    });
    // Reset selection
    /* istanbul ignore next */
    cov_qig5kn2rc().s[205]++;
    setSelectedProduct(null);
    /* istanbul ignore next */
    cov_qig5kn2rc().s[206]++;
    setProductStock(null);
    /* istanbul ignore next */
    cov_qig5kn2rc().s[207]++;
    setQuantity(1);
    /* istanbul ignore next */
    cov_qig5kn2rc().s[208]++;
    setCustomPrice(0);
  };
  // Remove line item from invoice
  /* istanbul ignore next */
  cov_qig5kn2rc().s[209]++;
  const handleRemoveLineItem = itemId => {
    /* istanbul ignore next */
    cov_qig5kn2rc().f[26]++;
    cov_qig5kn2rc().s[210]++;
    setFormData({
      ...formData,
      items: formData.items.filter(item => {
        /* istanbul ignore next */
        cov_qig5kn2rc().f[27]++;
        cov_qig5kn2rc().s[211]++;
        return item.id !== itemId;
      })
    });
  };
  // Update item quantity
  /* istanbul ignore next */
  cov_qig5kn2rc().s[212]++;
  const handleUpdateItemQuantity = (itemId, newQuantity) => {
    /* istanbul ignore next */
    cov_qig5kn2rc().f[28]++;
    cov_qig5kn2rc().s[213]++;
    console.log("Updating quantity:", itemId, newQuantity);
    /* istanbul ignore next */
    cov_qig5kn2rc().s[214]++;
    if (newQuantity <= 0) {
      /* istanbul ignore next */
      cov_qig5kn2rc().b[65][0]++;
      cov_qig5kn2rc().s[215]++;
      // If new quantity is invalid, find the item and revert its input to the current quantity
      setFormData(prevData => {
        /* istanbul ignore next */
        cov_qig5kn2rc().f[29]++;
        const currentItem =
        /* istanbul ignore next */
        (cov_qig5kn2rc().s[216]++, prevData.items.find(item => {
          /* istanbul ignore next */
          cov_qig5kn2rc().f[30]++;
          cov_qig5kn2rc().s[217]++;
          return item.id === itemId;
        }));
        /* istanbul ignore next */
        cov_qig5kn2rc().s[218]++;
        if (currentItem) {
          /* istanbul ignore next */
          cov_qig5kn2rc().b[66][0]++;
          // Trigger a re-render with the existing quantity to reset the input
          const updatedItems =
          /* istanbul ignore next */
          (cov_qig5kn2rc().s[219]++, prevData.items.map(item => {
            /* istanbul ignore next */
            cov_qig5kn2rc().f[31]++;
            cov_qig5kn2rc().s[220]++;
            if (item.id === itemId) {
              /* istanbul ignore next */
              cov_qig5kn2rc().b[67][0]++;
              cov_qig5kn2rc().s[221]++;
              return {
                ...item,
                quantity: currentItem.quantity
              };
            } else
            /* istanbul ignore next */
            {
              cov_qig5kn2rc().b[67][1]++;
            }
            cov_qig5kn2rc().s[222]++;
            return item;
          }));
          /* istanbul ignore next */
          cov_qig5kn2rc().s[223]++;
          return {
            ...prevData,
            items: updatedItems
          };
        } else
        /* istanbul ignore next */
        {
          cov_qig5kn2rc().b[66][1]++;
        }
        cov_qig5kn2rc().s[224]++;
        return prevData;
      });
      /* istanbul ignore next */
      cov_qig5kn2rc().s[225]++;
      return;
    } else
    /* istanbul ignore next */
    {
      cov_qig5kn2rc().b[65][1]++;
    }
    cov_qig5kn2rc().s[226]++;
    setFormData(prevData => {
      /* istanbul ignore next */
      cov_qig5kn2rc().f[32]++;
      cov_qig5kn2rc().s[227]++;
      console.log("Previous formData:", prevData);
      const updatedItems =
      /* istanbul ignore next */
      (cov_qig5kn2rc().s[228]++, prevData.items.map(item => {
        /* istanbul ignore next */
        cov_qig5kn2rc().f[33]++;
        cov_qig5kn2rc().s[229]++;
        if (item.id === itemId) {
          /* istanbul ignore next */
          cov_qig5kn2rc().b[68][0]++;
          cov_qig5kn2rc().s[230]++;
          console.log("Updating item:", item.id, "from", item.quantity, "to", newQuantity);
          /* istanbul ignore next */
          cov_qig5kn2rc().s[231]++;
          return {
            ...item,
            quantity: newQuantity,
            total: item.price * newQuantity
          };
        } else
        /* istanbul ignore next */
        {
          cov_qig5kn2rc().b[68][1]++;
        }
        cov_qig5kn2rc().s[232]++;
        return item;
      }));
      /* istanbul ignore next */
      cov_qig5kn2rc().s[233]++;
      console.log("Updated items:", updatedItems);
      /* istanbul ignore next */
      cov_qig5kn2rc().s[234]++;
      return {
        ...prevData,
        items: updatedItems
      };
    });
  };
  // Handle form submission
  /* istanbul ignore next */
  cov_qig5kn2rc().s[235]++;
  const handleSubmit = async e => {
    /* istanbul ignore next */
    cov_qig5kn2rc().f[34]++;
    cov_qig5kn2rc().s[236]++;
    e.preventDefault();
    // Final credit limit check for wholesale customers
    /* istanbul ignore next */
    cov_qig5kn2rc().s[237]++;
    if (
    /* istanbul ignore next */
    (cov_qig5kn2rc().b[70][0]++, selectedCustomer) &&
    /* istanbul ignore next */
    (cov_qig5kn2rc().b[70][1]++, selectedCustomer.customerType === "wholesale") &&
    /* istanbul ignore next */
    (cov_qig5kn2rc().b[70][2]++, selectedCustomer.creditLimit)) {
      /* istanbul ignore next */
      cov_qig5kn2rc().b[69][0]++;
      const finalTotal =
      /* istanbul ignore next */
      (cov_qig5kn2rc().s[238]++, formData.items.reduce((sum, item) => {
        /* istanbul ignore next */
        cov_qig5kn2rc().f[35]++;
        cov_qig5kn2rc().s[239]++;
        return sum + item.total;
      }, 0));
      /* istanbul ignore next */
      cov_qig5kn2rc().s[240]++;
      if (finalTotal > selectedCustomer.creditLimit) {
        /* istanbul ignore next */
        cov_qig5kn2rc().b[71][0]++;
        cov_qig5kn2rc().s[241]++;
        alert(`The total invoice amount of Rs. ${finalTotal.toLocaleString()} exceeds the customer's credit limit of Rs. ${selectedCustomer.creditLimit.toLocaleString()}. Please remove some items or reduce quantities.`);
        /* istanbul ignore next */
        cov_qig5kn2rc().s[242]++;
        return;
      } else
      /* istanbul ignore next */
      {
        cov_qig5kn2rc().b[71][1]++;
      }
    } else
    /* istanbul ignore next */
    {
      cov_qig5kn2rc().b[69][1]++;
    }
    cov_qig5kn2rc().s[243]++;
    setIsSubmitting(true);
    /* istanbul ignore next */
    cov_qig5kn2rc().s[244]++;
    try {
      // Transform data for API
      const invoiceData =
      /* istanbul ignore next */
      (cov_qig5kn2rc().s[245]++, {
        customerId: formData.customerId,
        total: invoiceTotal,
        status: formData.status,
        paymentMethod: formData.paymentMethod,
        notes: formData.notes,
        invoiceDate: formData.invoiceDate,
        dueDate: formData.dueDate,
        shopId: formData.shopId,
        items: formData.items.map(item => {
          /* istanbul ignore next */
          cov_qig5kn2rc().f[36]++;
          cov_qig5kn2rc().s[246]++;
          return {
            productId: item.productId,
            quantity: item.quantity,
            price: item.price,
            total: item.total
          };
        }),
        discountType: formData.discountType,
        discountValue: formData.discountValue,
        // Include the sendSms flag
        sendSms: sendSms
      });
      /* istanbul ignore next */
      cov_qig5kn2rc().s[247]++;
      console.log("Submitting invoice data:", invoiceData);
      // Update invoice via API
      const response =
      /* istanbul ignore next */
      (cov_qig5kn2rc().s[248]++, await fetch(`/api/invoices/${params.id}`, {
        method: "PUT",
        headers: {
          "Content-Type": "application/json"
        },
        body: JSON.stringify(invoiceData)
      }));
      /* istanbul ignore next */
      cov_qig5kn2rc().s[249]++;
      if (!response.ok) {
        /* istanbul ignore next */
        cov_qig5kn2rc().b[72][0]++;
        const errorData =
        /* istanbul ignore next */
        (cov_qig5kn2rc().s[250]++, await response.json());
        /* istanbul ignore next */
        cov_qig5kn2rc().s[251]++;
        console.error("API error response:", errorData);
        /* istanbul ignore next */
        cov_qig5kn2rc().s[252]++;
        throw new Error(
        /* istanbul ignore next */
        (cov_qig5kn2rc().b[73][0]++, errorData.error) ||
        /* istanbul ignore next */
        (cov_qig5kn2rc().b[73][1]++, errorData.message) ||
        /* istanbul ignore next */
        (cov_qig5kn2rc().b[73][2]++, "Failed to update invoice"));
      } else
      /* istanbul ignore next */
      {
        cov_qig5kn2rc().b[72][1]++;
      }
      // Redirect to invoice detail page
      cov_qig5kn2rc().s[253]++;
      router.push(`/invoices/${params.id}`);
      /* istanbul ignore next */
      cov_qig5kn2rc().s[254]++;
      router.refresh();
    } catch (error) {
      /* istanbul ignore next */
      cov_qig5kn2rc().s[255]++;
      console.error("Error updating invoice:", error);
      /* istanbul ignore next */
      cov_qig5kn2rc().s[256]++;
      if (error instanceof Error) {
        /* istanbul ignore next */
        cov_qig5kn2rc().b[74][0]++;
        cov_qig5kn2rc().s[257]++;
        alert(error.message);
      } else {
        /* istanbul ignore next */
        cov_qig5kn2rc().b[74][1]++;
        cov_qig5kn2rc().s[258]++;
        alert("Failed to update invoice. Please try again.");
      }
    } finally {
      /* istanbul ignore next */
      cov_qig5kn2rc().s[259]++;
      setIsSubmitting(false);
    }
  };
  /* istanbul ignore next */
  cov_qig5kn2rc().s[260]++;
  if (isLoading) {
    /* istanbul ignore next */
    cov_qig5kn2rc().b[75][0]++;
    cov_qig5kn2rc().s[261]++;
    return /*#__PURE__*/(0, _jsxruntime.jsx)(_MainLayout.default, {
      children: /*#__PURE__*/(0, _jsxruntime.jsxs)("div", {
        className: "space-y-6 animate-pulse",
        children: [/*#__PURE__*/(0, _jsxruntime.jsx)("div", {
          className: "h-10 bg-gray-200 rounded w-64 mb-6"
        }), /*#__PURE__*/(0, _jsxruntime.jsx)("div", {
          className: "bg-tertiary p-8 rounded-xl border border-gray-200",
          children: /*#__PURE__*/(0, _jsxruntime.jsxs)("div", {
            className: "space-y-4",
            children: [/*#__PURE__*/(0, _jsxruntime.jsx)("div", {
              className: "h-8 bg-gray-200 rounded w-1/3"
            }), /*#__PURE__*/(0, _jsxruntime.jsxs)("div", {
              className: "grid grid-cols-2 gap-6",
              children: [/*#__PURE__*/(0, _jsxruntime.jsxs)("div", {
                className: "space-y-2",
                children: [/*#__PURE__*/(0, _jsxruntime.jsx)("div", {
                  className: "h-4 bg-gray-200 rounded w-32"
                }), /*#__PURE__*/(0, _jsxruntime.jsx)("div", {
                  className: "h-10 bg-gray-200 rounded w-full"
                })]
              }), /*#__PURE__*/(0, _jsxruntime.jsxs)("div", {
                className: "space-y-2",
                children: [/*#__PURE__*/(0, _jsxruntime.jsx)("div", {
                  className: "h-4 bg-gray-200 rounded w-32"
                }), /*#__PURE__*/(0, _jsxruntime.jsx)("div", {
                  className: "h-10 bg-gray-200 rounded w-full"
                })]
              })]
            }), /*#__PURE__*/(0, _jsxruntime.jsx)("div", {
              className: "h-px bg-gray-200 my-6"
            }), /*#__PURE__*/(0, _jsxruntime.jsx)("div", {
              className: "h-64 bg-gray-200 rounded w-full"
            })]
          })
        })]
      })
    });
  } else
  /* istanbul ignore next */
  {
    cov_qig5kn2rc().b[75][1]++;
  }
  cov_qig5kn2rc().s[262]++;
  return /*#__PURE__*/(0, _jsxruntime.jsx)(_MainLayout.default, {
    children: /*#__PURE__*/(0, _jsxruntime.jsxs)("div", {
      className: "space-y-6",
      children: [/*#__PURE__*/(0, _jsxruntime.jsxs)("div", {
        className: "flex flex-col md:flex-row md:items-center md:justify-between gap-4",
        children: [/*#__PURE__*/(0, _jsxruntime.jsxs)("div", {
          children: [/*#__PURE__*/(0, _jsxruntime.jsx)("h1", {
            className: "text-2xl font-bold text-gray-900",
            children: "Edit Invoice"
          }), /*#__PURE__*/(0, _jsxruntime.jsxs)("p", {
            className: "text-gray-500",
            children: ["Edit invoice #", invoiceNumber]
          })]
        }), /*#__PURE__*/(0, _jsxruntime.jsx)("div", {
          className: "flex gap-3",
          children: /*#__PURE__*/(0, _jsxruntime.jsxs)(_Button.Button, {
            variant: "outline",
            size: "sm",
            onClick: () => {
              /* istanbul ignore next */
              cov_qig5kn2rc().f[37]++;
              cov_qig5kn2rc().s[263]++;
              return router.back();
            },
            children: [/*#__PURE__*/(0, _jsxruntime.jsx)(_lucidereact.ArrowLeft, {
              className: "w-4 h-4 mr-2"
            }), "Back"]
          })
        })]
      }), /*#__PURE__*/(0, _jsxruntime.jsx)("div", {
        className: "bg-tertiary rounded-lg shadow-sm border border-gray-200 p-6",
        children: /*#__PURE__*/(0, _jsxruntime.jsxs)("form", {
          onSubmit: handleSubmit,
          className: "space-y-8",
          children: [/*#__PURE__*/(0, _jsxruntime.jsxs)("div", {
            className: "grid grid-cols-1 md:grid-cols-2 gap-6",
            children: [/*#__PURE__*/(0, _jsxruntime.jsxs)("div", {
              children: [/*#__PURE__*/(0, _jsxruntime.jsx)("h2", {
                className: "text-lg font-semibold mb-4 text-gray-900 border-b pb-2",
